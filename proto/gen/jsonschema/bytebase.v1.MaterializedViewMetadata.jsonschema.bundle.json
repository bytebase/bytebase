{
  "$defs": {
    "bytebase.v1.BoundingBox.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the spatial bounds for GEOMETRY spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum X coordinate"
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum X coordinate"
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum Y coordinate"
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum Y coordinate"
        }
      },
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.v1.DependencyColumn.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DependencyColumn is the metadata for dependency columns.",
      "properties": {
        "column": {
          "default": "",
          "description": "The column is the name of a reference column.",
          "type": "string"
        },
        "schema": {
          "default": "",
          "description": "The schema is the schema of a reference column.",
          "type": "string"
        },
        "table": {
          "default": "",
          "description": "The table is the table of a reference column.",
          "type": "string"
        }
      },
      "title": "Dependency Column",
      "type": "object"
    },
    "bytebase.v1.DimensionConstraint.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionConstraint defines constraints for a spatial dimension.",
      "patternProperties": {
        "^(max_value)$": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum value for this dimension"
        },
        "^(min_value)$": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum value for this dimension"
        }
      },
      "properties": {
        "dimension": {
          "default": "",
          "description": "Dimension name/type (X, Y, Z, M, etc.)",
          "type": "string"
        },
        "maxValue": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum value for this dimension"
        },
        "minValue": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum value for this dimension"
        },
        "tolerance": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Tolerance for this dimension"
        }
      },
      "title": "Dimension Constraint",
      "type": "object"
    },
    "bytebase.v1.DimensionalConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionalConfig defines dimensional and constraint parameters for spatial indexes.",
      "patternProperties": {
        "^(data_type)$": {
          "default": "",
          "description": "Spatial data type (GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.)",
          "type": "string"
        }
      },
      "properties": {
        "constraints": {
          "description": "Coordinate system constraints",
          "items": {
            "$ref": "#/$defs/bytebase.v1.DimensionConstraint.jsonschema.json"
          },
          "type": "array"
        },
        "dataType": {
          "default": "",
          "description": "Spatial data type (GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.)",
          "type": "string"
        },
        "dimensions": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of dimensions (2-4, default 2)"
        },
        "srid": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Spatial reference system identifier (SRID)"
        }
      },
      "title": "Dimensional Config",
      "type": "object"
    },
    "bytebase.v1.GridLevel.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a tessellation grid level with its density.",
      "properties": {
        "density": {
          "default": "",
          "description": "Grid density (LOW, MEDIUM, HIGH)",
          "type": "string"
        },
        "level": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Grid level number (1-4 for SQL Server)"
        }
      },
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.v1.IndexMetadata.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "IndexMetadata is the metadata for indexes.",
      "patternProperties": {
        "^(is_constraint)$": {
          "default": false,
          "description": "It's a PostgreSQL specific field.\n The unique constraint and unique index are not the same thing in PostgreSQL.",
          "type": "boolean"
        },
        "^(key_length)$": {
          "description": "The key_lengths are the ordered key lengths of an index.\n If the key length is not specified, it's -1.",
          "items": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "pattern": "^-?[0-9]+$",
                "type": "string"
              }
            ]
          },
          "type": "array"
        },
        "^(opclass_defaults)$": {
          "description": "True if the operator class is the default. (PostgreSQL specific).",
          "items": {
            "type": "boolean"
          },
          "type": "array"
        },
        "^(opclass_names)$": {
          "description": "https://www.postgresql.org/docs/current/catalog-pg-opclass.html\n Name of the operator class for each column. (PostgreSQL specific).",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "^(parent_index_name)$": {
          "default": "",
          "description": "The index name of the parent index.",
          "type": "string"
        },
        "^(parent_index_schema)$": {
          "default": "",
          "description": "The schema name of the parent index.",
          "type": "string"
        },
        "^(spatial_config)$": {
          "$ref": "#/$defs/bytebase.v1.SpatialIndexConfig.jsonschema.json",
          "description": "Spatial index configuration for spatial databases like SQL Server, PostgreSQL with PostGIS, etc."
        }
      },
      "properties": {
        "comment": {
          "default": "",
          "description": "The comment is the comment of an index.",
          "type": "string"
        },
        "definition": {
          "default": "",
          "description": "The definition of an index.",
          "type": "string"
        },
        "descending": {
          "description": "The descending is the ordered descending of an index.",
          "items": {
            "type": "boolean"
          },
          "type": "array"
        },
        "expressions": {
          "description": "The expressions are the ordered columns or expressions of an index.\n This could refer to a column or an expression.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "granularity": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "The number of granules in the block. It's a ClickHouse specific field."
        },
        "isConstraint": {
          "default": false,
          "description": "It's a PostgreSQL specific field.\n The unique constraint and unique index are not the same thing in PostgreSQL.",
          "type": "boolean"
        },
        "keyLength": {
          "description": "The key_lengths are the ordered key lengths of an index.\n If the key length is not specified, it's -1.",
          "items": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "pattern": "^-?[0-9]+$",
                "type": "string"
              }
            ]
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "description": "The name is the name of an index.",
          "type": "string"
        },
        "opclassDefaults": {
          "description": "True if the operator class is the default. (PostgreSQL specific).",
          "items": {
            "type": "boolean"
          },
          "type": "array"
        },
        "opclassNames": {
          "description": "https://www.postgresql.org/docs/current/catalog-pg-opclass.html\n Name of the operator class for each column. (PostgreSQL specific).",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "parentIndexName": {
          "default": "",
          "description": "The index name of the parent index.",
          "type": "string"
        },
        "parentIndexSchema": {
          "default": "",
          "description": "The schema name of the parent index.",
          "type": "string"
        },
        "primary": {
          "default": false,
          "description": "The primary is whether the index is a primary key index.",
          "type": "boolean"
        },
        "spatialConfig": {
          "$ref": "#/$defs/bytebase.v1.SpatialIndexConfig.jsonschema.json",
          "description": "Spatial index configuration for spatial databases like SQL Server, PostgreSQL with PostGIS, etc."
        },
        "type": {
          "default": "",
          "description": "The type is the type of an index.",
          "type": "string"
        },
        "unique": {
          "default": false,
          "description": "The unique is whether the index is unique.",
          "type": "boolean"
        },
        "visible": {
          "default": false,
          "description": "The visible is whether the index is visible.",
          "type": "boolean"
        }
      },
      "title": "Index Metadata",
      "type": "object"
    },
    "bytebase.v1.MaterializedViewMetadata.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "MaterializedViewMetadata is the metadata for materialized views.",
      "patternProperties": {
        "^(dependency_columns)$": {
          "description": "The dependency_columns is the list of dependency columns of a materialized\n view.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.DependencyColumn.jsonschema.json"
          },
          "type": "array"
        },
        "^(skip_dump)$": {
          "default": false,
          "type": "boolean"
        }
      },
      "properties": {
        "comment": {
          "default": "",
          "description": "The comment is the comment of a materialized view.",
          "type": "string"
        },
        "definition": {
          "default": "",
          "description": "The definition is the definition of a materialized view.",
          "type": "string"
        },
        "dependencyColumns": {
          "description": "The dependency_columns is the list of dependency columns of a materialized\n view.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.DependencyColumn.jsonschema.json"
          },
          "type": "array"
        },
        "indexes": {
          "description": "The indexes is the list of indexes in a table.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.IndexMetadata.jsonschema.json"
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "description": "The name is the name of a materialized view.",
          "type": "string"
        },
        "skipDump": {
          "default": false,
          "type": "boolean"
        },
        "triggers": {
          "description": "The columns is the ordered list of columns in a table.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.TriggerMetadata.jsonschema.json"
          },
          "type": "array"
        }
      },
      "title": "Materialized View Metadata",
      "type": "object"
    },
    "bytebase.v1.SpatialIndexConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "SpatialIndexConfig defines the spatial index configuration for spatial databases.",
      "properties": {
        "dimensional": {
          "$ref": "#/$defs/bytebase.v1.DimensionalConfig.jsonschema.json",
          "description": "Dimensional configuration"
        },
        "method": {
          "default": "",
          "description": "Spatial indexing method (e.g., \"SPATIAL\", \"R-TREE\", \"GIST\")",
          "type": "string"
        },
        "storage": {
          "$ref": "#/$defs/bytebase.v1.StorageConfig.jsonschema.json",
          "description": "Storage and performance configuration"
        },
        "tessellation": {
          "$ref": "#/$defs/bytebase.v1.TessellationConfig.jsonschema.json",
          "description": "Tessellation configuration for grid-based spatial indexes"
        }
      },
      "title": "Spatial Index Config",
      "type": "object"
    },
    "bytebase.v1.StorageConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "StorageConfig defines storage and performance parameters for spatial indexes.",
      "patternProperties": {
        "^(allow_page_locks)$": {
          "default": false,
          "type": "boolean"
        },
        "^(allow_row_locks)$": {
          "default": false,
          "type": "boolean"
        },
        "^(commit_interval)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(data_compression)$": {
          "default": "",
          "type": "string"
        },
        "^(drop_existing)$": {
          "default": false,
          "type": "boolean"
        },
        "^(pad_index)$": {
          "default": false,
          "description": "SQL Server specific parameters",
          "type": "boolean"
        },
        "^(sdo_level)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(sort_in_tempdb)$": {
          "default": "",
          "type": "string"
        },
        "^(work_tablespace)$": {
          "default": "",
          "type": "string"
        }
      },
      "properties": {
        "allowPageLocks": {
          "default": false,
          "type": "boolean"
        },
        "allowRowLocks": {
          "default": false,
          "type": "boolean"
        },
        "buffering": {
          "default": "",
          "description": "Buffering mode for PostgreSQL (auto, on, off)",
          "type": "string"
        },
        "commitInterval": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "dataCompression": {
          "default": "",
          "type": "string"
        },
        "dropExisting": {
          "default": false,
          "type": "boolean"
        },
        "fillfactor": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Fill factor percentage (1-100)"
        },
        "maxdop": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "online": {
          "default": false,
          "type": "boolean"
        },
        "padIndex": {
          "default": false,
          "description": "SQL Server specific parameters",
          "type": "boolean"
        },
        "sdoLevel": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "sortInTempdb": {
          "default": "",
          "type": "string"
        },
        "tablespace": {
          "default": "",
          "description": "Tablespace configuration for Oracle",
          "type": "string"
        },
        "workTablespace": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Storage Config",
      "type": "object"
    },
    "bytebase.v1.TessellationConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "patternProperties": {
        "^(bounding_box)$": {
          "$ref": "#/$defs/bytebase.v1.BoundingBox.jsonschema.json",
          "description": "Bounding box for GEOMETRY tessellation (not used for GEOGRAPHY)"
        },
        "^(cells_per_object)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of cells per object (1-8192 for SQL Server)"
        },
        "^(grid_levels)$": {
          "description": "Grid levels and densities for multi-level tessellation",
          "items": {
            "$ref": "#/$defs/bytebase.v1.GridLevel.jsonschema.json"
          },
          "type": "array"
        }
      },
      "properties": {
        "boundingBox": {
          "$ref": "#/$defs/bytebase.v1.BoundingBox.jsonschema.json",
          "description": "Bounding box for GEOMETRY tessellation (not used for GEOGRAPHY)"
        },
        "cellsPerObject": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of cells per object (1-8192 for SQL Server)"
        },
        "gridLevels": {
          "description": "Grid levels and densities for multi-level tessellation",
          "items": {
            "$ref": "#/$defs/bytebase.v1.GridLevel.jsonschema.json"
          },
          "type": "array"
        },
        "scheme": {
          "default": "",
          "description": "Tessellation scheme (e.g., \"GEOMETRY_GRID\", \"GEOGRAPHY_GRID\", \"GEOMETRY_AUTO_GRID\")",
          "type": "string"
        }
      },
      "title": "Tessellation Config",
      "type": "object"
    },
    "bytebase.v1.TriggerMetadata.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(character_set_client)$": {
          "default": "",
          "description": "The character set used by the client creating the trigger.",
          "type": "string"
        },
        "^(collation_connection)$": {
          "default": "",
          "description": "The collation used for the connection when creating the trigger.",
          "type": "string"
        },
        "^(skip_dump)$": {
          "default": false,
          "description": "Whether to skip this trigger during schema dump operations.",
          "type": "boolean"
        },
        "^(sql_mode)$": {
          "default": "",
          "description": "The SQL mode setting for the trigger.",
          "type": "string"
        }
      },
      "properties": {
        "body": {
          "default": "",
          "description": "The body is the body of the trigger.",
          "type": "string"
        },
        "characterSetClient": {
          "default": "",
          "description": "The character set used by the client creating the trigger.",
          "type": "string"
        },
        "collationConnection": {
          "default": "",
          "description": "The collation used for the connection when creating the trigger.",
          "type": "string"
        },
        "comment": {
          "default": "",
          "description": "The comment describing the trigger.",
          "type": "string"
        },
        "event": {
          "default": "",
          "description": "The event is the event of the trigger, such as INSERT, UPDATE, DELETE,\n TRUNCATE.",
          "type": "string"
        },
        "name": {
          "default": "",
          "description": "The name is the name of the trigger.",
          "type": "string"
        },
        "skipDump": {
          "default": false,
          "description": "Whether to skip this trigger during schema dump operations.",
          "type": "boolean"
        },
        "sqlMode": {
          "default": "",
          "description": "The SQL mode setting for the trigger.",
          "type": "string"
        },
        "timing": {
          "default": "",
          "description": "The timing is the timing of the trigger, such as BEFORE, AFTER.",
          "type": "string"
        }
      },
      "title": "Trigger Metadata",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.MaterializedViewMetadata.jsonschema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.MaterializedViewMetadata.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
