{
  "$id": "bytebase.v1.CheckReleaseResponse.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "affected_rows": {
      "description": "The total affected rows across all checks.",
      "type": "integer"
    },
    "results": {
      "description": "The check results for each file and target combination.",
      "items": {
        "$ref": "bytebase.v1.CheckReleaseResponse.CheckResult.schema.strict.json"
      },
      "type": "array"
    },
    "risk_level": {
      "description": "The aggregated risk level of the check.",
      "enum": [
        "RISK_LEVEL_UNSPECIFIED",
        "LOW",
        "MODERATE",
        "HIGH"
      ],
      "title": "Risk Level",
      "type": "string"
    }
  },
  "required": [
    "affected_rows",
    "risk_level"
  ],
  "title": "Check Release Response",
  "type": "object"
}
