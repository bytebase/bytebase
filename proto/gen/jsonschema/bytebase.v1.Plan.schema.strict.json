{
  "$id": "bytebase.v1.Plan.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "create_time": {
      "$ref": "google.protobuf.Timestamp.schema.strict.json"
    },
    "creator": {
      "description": "Format: users/hello@world.com",
      "type": "string"
    },
    "deployment": {
      "$ref": "bytebase.v1.Plan.Deployment.schema.strict.json"
    },
    "description": {
      "description": "The description of the plan.",
      "maxLength": 10000,
      "type": "string"
    },
    "issue": {
      "description": "The issue associated with the plan.\n Can be empty.\n Format: projects/{project}/issues/{issue}",
      "type": "string"
    },
    "name": {
      "description": "The name of the plan.\n `plan` is a system generated ID.\n Format: projects/{project}/plans/{plan}",
      "type": "string"
    },
    "plan_check_run_status_count": {
      "additionalProperties": {
        "maximum": 2147483647,
        "minimum": -2147483648,
        "type": "integer"
      },
      "description": "The status count of the latest plan check runs.\n Keys are:\n - SUCCESS\n - WARNING\n - ERROR\n - RUNNING",
      "propertyNames": {
        "type": "string"
      },
      "type": "object"
    },
    "rollout": {
      "description": "The rollout associated with the plan.\n Can be empty.\n Format: projects/{project}/rollouts/{rollout}",
      "type": "string"
    },
    "specs": {
      "description": "The deployment specs for the plan.",
      "items": {
        "$ref": "bytebase.v1.Plan.Spec.schema.strict.json"
      },
      "type": "array"
    },
    "state": {
      "description": "The state of the plan.",
      "enum": [
        "STATE_UNSPECIFIED",
        "ACTIVE",
        "DELETED"
      ],
      "title": "State",
      "type": "string"
    },
    "title": {
      "description": "The title of the plan.",
      "maxLength": 200,
      "type": "string"
    },
    "update_time": {
      "$ref": "google.protobuf.Timestamp.schema.strict.json"
    }
  },
  "required": [
    "name",
    "state",
    "issue",
    "rollout",
    "title",
    "description",
    "creator"
  ],
  "title": "Plan",
  "type": "object"
}
