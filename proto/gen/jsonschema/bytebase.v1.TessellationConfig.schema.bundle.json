{
  "$defs": {
    "bytebase.v1.BoundingBox.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the spatial bounds for GEOMETRY spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum X coordinate"
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum X coordinate"
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum Y coordinate"
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum Y coordinate"
        }
      },
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.v1.GridLevel.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a tessellation grid level with its density.",
      "properties": {
        "density": {
          "default": "",
          "description": "Grid density (LOW, MEDIUM, HIGH)",
          "type": "string"
        },
        "level": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Grid level number (1-4 for SQL Server)"
        }
      },
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.v1.TessellationConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "patternProperties": {
        "^(boundingBox)$": {
          "$ref": "#/$defs/bytebase.v1.BoundingBox.schema.json",
          "description": "Bounding box for GEOMETRY tessellation (not used for GEOGRAPHY)"
        },
        "^(cellsPerObject)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of cells per object (1-8192 for SQL Server)"
        },
        "^(gridLevels)$": {
          "description": "Grid levels and densities for multi-level tessellation",
          "items": {
            "$ref": "#/$defs/bytebase.v1.GridLevel.schema.json"
          },
          "type": "array"
        }
      },
      "properties": {
        "bounding_box": {
          "$ref": "#/$defs/bytebase.v1.BoundingBox.schema.json",
          "description": "Bounding box for GEOMETRY tessellation (not used for GEOGRAPHY)"
        },
        "cells_per_object": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of cells per object (1-8192 for SQL Server)"
        },
        "grid_levels": {
          "description": "Grid levels and densities for multi-level tessellation",
          "items": {
            "$ref": "#/$defs/bytebase.v1.GridLevel.schema.json"
          },
          "type": "array"
        },
        "scheme": {
          "default": "",
          "description": "Tessellation scheme (e.g., \"GEOMETRY_GRID\", \"GEOGRAPHY_GRID\", \"GEOMETRY_AUTO_GRID\")",
          "type": "string"
        }
      },
      "title": "Tessellation Config",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.TessellationConfig.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.TessellationConfig.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
