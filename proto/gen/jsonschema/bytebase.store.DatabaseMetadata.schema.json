{
  "$id": "bytebase.store.DatabaseMetadata.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "DatabaseMetadata is the metadata for databases.",
  "patternProperties": {
    "^(backupAvailable)$": {
      "default": false,
      "type": "boolean"
    },
    "^(lastSyncTime)$": {
      "$ref": "google.protobuf.Timestamp.schema.json"
    }
  },
  "properties": {
    "backup_available": {
      "default": false,
      "type": "boolean"
    },
    "datashare": {
      "default": false,
      "type": "boolean"
    },
    "drifted": {
      "default": false,
      "description": "The schema has drifted from the source of truth.",
      "type": "boolean"
    },
    "labels": {
      "additionalProperties": {
        "type": "string"
      },
      "propertyNames": {
        "type": "string"
      },
      "type": "object"
    },
    "last_sync_time": {
      "$ref": "google.protobuf.Timestamp.schema.json"
    },
    "version": {
      "default": "",
      "description": "The version of database schema.",
      "type": "string"
    }
  },
  "title": "Database Metadata",
  "type": "object"
}
