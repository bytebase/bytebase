{
  "$defs": {
    "bytebase.v1.QueryDataPolicy.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "QueryDataPolicy is the policy configuration for querying data.",
      "properties": {
        "disable_copy_data": {
          "description": "Disable copying query results.",
          "type": "boolean"
        },
        "disable_export": {
          "description": "Disable data export in the SQL editor.",
          "type": "boolean"
        },
        "maximum_result_rows": {
          "description": "The maximum number of rows to return.\n The default value is -1, means no limit.",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "maximum_result_size": {
          "description": "The maximum result size limit in bytes.\n The default value is 100MB, we will use the default value if the setting not exists, or the limit \u003c= 0.",
          "type": "integer"
        },
        "timeout": {
          "$ref": "#/$defs/google.protobuf.Duration.schema.strict.json",
          "description": "The query timeout duration."
        }
      },
      "required": [
        "disable_export",
        "maximum_result_size",
        "maximum_result_rows",
        "disable_copy_data"
      ],
      "title": "Query Data Policy",
      "type": "object"
    },
    "google.protobuf.Duration.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "format": "duration",
      "title": "Duration",
      "type": "string"
    }
  },
  "$id": "bytebase.v1.QueryDataPolicy.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.QueryDataPolicy.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
