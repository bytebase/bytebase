{
  "$defs": {
    "bytebase.v1.IdentityProviderContext.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Context for identity provider authentication.",
      "properties": {
        "oauth2_context": {
          "$ref": "#/$defs/bytebase.v1.OAuth2IdentityProviderContext.schema.strict.json",
          "description": "OAuth2 authentication context."
        },
        "oidc_context": {
          "$ref": "#/$defs/bytebase.v1.OIDCIdentityProviderContext.schema.strict.json",
          "description": "OpenID Connect authentication context."
        }
      },
      "title": "Identity Provider Context",
      "type": "object"
    },
    "bytebase.v1.OAuth2IdentityProviderContext.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "OAuth2 authentication context.",
      "properties": {
        "code": {
          "description": "Authorization code from OAuth2 provider.",
          "type": "string"
        }
      },
      "required": [
        "code"
      ],
      "title": "O Auth2 Identity Provider Context",
      "type": "object"
    },
    "bytebase.v1.OIDCIdentityProviderContext.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "OpenID Connect authentication context.",
      "properties": {
        "code": {
          "description": "Authorization code from OIDC provider.",
          "type": "string"
        }
      },
      "required": [
        "code"
      ],
      "title": "OIDC Identity Provider Context",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.IdentityProviderContext.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.IdentityProviderContext.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
