          "default": "",
          "description": "The database containing the table.\n Format: instances/{instance}/databases/{database}",
          "type": "string"
        },
        "schema": {
          "default": "",
          "description": "Schema name (for databases that support schemas).",
          "type": "string"
        },
        "table": {
          "default": "",
          "description": "Table name.",
          "type": "string"
        }
      },
      "title": "Table",
      "type": "object"
    },
    "bytebase.store.PriorBackupDetail.Item.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Item represents a single backup operation for a table.",
      "patternProperties": {
        "^(endPosition)$": {
          "$ref": "#/$defs/bytebase.store.Position.schema.json",
          "description": "Ending position in SQL for this backup operation."
        },
        "^(sourceTable)$": {
          "$ref": "#/$defs/bytebase.store.PriorBackupDetail.Item.Table.schema.json",
          "description": "The original table that was backed up."
        },
        "^(startPosition)$": {
          "$ref": "#/$defs/bytebase.store.Position.schema.json",
          "description": "Starting position in SQL for this backup operation."
        },
        "^(targetTable)$": {
          "$ref": "#/$defs/bytebase.store.PriorBackupDetail.Item.Table.schema.json",
          "description": "The backup table where data was copied."
        }
      },
      "properties": {
        "end_position": {
          "$ref": "#/$defs/bytebase.store.Position.schema.json",
          "description": "Ending position in SQL for this backup operation."
        },
        "source_table": {
          "$ref": "#/$defs/bytebase.store.PriorBackupDetail.Item.Table.schema.json",
          "description": "The original table that was backed up."
        },
        "start_position": {
          "$ref": "#/$defs/bytebase.store.Position.schema.json",
          "description": "Starting position in SQL for this backup operation."
        },
        "target_table": {
          "$ref": "#/$defs/bytebase.store.PriorBackupDetail.Item.Table.schema.json",
          "description": "The backup table where data was copied."
        }
      },
      "title": "Item",
      "type": "object"
    },
