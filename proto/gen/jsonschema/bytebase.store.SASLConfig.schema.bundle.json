{
  "$defs": {
    "bytebase.store.KerberosConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(kdcHost)$": {
          "default": "",
          "type": "string"
        },
        "^(kdcPort)$": {
          "default": "",
          "type": "string"
        },
        "^(kdcTransportProtocol)$": {
          "default": "",
          "type": "string"
        }
      },
      "properties": {
        "instance": {
          "default": "",
          "type": "string"
        },
        "kdc_host": {
          "default": "",
          "type": "string"
        },
        "kdc_port": {
          "default": "",
          "type": "string"
        },
        "kdc_transport_protocol": {
          "default": "",
          "type": "string"
        },
        "keytab": {
          "default": null,
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "primary": {
          "default": "",
          "type": "string"
        },
        "realm": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Kerberos Config",
      "type": "object"
    },
    "bytebase.store.SASLConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(krbConfig)$": {
          "$ref": "#/$defs/bytebase.store.KerberosConfig.schema.json"
        }
      },
      "properties": {
        "krb_config": {
          "$ref": "#/$defs/bytebase.store.KerberosConfig.schema.json"
        }
      },
      "title": "SASL Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.SASLConfig.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.SASLConfig.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
