{
  "$id": "bytebase.store.DataSourceExternalSecret.AppRoleAuthOption.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(mount_path)$": {
      "default": "",
      "description": "The path where the approle auth method is mounted.",
      "type": "string"
    },
    "^(role_id)$": {
      "default": "",
      "type": "string"
    },
    "^(secret_id)$": {
      "default": "",
      "description": "The secret ID for the role without TTL.",
      "type": "string"
    }
  },
  "properties": {
    "mountPath": {
      "default": "",
      "description": "The path where the approle auth method is mounted.",
      "type": "string"
    },
    "roleId": {
      "default": "",
      "type": "string"
    },
    "secretId": {
      "default": "",
      "description": "The secret ID for the role without TTL.",
      "type": "string"
    },
    "type": {
      "anyOf": [
        {
          "pattern": "^SECRET_TYPE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "PLAIN",
            "ENVIRONMENT"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Secret Type"
    }
  },
  "title": "App Role Auth Option",
  "type": "object"
}
