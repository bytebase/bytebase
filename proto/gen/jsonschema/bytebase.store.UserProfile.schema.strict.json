{
  "$id": "bytebase.store.UserProfile.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "last_change_password_time": {
      "$ref": "google.protobuf.Timestamp.schema.strict.json"
    },
    "last_login_time": {
      "$ref": "google.protobuf.Timestamp.schema.strict.json"
    },
    "source": {
      "description": "The source indicates where the user comes from. For now we support Entra ID SCIM sync, so the source could be Entra ID.",
      "type": "string"
    }
  },
  "required": [
    "source"
  ],
  "title": "User Profile",
  "type": "object"
}
