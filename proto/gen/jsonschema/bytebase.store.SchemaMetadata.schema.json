{
  "$id": "bytebase.store.SchemaMetadata.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "SchemaMetadata is the metadata for schemas.\n This is the concept of schema in Postgres, but it's a no-op for MySQL.",
  "patternProperties": {
    "^(enumTypes)$": {
      "items": {
        "$ref": "bytebase.store.EnumTypeMetadata.schema.json"
      },
      "type": "array"
    },
    "^(externalTables)$": {
      "description": "The list of external tables in a schema.",
      "items": {
        "$ref": "bytebase.store.ExternalTableMetadata.schema.json"
      },
      "type": "array"
    },
    "^(materializedViews)$": {
      "description": "The list of materialized views in a schema.",
      "items": {
        "$ref": "bytebase.store.MaterializedViewMetadata.schema.json"
      },
      "type": "array"
    },
    "^(skipDump)$": {
      "default": false,
      "type": "boolean"
    }
  },
  "properties": {
    "comment": {
      "default": "",
      "type": "string"
    },
    "enum_types": {
      "items": {
        "$ref": "bytebase.store.EnumTypeMetadata.schema.json"
      },
      "type": "array"
    },
    "events": {
      "items": {
        "$ref": "bytebase.store.EventMetadata.schema.json"
      },
      "type": "array"
    },
    "external_tables": {
      "description": "The list of external tables in a schema.",
      "items": {
        "$ref": "bytebase.store.ExternalTableMetadata.schema.json"
      },
      "type": "array"
    },
    "functions": {
      "description": "The list of functions in a schema.",
      "items": {
        "$ref": "bytebase.store.FunctionMetadata.schema.json"
      },
      "type": "array"
    },
    "materialized_views": {
      "description": "The list of materialized views in a schema.",
      "items": {
        "$ref": "bytebase.store.MaterializedViewMetadata.schema.json"
      },
      "type": "array"
    },
    "name": {
      "default": "",
      "description": "The schema name.\n It is an empty string for databases without such concept such as MySQL.",
      "type": "string"
    },
    "owner": {
      "default": "",
      "type": "string"
    },
    "packages": {
      "description": "The list of packages in a schema.",
      "items": {
        "$ref": "bytebase.store.PackageMetadata.schema.json"
      },
      "type": "array"
    },
    "procedures": {
      "description": "The list of procedures in a schema.",
      "items": {
        "$ref": "bytebase.store.ProcedureMetadata.schema.json"
      },
      "type": "array"
    },
    "sequences": {
      "description": "The list of sequences in a schema.",
      "items": {
        "$ref": "bytebase.store.SequenceMetadata.schema.json"
      },
      "type": "array"
    },
    "skip_dump": {
      "default": false,
      "type": "boolean"
    },
    "streams": {
      "description": "The list of streams in a schema, currently only used for Snowflake.",
      "items": {
        "$ref": "bytebase.store.StreamMetadata.schema.json"
      },
      "type": "array"
    },
    "tables": {
      "description": "The list of tables in a schema.",
      "items": {
        "$ref": "bytebase.store.TableMetadata.schema.json"
      },
      "type": "array"
    },
    "tasks": {
      "description": "The list of tasks in a schema, currently only used for Snowflake.",
      "items": {
        "$ref": "bytebase.store.TaskMetadata.schema.json"
      },
      "type": "array"
    },
    "views": {
      "description": "The list of views in a schema.",
      "items": {
        "$ref": "bytebase.store.ViewMetadata.schema.json"
      },
      "type": "array"
    }
  },
  "title": "Schema Metadata",
  "type": "object"
}
