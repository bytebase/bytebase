{
  "$defs": {
    "bytebase.v1.ColumnCatalog.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Column metadata within a table.",
      "patternProperties": {
        "^(object_schema)$": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.jsonschema.json",
          "description": "Object schema for complex column types like JSON."
        },
        "^(semantic_type)$": {
          "default": "",
          "description": "The semantic type describing the data purpose.",
          "type": "string"
        }
      },
      "properties": {
        "classification": {
          "default": "",
          "description": "The data classification level for this column.",
          "type": "string"
        },
        "labels": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "User-defined labels for this column.",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "name": {
          "default": "",
          "description": "The column name.",
          "type": "string"
        },
        "objectSchema": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.jsonschema.json",
          "description": "Object schema for complex column types like JSON."
        },
        "semanticType": {
          "default": "",
          "description": "The semantic type describing the data purpose.",
          "type": "string"
        }
      },
      "title": "Column Catalog",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.ArrayKind.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Array type with element schema.",
      "properties": {
        "kind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.jsonschema.json",
          "description": "The schema of array elements."
        }
      },
      "title": "Array Kind",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.StructKind.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Structure type with named properties.",
      "properties": {
        "properties": {
          "additionalProperties": {
            "$ref": "#/$defs/bytebase.v1.ObjectSchema.jsonschema.json"
          },
          "description": "Properties of the struct.",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "title": "Struct Kind",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Schema definition for object-type columns.",
      "patternProperties": {
        "^(array_kind)$": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.ArrayKind.jsonschema.json",
          "description": "Array schema."
        },
        "^(semantic_type)$": {
          "default": "",
          "description": "The semantic type of this object.",
          "type": "string"
        },
        "^(struct_kind)$": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.StructKind.jsonschema.json",
          "description": "Struct schema."
        }
      },
      "properties": {
        "arrayKind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.ArrayKind.jsonschema.json",
          "description": "Array schema."
        },
        "semanticType": {
          "default": "",
          "description": "The semantic type of this object.",
          "type": "string"
        },
        "structKind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.StructKind.jsonschema.json",
          "description": "Struct schema."
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "STRING",
                "NUMBER",
                "BOOLEAN",
                "OBJECT",
                "ARRAY"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "The data type of this object.",
          "title": "Type"
        }
      },
      "title": "Object Schema",
      "type": "object"
    },
    "bytebase.v1.TableCatalog.Columns.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Column list for regular tables.",
      "properties": {
        "columns": {
          "description": "The columns in the table.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.ColumnCatalog.jsonschema.json"
          },
          "type": "array"
        }
      },
      "title": "Columns",
      "type": "object"
    },
    "bytebase.v1.TableCatalog.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Table metadata within a schema.",
      "patternProperties": {
        "^(object_schema)$": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.jsonschema.json",
          "description": "Object schema for JSON/XML columns."
        }
      },
      "properties": {
        "classification": {
          "default": "",
          "description": "The data classification level for this table.",
          "type": "string"
        },
        "columns": {
          "$ref": "#/$defs/bytebase.v1.TableCatalog.Columns.jsonschema.json",
          "description": "Regular table columns."
        },
        "name": {
          "default": "",
          "description": "The table name.",
          "type": "string"
        },
        "objectSchema": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.jsonschema.json",
          "description": "Object schema for JSON/XML columns."
        }
      },
      "title": "Table Catalog",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.TableCatalog.jsonschema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.TableCatalog.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
