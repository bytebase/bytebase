{
  "$defs": {
    "bytebase.store.DependencyTable.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "schema": {
          "default": "",
          "description": "The schema is the schema of a reference table.",
          "type": "string"
        },
        "table": {
          "default": "",
          "description": "The table is the name of a reference table.",
          "type": "string"
        }
      },
      "title": "Dependency Table",
      "type": "object"
    },
    "bytebase.store.FunctionMetadata.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "FunctionMetadata is the metadata for functions.",
      "patternProperties": {
        "^(characterSetClient)$": {
          "default": "",
          "description": "MySQL specific metadata.",
          "type": "string"
        },
        "^(collationConnection)$": {
          "default": "",
          "type": "string"
        },
        "^(databaseCollation)$": {
          "default": "",
          "type": "string"
        },
        "^(dependencyTables)$": {
          "description": "The dependency_tables is the list of dependency tables of a function.\n For PostgreSQL, it's the list of tables that the function depends on the return type definition.",
          "items": {
            "$ref": "#/$defs/bytebase.store.DependencyTable.schema.json"
          },
          "type": "array"
        },
        "^(skipDump)$": {
          "default": false,
          "type": "boolean"
        },
        "^(sqlMode)$": {
          "default": "",
          "type": "string"
        }
      },
      "properties": {
        "character_set_client": {
          "default": "",
          "description": "MySQL specific metadata.",
          "type": "string"
        },
        "collation_connection": {
          "default": "",
          "type": "string"
        },
        "comment": {
          "default": "",
          "type": "string"
        },
        "database_collation": {
          "default": "",
          "type": "string"
        },
        "definition": {
          "default": "",
          "description": "The definition is the definition of a function.",
          "type": "string"
        },
        "dependency_tables": {
          "description": "The dependency_tables is the list of dependency tables of a function.\n For PostgreSQL, it's the list of tables that the function depends on the return type definition.",
          "items": {
            "$ref": "#/$defs/bytebase.store.DependencyTable.schema.json"
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "description": "The name of the function.",
          "type": "string"
        },
        "signature": {
          "default": "",
          "description": "The signature is the name with the number and type of input arguments the\n function takes.",
          "type": "string"
        },
        "skip_dump": {
          "default": false,
          "type": "boolean"
        },
        "sql_mode": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Function Metadata",
      "type": "object"
    }
  },
  "$id": "bytebase.store.FunctionMetadata.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.FunctionMetadata.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
