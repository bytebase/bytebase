{
  "$id": "bytebase.v1.TaskRunLogEntry.CommandExecute.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Command execution details.",
  "properties": {
    "command_indexes": {
      "description": "Either `command_indexes` or `statement` is set.\n The indexes of the executed commands.",
      "items": {
        "maximum": 2147483647,
        "minimum": -2147483648,
        "type": "integer"
      },
      "type": "array"
    },
    "log_time": {
      "$ref": "google.protobuf.Timestamp.schema.strict.json",
      "description": "When the command was logged."
    },
    "response": {
      "$ref": "bytebase.v1.TaskRunLogEntry.CommandExecute.CommandResponse.schema.strict.json",
      "description": "The response from executing the command."
    },
    "statement": {
      "description": "The executed statement.",
      "type": "string"
    }
  },
  "required": [
    "statement"
  ],
  "title": "Command Execute",
  "type": "object"
}
