      "type": "object"
    },
    "bytebase.v1.ChangedResourceSchema.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "functions": {
          "items": {
            "$ref": "#/$defs/bytebase.v1.ChangedResourceFunction.jsonschema.json"
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "type": "string"
        },
        "procedures": {
          "items": {
            "$ref": "#/$defs/bytebase.v1.ChangedResourceProcedure.jsonschema.json"
          },
          "type": "array"
        },
        "tables": {
          "items": {
            "$ref": "#/$defs/bytebase.v1.ChangedResourceTable.jsonschema.json"
          },
          "type": "array"
        },
        "views": {
          "items": {
            "$ref": "#/$defs/bytebase.v1.ChangedResourceView.jsonschema.json"
          },
          "type": "array"
        }
      },
      "title": "Changed Resource Schema",
      "type": "object"
    },
    "bytebase.v1.ChangedResourceTable.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "name": {
          "default": "",
          "type": "string"
        },
        "ranges": {
          "description": "The ranges of sub-strings correspond to the statements on the sheet.",
          "items": {
