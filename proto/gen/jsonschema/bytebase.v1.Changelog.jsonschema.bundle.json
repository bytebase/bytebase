          },
          "type": "array"
        }
      },
      "title": "Changed Resource Table",
      "type": "object"
    },
    "bytebase.v1.ChangedResourceView.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "name": {
          "default": "",
          "type": "string"
        },
        "ranges": {
          "description": "The ranges of sub-strings correspond to the statements on the sheet.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.Range.jsonschema.json"
          },
