      "properties": {
        "name": {
          "default": "",
          "type": "string"
        },
        "schemas": {
          "items": {
            "$ref": "#/$defs/bytebase.v1.ChangedResourceSchema.jsonschema.json"
          },
          "type": "array"
        }
      },
