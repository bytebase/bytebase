{
  "$id": "bytebase.store.ChangelogPayload.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "changedResources": {
      "$ref": "bytebase.store.ChangedResources.jsonschema.strict.json"
    },
    "dumpVersion": {
      "description": "Dump format version for drift detection reliability.\n Engine-specific version stored when baseline/migration is created.\n 0 = legacy changelog (pre-versioning).",
      "maximum": 2147483647,
      "minimum": -2147483648,
      "type": "integer"
    },
    "gitCommit": {
      "type": "string"
    },
    "issue": {
      "description": "Format: projects/{project}/issues/{issue}",
      "type": "string"
    },
    "revision": {
      "description": "The revision uid.\n optional",
      "type": "integer"
    },
    "sheet": {
      "description": "The sheet that holds the content.\n Format: projects/{project}/sheets/{sheet}",
      "type": "string"
    },
    "taskRun": {
      "description": "Format: projects/{project}/rollouts/{rollout}/stages/{stage}/tasks/{task}/taskruns/{taskrun}",
      "type": "string"
    },
    "type": {
      "enum": [
        "TYPE_UNSPECIFIED",
        "BASELINE",
        "MIGRATE",
        "SDL"
      ],
      "title": "Type",
      "type": "string"
    },
    "version": {
      "type": "string"
    }
  },
  "required": [
    "taskRun",
    "issue",
    "revision",
    "sheet",
    "version",
    "type",
    "gitCommit",
    "dumpVersion"
  ],
  "title": "Changelog Payload",
  "type": "object"
}
