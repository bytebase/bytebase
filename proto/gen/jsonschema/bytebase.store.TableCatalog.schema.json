{
  "$id": "bytebase.store.TableCatalog.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(objectSchema)$": {
      "$ref": "bytebase.store.ObjectSchema.schema.json"
    }
  },
  "properties": {
    "classification": {
      "default": "",
      "type": "string"
    },
    "columns": {
      "description": "The column_configs is the ordered list of configs for columns in a table.",
      "items": {
        "$ref": "bytebase.store.ColumnCatalog.schema.json"
      },
      "type": "array"
    },
    "name": {
      "default": "",
      "description": "The name of the table.",
      "type": "string"
    },
    "object_schema": {
      "$ref": "bytebase.store.ObjectSchema.schema.json"
    }
  },
  "title": "Table Catalog",
  "type": "object"
}
