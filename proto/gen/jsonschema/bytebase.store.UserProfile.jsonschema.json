{
  "$id": "bytebase.store.UserProfile.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(last_change_password_time)$": {
      "$ref": "google.protobuf.Timestamp.jsonschema.json"
    },
    "^(last_login_time)$": {
      "$ref": "google.protobuf.Timestamp.jsonschema.json"
    }
  },
  "properties": {
    "lastChangePasswordTime": {
      "$ref": "google.protobuf.Timestamp.jsonschema.json"
    },
    "lastLoginTime": {
      "$ref": "google.protobuf.Timestamp.jsonschema.json"
    },
    "source": {
      "default": "",
      "description": "The source indicates where the user comes from. For now we support Entra ID SCIM sync, so the source could be Entra ID.",
      "type": "string"
    }
  },
  "title": "User Profile",
  "type": "object"
}
