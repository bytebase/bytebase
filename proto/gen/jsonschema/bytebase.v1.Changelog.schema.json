{
  "$id": "bytebase.v1.Changelog.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(changedResources)$": {
      "$ref": "bytebase.v1.ChangedResources.schema.json"
    },
    "^(createTime)$": {
      "$ref": "google.protobuf.Timestamp.schema.json"
    },
    "^(dumpVersionMismatch)$": {
      "default": false,
      "description": "True if this changelog's dump version differs from current engine dump version.\n When true and drift is detected, the drift may be a false positive from Bytebase upgrade.",
      "type": "boolean"
    },
    "^(prevSchema)$": {
      "default": "",
      "type": "string"
    },
    "^(prevSchemaSize)$": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "^(schemaSize)$": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "^(statementSheet)$": {
      "default": "",
      "description": "The name of the sheet resource.\n Format: projects/{project}/sheets/{sheet}",
      "type": "string"
    },
    "^(statementSize)$": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "^(taskRun)$": {
      "default": "",
      "description": "Format: projects/{projects}/rollouts/{rollout}/stages/{stage}/tasks/{task}/taskRuns/{taskRun}",
      "type": "string"
    }
  },
  "properties": {
    "changed_resources": {
      "$ref": "bytebase.v1.ChangedResources.schema.json"
    },
    "create_time": {
      "$ref": "google.protobuf.Timestamp.schema.json"
    },
    "dump_version_mismatch": {
      "default": false,
      "description": "True if this changelog's dump version differs from current engine dump version.\n When true and drift is detected, the drift may be a false positive from Bytebase upgrade.",
      "type": "boolean"
    },
    "issue": {
      "default": "",
      "description": "Format: projects/{project}/issues/{issue}",
      "type": "string"
    },
    "name": {
      "default": "",
      "description": "Format: instances/{instance}/databases/{database}/changelogs/{changelog}",
      "type": "string"
    },
    "prev_schema": {
      "default": "",
      "type": "string"
    },
    "prev_schema_size": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "revision": {
      "default": "",
      "description": "Could be empty\n Or present but not found if deleted",
      "type": "string"
    },
    "schema": {
      "default": "",
      "type": "string"
    },
    "schema_size": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "statement": {
      "default": "",
      "description": "The statement is used for preview purpose.",
      "type": "string"
    },
    "statement_sheet": {
      "default": "",
      "description": "The name of the sheet resource.\n Format: projects/{project}/sheets/{sheet}",
      "type": "string"
    },
    "statement_size": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "status": {
      "anyOf": [
        {
          "pattern": "^STATUS_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "PENDING",
            "DONE",
            "FAILED"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Status"
    },
    "task_run": {
      "default": "",
      "description": "Format: projects/{projects}/rollouts/{rollout}/stages/{stage}/tasks/{task}/taskRuns/{taskRun}",
      "type": "string"
    },
    "type": {
      "anyOf": [
        {
          "pattern": "^TYPE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "BASELINE",
            "MIGRATE",
            "SDL"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Type"
    },
    "version": {
      "default": "",
      "description": "Could be empty",
      "type": "string"
    }
  },
  "title": "Changelog",
  "type": "object"
}
