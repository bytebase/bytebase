{
  "$defs": {
    "bytebase.v1.Algorithm.FullMask.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "substitution": {
          "description": "substitution is the string used to replace the original value, the\n max length of the string is 16 bytes.",
          "type": "string"
        }
      },
      "required": [
        "substitution"
      ],
      "title": "Full Mask",
      "type": "object"
    },
    "bytebase.v1.Algorithm.InnerOuterMask.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "prefixLen": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "substitution": {
          "type": "string"
        },
        "suffixLen": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "type": {
          "enum": [
            "MASK_TYPE_UNSPECIFIED",
            "INNER",
            "OUTER"
          ],
          "title": "Mask Type",
          "type": "string"
        }
      },
      "required": [
        "prefixLen",
        "suffixLen",
        "type",
        "substitution"
      ],
      "title": "Inner Outer Mask",
      "type": "object"
    },
    "bytebase.v1.Algorithm.MD5Mask.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "salt": {
          "description": "salt is the salt value to generate a different hash that with the word alone.",
          "type": "string"
        }
      },
      "required": [
        "salt"
      ],
      "title": "M D5 Mask",
      "type": "object"
    },
    "bytebase.v1.Algorithm.RangeMask.Slice.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "end": {
          "description": "stop is the stop index of the original value, should be less than the length of the original value.",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "start": {
          "description": "start is the start index of the original value, start from 0 and should be less than stop.",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "substitution": {
          "description": "substitution is the string used to replace the OriginalValue[start:end).",
          "type": "string"
        }
      },
      "required": [
        "start",
        "end",
        "substitution"
      ],
      "title": "Slice",
      "type": "object"
    },
    "bytebase.v1.Algorithm.RangeMask.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "slices": {
          "description": "We store it as a repeated field to face the fact that the original value may have multiple parts should be masked.\n But frontend can be started with a single rule easily.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.Algorithm.RangeMask.Slice.jsonschema.strict.json"
          },
          "type": "array"
        }
      },
      "title": "Range Mask",
      "type": "object"
    },
    "bytebase.v1.Algorithm.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "fullMask": {
          "$ref": "#/$defs/bytebase.v1.Algorithm.FullMask.jsonschema.strict.json"
        },
        "innerOuterMask": {
          "$ref": "#/$defs/bytebase.v1.Algorithm.InnerOuterMask.jsonschema.strict.json"
        },
        "md5Mask": {
          "$ref": "#/$defs/bytebase.v1.Algorithm.MD5Mask.jsonschema.strict.json"
        },
        "rangeMask": {
          "$ref": "#/$defs/bytebase.v1.Algorithm.RangeMask.jsonschema.strict.json"
        }
      },
      "title": "Algorithm",
      "type": "object"
    },
    "bytebase.v1.SemanticTypeSetting.SemanticType.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "algorithm": {
          "$ref": "#/$defs/bytebase.v1.Algorithm.jsonschema.strict.json"
        },
        "description": {
          "description": "the description of the semantic type, it can be empty.",
          "type": "string"
        },
        "icon": {
          "description": "icon is the icon for semantic type, it can be emoji or base64 encoded image.",
          "type": "string"
        },
        "id": {
          "description": "id is the uuid for semantic type.",
          "type": "string"
        },
        "title": {
          "description": "the title of the semantic type, it should not be empty.",
          "type": "string"
        }
      },
      "required": [
        "id",
        "title",
        "description",
        "icon"
      ],
      "title": "Semantic Type",
      "type": "object"
    },
    "bytebase.v1.SemanticTypeSetting.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "types": {
          "items": {
            "$ref": "#/$defs/bytebase.v1.SemanticTypeSetting.SemanticType.jsonschema.strict.json"
          },
          "type": "array"
        }
      },
      "title": "Semantic Type Setting",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.SemanticTypeSetting.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.SemanticTypeSetting.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
