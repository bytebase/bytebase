{
  "$defs": {
    "bytebase.v1.Sheet.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(contentSize)$": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "content_size is the full size of the content, may not match the size of the `content` field."
        },
        "^(createTime)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "The create time of the sheet."
        }
      },
      "properties": {
        "content": {
          "default": null,
          "description": "The content of the sheet.\n By default, it will be cut off, if it doesn't match the `content_size`, you can\n set the `raw` to true in GetSheet request to retrieve the full content.",
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "content_size": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "content_size is the full size of the content, may not match the size of the `content` field."
        },
        "create_time": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "The create time of the sheet."
        },
        "creator": {
          "default": "",
          "description": "The creator of the Sheet.\n Format: users/{email}",
          "type": "string"
        },
        "engine": {
          "anyOf": [
            {
              "pattern": "^ENGINE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "CLICKHOUSE",
                "MYSQL",
                "POSTGRES",
                "SNOWFLAKE",
                "SQLITE",
                "TIDB",
                "MONGODB",
                "REDIS",
                "ORACLE",
                "SPANNER",
                "MSSQL",
                "REDSHIFT",
                "MARIADB",
                "OCEANBASE",
                "STARROCKS",
                "DORIS",
                "HIVE",
                "ELASTICSEARCH",
                "BIGQUERY",
                "DYNAMODB",
                "DATABRICKS",
                "COCKROACHDB",
                "COSMOSDB",
                "TRINO",
                "CASSANDRA"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "The SQL dialect.",
          "title": "Engine"
        },
        "name": {
          "default": "",
          "description": "The name of the sheet resource.\n Format: projects/{project}/sheets/{sheet}\n The sheet ID is generated by the server on creation and cannot be changed.",
          "type": "string"
        },
        "payload": {
          "$ref": "#/$defs/bytebase.v1.SheetPayload.schema.json",
          "description": "Additional metadata and configuration for the sheet."
        },
        "title": {
          "default": "",
          "description": "The title of the sheet.",
          "maxLength": 200,
          "type": "string"
        }
      },
      "title": "Sheet",
      "type": "object"
    },
    "bytebase.v1.SheetCommand.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "end": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "start": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        }
      },
      "title": "Sheet Command",
      "type": "object"
    },
    "bytebase.v1.SheetPayload.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "commands": {
          "description": "The start and end position of each command in the sheet statement.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.SheetCommand.schema.json"
          },
          "type": "array"
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "SCHEMA_DESIGN"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Type"
        }
      },
      "title": "Sheet Payload",
      "type": "object"
    },
    "google.protobuf.Timestamp.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    }
  },
  "$id": "bytebase.v1.Sheet.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.Sheet.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
