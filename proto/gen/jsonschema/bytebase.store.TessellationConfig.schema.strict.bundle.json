{
  "$defs": {
    "bytebase.store.BoundingBox.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the bounding box for spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        }
      },
      "required": [
        "xmin",
        "ymin",
        "xmax",
        "ymax"
      ],
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.store.GridLevel.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a grid level for spatial tessellation.",
      "properties": {
        "density": {
          "type": "string"
        },
        "level": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      },
      "required": [
        "level",
        "density"
      ],
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.store.TessellationConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "properties": {
        "bounding_box": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.schema.strict.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "cells_per_object": {
          "description": "Cells per object (SQL Server)",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "grid_levels": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.schema.strict.json"
          },
          "type": "array"
        },
        "scheme": {
          "description": "Tessellation scheme\n Examples: GEOMETRY_GRID, GEOGRAPHY_GRID, GEOMETRY_AUTO_GRID, GEOGRAPHY_AUTO_GRID",
          "type": "string"
        }
      },
      "required": [
        "scheme",
        "cells_per_object"
      ],
      "title": "Tessellation Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.TessellationConfig.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.TessellationConfig.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
