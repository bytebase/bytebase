{
  "$id": "bytebase.v1.PlanCheckRun.Result.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(sqlReviewReport)$": {
      "$ref": "bytebase.v1.PlanCheckRun.Result.SqlReviewReport.schema.json"
    },
    "^(sqlSummaryReport)$": {
      "$ref": "bytebase.v1.PlanCheckRun.Result.SqlSummaryReport.schema.json"
    }
  },
  "properties": {
    "code": {
      "anyOf": [
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "content": {
      "default": "",
      "type": "string"
    },
    "sql_review_report": {
      "$ref": "bytebase.v1.PlanCheckRun.Result.SqlReviewReport.schema.json"
    },
    "sql_summary_report": {
      "$ref": "bytebase.v1.PlanCheckRun.Result.SqlSummaryReport.schema.json"
    },
    "status": {
      "anyOf": [
        {
          "pattern": "^ADVICE_LEVEL_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "SUCCESS",
            "WARNING",
            "ERROR"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Level"
    },
    "title": {
      "default": "",
      "type": "string"
    }
  },
  "title": "Result",
  "type": "object"
}
