{
  "$defs": {
    "bytebase.v1.DependencyTable.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "schema": {
          "description": "The schema is the schema of a reference table.",
          "type": "string"
        },
        "table": {
          "description": "The table is the name of a reference table.",
          "type": "string"
        }
      },
      "required": [
        "schema",
        "table"
      ],
      "title": "Dependency Table",
      "type": "object"
    },
    "bytebase.v1.FunctionMetadata.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "FunctionMetadata is the metadata for functions.",
      "properties": {
        "characterSetClient": {
          "description": "MySQL specific metadata.",
          "type": "string"
        },
        "collationConnection": {
          "type": "string"
        },
        "comment": {
          "type": "string"
        },
        "databaseCollation": {
          "type": "string"
        },
        "definition": {
          "description": "The definition is the definition of a function.",
          "type": "string"
        },
        "dependencyTables": {
          "description": "The dependency_tables is the list of dependency tables of a function.\n For PostgreSQL, it's the list of tables that the function depends on the return type definition.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.DependencyTable.jsonschema.strict.json"
          },
          "type": "array"
        },
        "name": {
          "description": "The name is the name of a function.",
          "type": "string"
        },
        "signature": {
          "description": "The signature is the name with the number and type of input arguments the\n function takes.",
          "type": "string"
        },
        "skipDump": {
          "type": "boolean"
        },
        "sqlMode": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "definition",
        "signature",
        "characterSetClient",
        "collationConnection",
        "databaseCollation",
        "sqlMode",
        "comment",
        "skipDump"
      ],
      "title": "Function Metadata",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.FunctionMetadata.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.FunctionMetadata.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
