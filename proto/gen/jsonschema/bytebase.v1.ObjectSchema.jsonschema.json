{
  "$id": "bytebase.v1.ObjectSchema.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Schema definition for object-type columns.",
  "patternProperties": {
    "^(array_kind)$": {
      "$ref": "bytebase.v1.ObjectSchema.ArrayKind.jsonschema.json",
      "description": "Array schema."
    },
    "^(semantic_type)$": {
      "default": "",
      "description": "The semantic type of this object.",
      "type": "string"
    },
    "^(struct_kind)$": {
      "$ref": "bytebase.v1.ObjectSchema.StructKind.jsonschema.json",
      "description": "Struct schema."
    }
  },
  "properties": {
    "arrayKind": {
      "$ref": "bytebase.v1.ObjectSchema.ArrayKind.jsonschema.json",
      "description": "Array schema."
    },
    "semanticType": {
      "default": "",
      "description": "The semantic type of this object.",
      "type": "string"
    },
    "structKind": {
      "$ref": "bytebase.v1.ObjectSchema.StructKind.jsonschema.json",
      "description": "Struct schema."
    },
    "type": {
      "anyOf": [
        {
          "pattern": "^TYPE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "STRING",
            "NUMBER",
            "BOOLEAN",
            "OBJECT",
            "ARRAY"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "description": "The data type of this object.",
      "title": "Type"
    }
  },
  "title": "Object Schema",
  "type": "object"
}
