{
  "$defs": {
    "bytebase.store.BoundingBox.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the bounding box for spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        }
      },
      "required": [
        "xmin",
        "ymin",
        "xmax",
        "ymax"
      ],
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.store.DimensionalConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionalConfig defines dimensional and constraint parameters for spatial indexes.",
      "properties": {
        "dataType": {
          "description": "Spatial data type\n Examples: GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.",
          "type": "string"
        },
        "dimensions": {
          "description": "Number of dimensions (2-4, default 2)",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "layerGtype": {
          "description": "Oracle geometry type constraint\n Examples: POINT, LINE, POLYGON, COLLECTION",
          "type": "string"
        },
        "operatorClass": {
          "description": "PostgreSQL operator class\n Examples: gist_geometry_ops_2d, gist_geometry_ops_nd, etc.",
          "type": "string"
        },
        "parallelBuild": {
          "description": "Parallel index creation",
          "type": "boolean"
        }
      },
      "required": [
        "dimensions",
        "dataType",
        "operatorClass",
        "layerGtype",
        "parallelBuild"
      ],
      "title": "Dimensional Config",
      "type": "object"
    },
    "bytebase.store.GridLevel.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a grid level for spatial tessellation.",
      "properties": {
        "density": {
          "type": "string"
        },
        "level": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      },
      "required": [
        "level",
        "density"
      ],
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.store.SpatialIndexConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "SpatialIndexConfig is the configuration for spatial indexes across different database engines.",
      "properties": {
        "dimensional": {
          "$ref": "#/$defs/bytebase.store.DimensionalConfig.jsonschema.strict.json",
          "description": "Dimensional and constraint parameters"
        },
        "engineSpecific": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "Database-specific parameters (stored as key-value pairs for extensibility)",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "method": {
          "description": "Index method/type (database-specific)\n Examples: \"SPATIAL\" (MySQL/SQL Server), \"GIST\"/\"SPGIST\" (PostgreSQL), \"MDSYS.SPATIAL_INDEX_V2\" (Oracle)",
          "type": "string"
        },
        "storage": {
          "$ref": "#/$defs/bytebase.store.StorageConfig.jsonschema.strict.json",
          "description": "Storage and performance parameters"
        },
        "tessellation": {
          "$ref": "#/$defs/bytebase.store.TessellationConfig.jsonschema.strict.json",
          "description": "Tessellation configuration (primarily SQL Server)"
        }
      },
      "required": [
        "method"
      ],
      "title": "Spatial Index Config",
      "type": "object"
    },
    "bytebase.store.StorageConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "StorageConfig defines storage and performance parameters for spatial indexes.",
      "properties": {
        "allowPageLocks": {
          "type": "boolean"
        },
        "allowRowLocks": {
          "type": "boolean"
        },
        "buffering": {
          "type": "string"
        },
        "commitInterval": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "dataCompression": {
          "type": "string"
        },
        "dropExisting": {
          "type": "boolean"
        },
        "fillfactor": {
          "description": "PostgreSQL parameters",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "maxdop": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "online": {
          "type": "boolean"
        },
        "padIndex": {
          "description": "SQL Server parameters",
          "type": "boolean"
        },
        "sdoLevel": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "sortInTempdb": {
          "type": "string"
        },
        "tablespace": {
          "description": "Oracle parameters",
          "type": "string"
        },
        "workTablespace": {
          "type": "string"
        }
      },
      "required": [
        "fillfactor",
        "buffering",
        "tablespace",
        "workTablespace",
        "sdoLevel",
        "commitInterval",
        "padIndex",
        "sortInTempdb",
        "dropExisting",
        "online",
        "allowRowLocks",
        "allowPageLocks",
        "maxdop",
        "dataCompression"
      ],
      "title": "Storage Config",
      "type": "object"
    },
    "bytebase.store.TessellationConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "properties": {
        "boundingBox": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.jsonschema.strict.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "cellsPerObject": {
          "description": "Cells per object (SQL Server)",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "gridLevels": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.jsonschema.strict.json"
          },
          "type": "array"
        },
        "scheme": {
          "description": "Tessellation scheme\n Examples: GEOMETRY_GRID, GEOGRAPHY_GRID, GEOMETRY_AUTO_GRID, GEOGRAPHY_AUTO_GRID",
          "type": "string"
        }
      },
      "required": [
        "scheme",
        "cellsPerObject"
      ],
      "title": "Tessellation Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.SpatialIndexConfig.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.SpatialIndexConfig.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
