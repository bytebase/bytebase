{
  "$id": "bytebase.v1.UpdateReleaseRequest.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "allow_missing": {
      "description": "If set to true, and the release is not found, a new release will be created.\n In this situation, `update_mask` is ignored.",
      "type": "boolean"
    },
    "release": {
      "$ref": "bytebase.v1.Release.schema.strict.json",
      "description": "The release to update."
    },
    "update_mask": {
      "$ref": "google.protobuf.FieldMask.schema.strict.json",
      "description": "The list of fields to be updated."
    }
  },
  "required": [
    "allow_missing"
  ],
  "title": "Update Release Request",
  "type": "object"
}
