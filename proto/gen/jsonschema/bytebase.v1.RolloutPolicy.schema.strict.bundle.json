{
  "$defs": {
    "bytebase.v1.RolloutPolicy.Checkers.RequiredStatusChecks.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "plan_check_enforcement": {
          "description": "Enforcement level for plan check results during rollout validation.",
          "enum": [
            "PLAN_CHECK_ENFORCEMENT_UNSPECIFIED",
            "ERROR_ONLY",
            "STRICT"
          ],
          "title": "Plan Check Enforcement",
          "type": "string"
        }
      },
      "required": [
        "plan_check_enforcement"
      ],
      "title": "Required Status Checks",
      "type": "object"
    },
    "bytebase.v1.RolloutPolicy.Checkers.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "required_issue_approval": {
          "description": "Whether issue approval is required before proceeding with rollout.",
          "type": "boolean"
        },
        "required_status_checks": {
          "$ref": "#/$defs/bytebase.v1.RolloutPolicy.Checkers.RequiredStatusChecks.schema.strict.json",
          "description": "Status checks that must pass before rollout can be executed."
        }
      },
      "required": [
        "required_issue_approval"
      ],
      "title": "Checkers",
      "type": "object"
    },
    "bytebase.v1.RolloutPolicy.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Rollout policy configuration.",
      "properties": {
        "automatic": {
          "description": "Whether rollout is automatic without manual approval.",
          "type": "boolean"
        },
        "checkers": {
          "$ref": "#/$defs/bytebase.v1.RolloutPolicy.Checkers.schema.strict.json",
          "description": "Checkers that must pass before rollout execution.\n These checks are performed in UI workflows only."
        },
        "roles": {
          "description": "The roles that can approve rollout execution.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "required": [
        "automatic"
      ],
      "title": "Rollout Policy",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.RolloutPolicy.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.RolloutPolicy.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
