{
  "$defs": {
    "bytebase.store.ReleasePayload.File.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(enableGhost)$": {
          "default": false,
          "description": "Whether to use gh-ost for online schema migration.",
          "type": "boolean"
        },
        "^(sheetSha256)$": {
          "default": "",
          "description": "The SHA256 hash value of the sheet.",
          "type": "string"
        }
      },
      "properties": {
        "enable_ghost": {
          "default": false,
          "description": "Whether to use gh-ost for online schema migration.",
          "type": "boolean"
        },
        "id": {
          "default": "",
          "description": "The unique identifier for the file.",
          "type": "string"
        },
        "path": {
          "default": "",
          "description": "The path of the file, e.g., `2.2/V0001_create_table.sql`.",
          "type": "string"
        },
        "sheet": {
          "default": "",
          "description": "The sheet that holds the content.\n Format: projects/{project}/sheets/{sheet}",
          "type": "string"
        },
        "sheet_sha256": {
          "default": "",
          "description": "The SHA256 hash value of the sheet.",
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^SCHEMA_CHANGE_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "VERSIONED",
                "DECLARATIVE"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Schema Change Type"
        },
        "version": {
          "default": "",
          "type": "string"
        }
      },
      "title": "File",
      "type": "object"
    },
    "bytebase.store.ReleasePayload.VCSSource.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(vcsType)$": {
          "anyOf": [
            {
              "pattern": "^VCS_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "GITHUB",
                "GITLAB",
                "BITBUCKET",
                "AZURE_DEVOPS"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "VCS Type"
        }
      },
      "properties": {
        "url": {
          "default": "",
          "type": "string"
        },
        "vcs_type": {
          "anyOf": [
            {
              "pattern": "^VCS_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "GITHUB",
                "GITLAB",
                "BITBUCKET",
                "AZURE_DEVOPS"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "VCS Type"
        }
      },
      "title": "VCS Source",
      "type": "object"
    },
    "bytebase.store.ReleasePayload.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(vcsSource)$": {
          "$ref": "#/$defs/bytebase.store.ReleasePayload.VCSSource.schema.json"
        }
      },
      "properties": {
        "files": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ReleasePayload.File.schema.json"
          },
          "type": "array"
        },
        "title": {
          "default": "",
          "type": "string"
        },
        "vcs_source": {
          "$ref": "#/$defs/bytebase.store.ReleasePayload.VCSSource.schema.json"
        }
      },
      "title": "Release Payload",
      "type": "object"
    }
  },
  "$id": "bytebase.store.ReleasePayload.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.ReleasePayload.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
