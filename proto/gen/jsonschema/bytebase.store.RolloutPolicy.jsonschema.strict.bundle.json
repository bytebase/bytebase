{
  "$defs": {
    "bytebase.store.RolloutPolicy.Checkers.RequiredStatusChecks.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "planCheckEnforcement": {
          "description": "Enforcement level for plan check results during rollout validation.",
          "enum": [
            "PLAN_CHECK_ENFORCEMENT_UNSPECIFIED",
            "ERROR_ONLY",
            "STRICT"
          ],
          "title": "Plan Check Enforcement",
          "type": "string"
        }
      },
      "required": [
        "planCheckEnforcement"
      ],
      "title": "Required Status Checks",
      "type": "object"
    },
    "bytebase.store.RolloutPolicy.Checkers.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "requiredIssueApproval": {
          "description": "Whether issue approval is required before proceeding with rollout.",
          "type": "boolean"
        },
        "requiredStatusChecks": {
          "$ref": "#/$defs/bytebase.store.RolloutPolicy.Checkers.RequiredStatusChecks.jsonschema.strict.json",
          "description": "Status checks that must pass before rollout can be executed."
        }
      },
      "required": [
        "requiredIssueApproval"
      ],
      "title": "Checkers",
      "type": "object"
    },
    "bytebase.store.RolloutPolicy.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "automatic": {
          "type": "boolean"
        },
        "checkers": {
          "$ref": "#/$defs/bytebase.store.RolloutPolicy.Checkers.jsonschema.strict.json",
          "description": "Checkers that must pass before rollout execution.\n These checks are performed in UI workflows only."
        },
        "roles": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "required": [
        "automatic"
      ],
      "title": "Rollout Policy",
      "type": "object"
    }
  },
  "$id": "bytebase.store.RolloutPolicy.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.RolloutPolicy.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
