{
  "$id": "bytebase.v1.EnumTypeMetadata.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "comment": {
      "description": "The comment describing the enum type.",
      "type": "string"
    },
    "name": {
      "description": "The name of a type.",
      "type": "string"
    },
    "skip_dump": {
      "description": "Whether to skip this enum type during schema dump operations.",
      "type": "boolean"
    },
    "values": {
      "description": "The enum values of a type.",
      "items": {
        "type": "string"
      },
      "type": "array"
    }
  },
  "required": [
    "name",
    "comment",
    "skip_dump"
  ],
  "title": "Enum Type Metadata",
  "type": "object"
}
