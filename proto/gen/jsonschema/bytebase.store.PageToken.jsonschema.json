{
  "$id": "bytebase.store.PageToken.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "PageToken is used internally for obfuscating pagination tokens.",
  "properties": {
    "limit": {
      "anyOf": [
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "Maximum number of items to return."
    },
    "offset": {
      "anyOf": [
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "Number of items to skip before starting to return results."
    }
  },
  "title": "Page Token",
  "type": "object"
}
