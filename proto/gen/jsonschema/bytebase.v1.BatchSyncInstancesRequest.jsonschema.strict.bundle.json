{
  "$defs": {
    "bytebase.v1.BatchSyncInstancesRequest.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "requests": {
          "description": "The request message specifying the instances to sync.\n A maximum of 1000 instances can be synced in a batch.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.SyncInstanceRequest.jsonschema.strict.json"
          },
          "type": "array"
        }
      },
      "title": "Batch Sync Instances Request",
      "type": "object"
    },
    "bytebase.v1.SyncInstanceRequest.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "enableFullSync": {
          "description": "When full sync is enabled, all databases in the instance will be synchronized. Otherwise, only\n the instance metadata (such as the database list) and any newly discovered databases will be synced.",
          "type": "boolean"
        },
        "name": {
          "description": "The name of instance.\n Format: instances/{instance}",
          "type": "string"
        }
      },
      "required": [
        "name",
        "enableFullSync"
      ],
      "title": "Sync Instance Request",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.BatchSyncInstancesRequest.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.BatchSyncInstancesRequest.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
