{
  "$defs": {
    "bytebase.store.PlanConfig.ChangeDatabaseConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(enableGhost)$": {
          "default": false,
          "description": "Whether to use gh-ost for online schema migration.",
          "type": "boolean"
        },
        "^(enablePriorBackup)$": {
          "default": false,
          "description": "If set, a backup of the modified data will be created automatically before any changes are applied.",
          "type": "boolean"
        },
        "^(ghostFlags)$": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "properties": {
        "enable_ghost": {
          "default": false,
          "description": "Whether to use gh-ost for online schema migration.",
          "type": "boolean"
        },
        "enable_prior_backup": {
          "default": false,
          "description": "If set, a backup of the modified data will be created automatically before any changes are applied.",
          "type": "boolean"
        },
        "ghost_flags": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "release": {
          "default": "",
          "description": "The resource name of the release.\n Format: projects/{project}/releases/{release}",
          "type": "string"
        },
        "sheet": {
          "default": "",
          "description": "The resource name of the sheet.\n Format: projects/{project}/sheets/{sheet}",
          "type": "string"
        },
        "targets": {
          "description": "The list of targets.\n Multi-database format: [instances/{instance-id}/databases/{database-name}].\n Single database group format: [projects/{project}/databaseGroups/{databaseGroup}].",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "MIGRATE",
                "SDL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Type"
        }
      },
      "title": "Change Database Config",
      "type": "object"
    },
    "bytebase.store.PlanConfig.CreateDatabaseConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(characterSet)$": {
          "default": "",
          "description": "character_set is the character set of the database.",
          "type": "string"
        }
      },
      "properties": {
        "character_set": {
          "default": "",
          "description": "character_set is the character set of the database.",
          "type": "string"
        },
        "cluster": {
          "default": "",
          "description": "cluster is the cluster of the database. This is only applicable to ClickHouse for \"ON CLUSTER \u003c\u003ccluster\u003e\u003e\".",
          "type": "string"
        },
        "collation": {
          "default": "",
          "description": "collation is the collation of the database.",
          "type": "string"
        },
        "database": {
          "default": "",
          "description": "The name of the database to create.",
          "type": "string"
        },
        "environment": {
          "default": "",
          "description": "The environment resource.\n Format: environments/prod where prod is the environment resource ID.",
          "type": "string"
        },
        "owner": {
          "default": "",
          "description": "owner is the owner of the database. This is only applicable to Postgres for \"WITH OWNER \u003c\u003cowner\u003e\u003e\".",
          "type": "string"
        },
        "table": {
          "default": "",
          "description": "table is the name of the table, if it is not empty, Bytebase should create a table after creating the database.\n For example, in MongoDB, it only creates the database when we first store data in that database.",
          "type": "string"
        },
        "target": {
          "default": "",
          "description": "The resource name of the instance on which the database is created.\n Format: instances/{instance}",
          "type": "string"
        }
      },
      "title": "Create Database Config",
      "type": "object"
    },
    "bytebase.store.PlanConfig.Deployment.DatabaseGroupMapping.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(databaseGroup)$": {
          "default": "",
          "description": "Format: projects/{project}/databaseGroups/{databaseGroup}.",
          "type": "string"
        }
      },
      "properties": {
        "database_group": {
          "default": "",
          "description": "Format: projects/{project}/databaseGroups/{databaseGroup}.",
          "type": "string"
        },
        "databases": {
          "description": "Format: instances/{instance-id}/databases/{database-name}.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Database Group Mapping",
      "type": "object"
    },
    "bytebase.store.PlanConfig.Deployment.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(databaseGroupMappings)$": {
          "description": "The database group mapping.",
          "items": {
            "$ref": "#/$defs/bytebase.store.PlanConfig.Deployment.DatabaseGroupMapping.schema.json"
          },
          "type": "array"
        }
      },
      "properties": {
        "database_group_mappings": {
          "description": "The database group mapping.",
          "items": {
            "$ref": "#/$defs/bytebase.store.PlanConfig.Deployment.DatabaseGroupMapping.schema.json"
          },
          "type": "array"
        },
        "environments": {
          "description": "The environments deploy order.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Deployment",
      "type": "object"
    },
    "bytebase.store.PlanConfig.ExportDataConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "format": {
          "anyOf": [
            {
              "pattern": "^FORMAT_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "CSV",
                "JSON",
                "SQL",
                "XLSX"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "The format of the exported file.",
          "title": "Export Format"
        },
        "password": {
          "description": "The zip password provided by users.\n Leave it empty if there is no need to encrypt the zip file.",
          "type": "string"
        },
        "sheet": {
          "default": "",
          "description": "The resource name of the sheet.\n Format: projects/{project}/sheets/{sheet}",
          "type": "string"
        },
        "targets": {
          "description": "The list of targets.\n Multi-database format: [instances/{instance-id}/databases/{database-name}].\n Single database group format: [projects/{project}/databaseGroups/{databaseGroup}].",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Export Data Config",
      "type": "object"
    },
    "bytebase.store.PlanConfig.Spec.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(changeDatabaseConfig)$": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.ChangeDatabaseConfig.schema.json"
        },
        "^(createDatabaseConfig)$": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.CreateDatabaseConfig.schema.json"
        },
        "^(exportDataConfig)$": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.ExportDataConfig.schema.json"
        }
      },
      "properties": {
        "change_database_config": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.ChangeDatabaseConfig.schema.json"
        },
        "create_database_config": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.CreateDatabaseConfig.schema.json"
        },
        "export_data_config": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.ExportDataConfig.schema.json"
        },
        "id": {
          "default": "",
          "description": "A UUID4 string that uniquely identifies the Spec.",
          "type": "string"
        }
      },
      "title": "Spec",
      "type": "object"
    },
    "bytebase.store.PlanConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "deployment": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.Deployment.schema.json"
        },
        "specs": {
          "items": {
            "$ref": "#/$defs/bytebase.store.PlanConfig.Spec.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Plan Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.PlanConfig.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.PlanConfig.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
