{
  "$defs": {
    "bytebase.v1.Worksheet.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(contentSize)$": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "content_size is the full size of the content, may not match the size of the `content` field."
        },
        "^(createTime)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "The create time of the worksheet."
        },
        "^(updateTime)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "The last update time of the worksheet."
        }
      },
      "properties": {
        "content": {
          "default": null,
          "description": "The content of the worksheet.\n By default, it will be cut off in SearchWorksheet() method. If it doesn't match the `content_size`, you can\n use GetWorksheet() request to retrieve the full content.",
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "content_size": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "content_size is the full size of the content, may not match the size of the `content` field."
        },
        "create_time": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "The create time of the worksheet."
        },
        "creator": {
          "default": "",
          "description": "The creator of the Worksheet.\n Format: users/{email}",
          "type": "string"
        },
        "database": {
          "default": "",
          "description": "The database resource name.\n Format: instances/{instance}/databases/{database}\n If the database parent doesn't exist, the database field is empty.",
          "type": "string"
        },
        "folders": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "description": "The name of the worksheet resource, generated by the server.\n Canonical parent is project.\n Format: worksheets/{worksheet}",
          "type": "string"
        },
        "project": {
          "default": "",
          "description": "The project resource name.\n Format: projects/{project}",
          "type": "string"
        },
        "starred": {
          "default": false,
          "description": "starred indicates whether the worksheet is starred by the current authenticated user.",
          "type": "boolean"
        },
        "title": {
          "default": "",
          "description": "The title of the worksheet.",
          "maxLength": 200,
          "type": "string"
        },
        "update_time": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json",
          "description": "The last update time of the worksheet."
        },
        "visibility": {
          "anyOf": [
            {
              "pattern": "^VISIBILITY_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "PROJECT_READ",
                "PROJECT_WRITE",
                "PRIVATE"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Visibility"
        }
      },
      "title": "Worksheet",
      "type": "object"
    },
    "google.protobuf.Timestamp.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    }
  },
  "$id": "bytebase.v1.Worksheet.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.Worksheet.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
