{
  "$defs": {
    "bytebase.store.RolloutPolicy.Checkers.RequiredStatusChecks.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(planCheckEnforcement)$": {
          "anyOf": [
            {
              "pattern": "^PLAN_CHECK_ENFORCEMENT_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "ERROR_ONLY",
                "STRICT"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "Enforcement level for plan check results during rollout validation.",
          "title": "Plan Check Enforcement"
        }
      },
      "properties": {
        "plan_check_enforcement": {
          "anyOf": [
            {
              "pattern": "^PLAN_CHECK_ENFORCEMENT_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "ERROR_ONLY",
                "STRICT"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "Enforcement level for plan check results during rollout validation.",
          "title": "Plan Check Enforcement"
        }
      },
      "title": "Required Status Checks",
      "type": "object"
    },
    "bytebase.store.RolloutPolicy.Checkers.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(requiredIssueApproval)$": {
          "default": false,
          "description": "Whether issue approval is required before proceeding with rollout.",
          "type": "boolean"
        },
        "^(requiredStatusChecks)$": {
          "$ref": "#/$defs/bytebase.store.RolloutPolicy.Checkers.RequiredStatusChecks.schema.json",
          "description": "Status checks that must pass before rollout can be executed."
        }
      },
      "properties": {
        "required_issue_approval": {
          "default": false,
          "description": "Whether issue approval is required before proceeding with rollout.",
          "type": "boolean"
        },
        "required_status_checks": {
          "$ref": "#/$defs/bytebase.store.RolloutPolicy.Checkers.RequiredStatusChecks.schema.json",
          "description": "Status checks that must pass before rollout can be executed."
        }
      },
      "title": "Checkers",
      "type": "object"
    },
    "bytebase.store.RolloutPolicy.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "automatic": {
          "default": false,
          "type": "boolean"
        },
        "checkers": {
          "$ref": "#/$defs/bytebase.store.RolloutPolicy.Checkers.schema.json",
          "description": "Checkers that must pass before rollout execution.\n These checks are performed in UI workflows only."
        },
        "roles": {
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Rollout Policy",
      "type": "object"
    }
  },
  "$id": "bytebase.store.RolloutPolicy.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.RolloutPolicy.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
