{
  "$id": "bytebase.store.ReleasePayload.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(vcsSource)$": {
      "$ref": "bytebase.store.ReleasePayload.VCSSource.schema.json"
    }
  },
  "properties": {
    "files": {
      "items": {
        "$ref": "bytebase.store.ReleasePayload.File.schema.json"
      },
      "type": "array"
    },
    "title": {
      "default": "",
      "type": "string"
    },
    "vcs_source": {
      "$ref": "bytebase.store.ReleasePayload.VCSSource.schema.json"
    }
  },
  "title": "Release Payload",
  "type": "object"
}
