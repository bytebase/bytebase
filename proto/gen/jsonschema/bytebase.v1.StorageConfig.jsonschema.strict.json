{
  "$id": "bytebase.v1.StorageConfig.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "StorageConfig defines storage and performance parameters for spatial indexes.",
  "properties": {
    "allowPageLocks": {
      "type": "boolean"
    },
    "allowRowLocks": {
      "type": "boolean"
    },
    "buffering": {
      "description": "Buffering mode for PostgreSQL (auto, on, off)",
      "type": "string"
    },
    "commitInterval": {
      "maximum": 2147483647,
      "minimum": -2147483648,
      "type": "integer"
    },
    "dataCompression": {
      "type": "string"
    },
    "dropExisting": {
      "type": "boolean"
    },
    "fillfactor": {
      "description": "Fill factor percentage (1-100)",
      "maximum": 2147483647,
      "minimum": -2147483648,
      "type": "integer"
    },
    "maxdop": {
      "maximum": 2147483647,
      "minimum": -2147483648,
      "type": "integer"
    },
    "online": {
      "type": "boolean"
    },
    "padIndex": {
      "description": "SQL Server specific parameters",
      "type": "boolean"
    },
    "sdoLevel": {
      "maximum": 2147483647,
      "minimum": -2147483648,
      "type": "integer"
    },
    "sortInTempdb": {
      "type": "string"
    },
    "tablespace": {
      "description": "Tablespace configuration for Oracle",
      "type": "string"
    },
    "workTablespace": {
      "type": "string"
    }
  },
  "required": [
    "fillfactor",
    "buffering",
    "tablespace",
    "workTablespace",
    "sdoLevel",
    "commitInterval",
    "padIndex",
    "sortInTempdb",
    "dropExisting",
    "online",
    "allowRowLocks",
    "allowPageLocks",
    "maxdop",
    "dataCompression"
  ],
  "title": "Storage Config",
  "type": "object"
}
