{
  "$defs": {
    "bytebase.store.TaskMetadata.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "comment": {
          "description": "The comment of the task.",
          "type": "string"
        },
        "condition": {
          "description": "The condition of the task.",
          "type": "string"
        },
        "definition": {
          "description": "The definition of the task.",
          "type": "string"
        },
        "id": {
          "description": "The Snowflake-generated ID of the task.\n Example: 01ad32a0-1bb6-5e93-0000-000000000001.",
          "type": "string"
        },
        "name": {
          "description": "The name of the task.",
          "type": "string"
        },
        "owner": {
          "description": "The owner of the task.",
          "type": "string"
        },
        "predecessors": {
          "description": "The predecessor tasks of the task.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "schedule": {
          "description": "The schedule interval of the task.",
          "type": "string"
        },
        "state": {
          "description": "The state of the task.",
          "enum": [
            "STATE_UNSPECIFIED",
            "STATE_STARTED",
            "STATE_SUSPENDED"
          ],
          "title": "State",
          "type": "string"
        },
        "warehouse": {
          "description": "The warehouse of the task.",
          "type": "string"
        }
      },
      "required": [
        "name",
        "id",
        "owner",
        "comment",
        "warehouse",
        "schedule",
        "state",
        "condition",
        "definition"
      ],
      "title": "Task Metadata",
      "type": "object"
    }
  },
  "$id": "bytebase.store.TaskMetadata.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.TaskMetadata.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
