{
  "$defs": {
    "bytebase.v1.Announcement.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "level": {
          "description": "The alert level of announcement",
          "enum": [
            "ALERT_LEVEL_UNSPECIFIED",
            "INFO",
            "WARNING",
            "CRITICAL"
          ],
          "title": "Alert Level",
          "type": "string"
        },
        "link": {
          "description": "The optional link, user can follow the link to check extra details",
          "type": "string"
        },
        "text": {
          "description": "The text of announcement",
          "type": "string"
        }
      },
      "required": [
        "level",
        "text",
        "link"
      ],
      "title": "Announcement",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.Announcement.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.Announcement.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
