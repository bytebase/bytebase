{
  "$id": "bytebase.v1.QueryHistory.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "create_time": {
      "$ref": "google.protobuf.Timestamp.schema.strict.json"
    },
    "creator": {
      "type": "string"
    },
    "database": {
      "description": "The database name to execute the query.\n Format: instances/{instance}/databases/{databaseName}",
      "type": "string"
    },
    "duration": {
      "$ref": "google.protobuf.Duration.schema.strict.json"
    },
    "error": {
      "type": "string"
    },
    "name": {
      "description": "The name for the query history.\n Format: queryHistories/{uid}",
      "type": "string"
    },
    "statement": {
      "type": "string"
    },
    "type": {
      "enum": [
        "TYPE_UNSPECIFIED",
        "QUERY",
        "EXPORT"
      ],
      "title": "Type",
      "type": "string"
    }
  },
  "required": [
    "name",
    "database",
    "creator",
    "statement",
    "type"
  ],
  "title": "Query History",
  "type": "object"
}
