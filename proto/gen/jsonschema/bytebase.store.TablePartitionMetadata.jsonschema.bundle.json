{
  "$defs": {
    "bytebase.store.BoundingBox.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the bounding box for spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        }
      },
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.store.CheckConstraintMetadata.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "expression": {
          "default": "",
          "description": "The expression is the expression of a check constraint.",
          "type": "string"
        },
        "name": {
          "default": "",
          "description": "The name of the check constraint.",
          "type": "string"
        }
      },
      "title": "Check Constraint Metadata",
      "type": "object"
    },
    "bytebase.store.DimensionalConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionalConfig defines dimensional and constraint parameters for spatial indexes.",
      "patternProperties": {
        "^(data_type)$": {
          "default": "",
          "description": "Spatial data type\n Examples: GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.",
          "type": "string"
        },
        "^(layer_gtype)$": {
          "default": "",
          "description": "Oracle geometry type constraint\n Examples: POINT, LINE, POLYGON, COLLECTION",
          "type": "string"
        },
        "^(operator_class)$": {
          "default": "",
          "description": "PostgreSQL operator class\n Examples: gist_geometry_ops_2d, gist_geometry_ops_nd, etc.",
          "type": "string"
        },
        "^(parallel_build)$": {
          "default": false,
          "description": "Parallel index creation",
          "type": "boolean"
        }
      },
      "properties": {
        "dataType": {
          "default": "",
          "description": "Spatial data type\n Examples: GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.",
          "type": "string"
        },
        "dimensions": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of dimensions (2-4, default 2)"
        },
        "layerGtype": {
          "default": "",
          "description": "Oracle geometry type constraint\n Examples: POINT, LINE, POLYGON, COLLECTION",
          "type": "string"
        },
        "operatorClass": {
          "default": "",
          "description": "PostgreSQL operator class\n Examples: gist_geometry_ops_2d, gist_geometry_ops_nd, etc.",
          "type": "string"
        },
        "parallelBuild": {
          "default": false,
          "description": "Parallel index creation",
          "type": "boolean"
        }
      },
      "title": "Dimensional Config",
      "type": "object"
    },
    "bytebase.store.ExcludeConstraintMetadata.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "expression": {
          "default": "",
          "description": "The expression is the full EXCLUDE constraint definition including \"EXCLUDE\" keyword.\n Example: \"EXCLUDE USING gist (room_id WITH =, during WITH \u0026\u0026)\"",
          "type": "string"
        },
        "name": {
          "default": "",
          "description": "The name of the EXCLUDE constraint.",
          "type": "string"
        }
      },
      "title": "Exclude Constraint Metadata",
      "type": "object"
    },
    "bytebase.store.GridLevel.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a grid level for spatial tessellation.",
      "properties": {
        "density": {
          "default": "",
          "type": "string"
        },
        "level": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        }
      },
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.store.IndexMetadata.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "IndexMetadata is the metadata for indexes.",
      "patternProperties": {
        "^(is_constraint)$": {
          "default": false,
          "description": "It's a PostgreSQL specific field.\n The unique constraint and unique index are not the same thing in PostgreSQL.",
          "type": "boolean"
        },
        "^(key_length)$": {
          "description": "The ordered list of key lengths for the index.\n If the key length is not specified, it is -1.",
          "items": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "pattern": "^-?[0-9]+$",
                "type": "string"
              }
            ]
          },
          "type": "array"
        },
        "^(opclass_defaults)$": {
          "description": "True if the operator class is the default. (PostgreSQL specific).",
          "items": {
            "type": "boolean"
          },
          "type": "array"
        },
        "^(opclass_names)$": {
          "description": "https://www.postgresql.org/docs/current/catalog-pg-opclass.html\n Name of the operator class for each column. (PostgreSQL specific).",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "^(parent_index_name)$": {
          "default": "",
          "description": "The index name of the parent index.",
          "type": "string"
        },
        "^(parent_index_schema)$": {
          "default": "",
          "description": "The schema name of the parent index.",
          "type": "string"
        },
        "^(spatial_config)$": {
          "$ref": "#/$defs/bytebase.store.SpatialIndexConfig.jsonschema.json",
          "description": "Spatial index specific configuration"
        }
      },
      "properties": {
        "comment": {
          "default": "",
          "description": "The comment is the comment of an index.",
          "type": "string"
        },
        "definition": {
          "default": "",
          "description": "The definition of an index.",
          "type": "string"
        },
        "descending": {
          "description": "The ordered list of descending flags for the index columns.",
          "items": {
            "type": "boolean"
          },
          "type": "array"
        },
        "expressions": {
          "description": "The expressions are the ordered columns or expressions of an index.\n This could refer to a column or an expression.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "granularity": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "The number of granules in the block. It's a ClickHouse specific field."
        },
        "isConstraint": {
          "default": false,
          "description": "It's a PostgreSQL specific field.\n The unique constraint and unique index are not the same thing in PostgreSQL.",
          "type": "boolean"
        },
        "keyLength": {
          "description": "The ordered list of key lengths for the index.\n If the key length is not specified, it is -1.",
          "items": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "pattern": "^-?[0-9]+$",
                "type": "string"
              }
            ]
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "description": "The name of the index.",
          "type": "string"
        },
        "opclassDefaults": {
          "description": "True if the operator class is the default. (PostgreSQL specific).",
          "items": {
            "type": "boolean"
          },
          "type": "array"
        },
        "opclassNames": {
          "description": "https://www.postgresql.org/docs/current/catalog-pg-opclass.html\n Name of the operator class for each column. (PostgreSQL specific).",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "parentIndexName": {
          "default": "",
          "description": "The index name of the parent index.",
          "type": "string"
        },
        "parentIndexSchema": {
          "default": "",
          "description": "The schema name of the parent index.",
          "type": "string"
        },
        "primary": {
          "default": false,
          "description": "The primary is whether the index is a primary key index.",
          "type": "boolean"
        },
        "spatialConfig": {
          "$ref": "#/$defs/bytebase.store.SpatialIndexConfig.jsonschema.json",
          "description": "Spatial index specific configuration"
        },
        "type": {
          "default": "",
          "description": "The type is the type of an index.",
          "type": "string"
        },
        "unique": {
          "default": false,
          "description": "The unique is whether the index is unique.",
          "type": "boolean"
        },
        "visible": {
          "default": false,
          "description": "The visible is whether the index is visible.",
          "type": "boolean"
        }
      },
      "title": "Index Metadata",
      "type": "object"
    },
    "bytebase.store.SpatialIndexConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "SpatialIndexConfig is the configuration for spatial indexes across different database engines.",
      "patternProperties": {
        "^(engine_specific)$": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "Database-specific parameters (stored as key-value pairs for extensibility)",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "properties": {
        "dimensional": {
          "$ref": "#/$defs/bytebase.store.DimensionalConfig.jsonschema.json",
          "description": "Dimensional and constraint parameters"
        },
        "engineSpecific": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "Database-specific parameters (stored as key-value pairs for extensibility)",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "method": {
          "default": "",
          "description": "Index method/type (database-specific)\n Examples: \"SPATIAL\" (MySQL/SQL Server), \"GIST\"/\"SPGIST\" (PostgreSQL), \"MDSYS.SPATIAL_INDEX_V2\" (Oracle)",
          "type": "string"
        },
        "storage": {
          "$ref": "#/$defs/bytebase.store.StorageConfig.jsonschema.json",
          "description": "Storage and performance parameters"
        },
        "tessellation": {
          "$ref": "#/$defs/bytebase.store.TessellationConfig.jsonschema.json",
          "description": "Tessellation configuration (primarily SQL Server)"
        }
      },
      "title": "Spatial Index Config",
      "type": "object"
    },
    "bytebase.store.StorageConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "StorageConfig defines storage and performance parameters for spatial indexes.",
      "patternProperties": {
        "^(allow_page_locks)$": {
          "default": false,
          "type": "boolean"
        },
        "^(allow_row_locks)$": {
          "default": false,
          "type": "boolean"
        },
        "^(commit_interval)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(data_compression)$": {
          "default": "",
          "type": "string"
        },
        "^(drop_existing)$": {
          "default": false,
          "type": "boolean"
        },
        "^(pad_index)$": {
          "default": false,
          "description": "SQL Server parameters",
          "type": "boolean"
        },
        "^(sdo_level)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(sort_in_tempdb)$": {
          "default": "",
          "type": "string"
        },
        "^(work_tablespace)$": {
          "default": "",
          "type": "string"
        }
      },
      "properties": {
        "allowPageLocks": {
          "default": false,
          "type": "boolean"
        },
        "allowRowLocks": {
          "default": false,
          "type": "boolean"
        },
        "buffering": {
          "default": "",
          "type": "string"
        },
        "commitInterval": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "dataCompression": {
          "default": "",
          "type": "string"
        },
        "dropExisting": {
          "default": false,
          "type": "boolean"
        },
        "fillfactor": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "PostgreSQL parameters"
        },
        "maxdop": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "online": {
          "default": false,
          "type": "boolean"
        },
        "padIndex": {
          "default": false,
          "description": "SQL Server parameters",
          "type": "boolean"
        },
        "sdoLevel": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "sortInTempdb": {
          "default": "",
          "type": "string"
        },
        "tablespace": {
          "default": "",
          "description": "Oracle parameters",
          "type": "string"
        },
        "workTablespace": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Storage Config",
      "type": "object"
    },
    "bytebase.store.TablePartitionMetadata.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TablePartitionMetadata is the metadata for table partitions.",
      "patternProperties": {
        "^(check_constraints)$": {
          "items": {
            "$ref": "#/$defs/bytebase.store.CheckConstraintMetadata.jsonschema.json"
          },
          "type": "array"
        },
        "^(exclude_constraints)$": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ExcludeConstraintMetadata.jsonschema.json"
          },
          "type": "array"
        },
        "^(use_default)$": {
          "default": "",
          "description": "The use_default is whether the users use the default partition, it stores\n the different value for different database engines. For MySQL, it's [INT]\n type, 0 means not use default partition, otherwise, it's equals to number\n in syntax [SUB]PARTITION {number}.",
          "type": "string"
        }
      },
      "properties": {
        "checkConstraints": {
          "items": {
            "$ref": "#/$defs/bytebase.store.CheckConstraintMetadata.jsonschema.json"
          },
          "type": "array"
        },
        "excludeConstraints": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ExcludeConstraintMetadata.jsonschema.json"
          },
          "type": "array"
        },
        "expression": {
          "default": "",
          "description": "The expression is the expression of a table partition.\n For PostgreSQL, the expression is the text of {FOR VALUES\n partition_bound_spec}, see\n https://www.postgresql.org/docs/current/sql-createtable.html. For MySQL,\n the expression is the `expr` or `column_list` of the following syntax.\n PARTITION BY\n    { [LINEAR] HASH(expr)\n    | [LINEAR] KEY [ALGORITHM={1 | 2}] (column_list)\n    | RANGE{(expr) | COLUMNS(column_list)}\n    | LIST{(expr) | COLUMNS(column_list)} }.",
          "type": "string"
        },
        "indexes": {
          "items": {
            "$ref": "#/$defs/bytebase.store.IndexMetadata.jsonschema.json"
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "description": "The name of the table partition.",
          "type": "string"
        },
        "subpartitions": {
          "description": "The subpartitions is the list of subpartitions in a table partition.",
          "items": {
            "$ref": "#/$defs/bytebase.store.TablePartitionMetadata.jsonschema.json"
          },
          "type": "array"
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "RANGE",
                "RANGE_COLUMNS",
                "LIST",
                "LIST_COLUMNS",
                "HASH",
                "LINEAR_HASH",
                "KEY",
                "LINEAR_KEY"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "The type of a table partition.",
          "title": "Type"
        },
        "useDefault": {
          "default": "",
          "description": "The use_default is whether the users use the default partition, it stores\n the different value for different database engines. For MySQL, it's [INT]\n type, 0 means not use default partition, otherwise, it's equals to number\n in syntax [SUB]PARTITION {number}.",
          "type": "string"
        },
        "value": {
          "default": "",
          "description": "The value is the value of a table partition.\n For MySQL, the value is for RANGE and LIST partition types,\n - For a RANGE partition, it contains the value set in the partition's\n VALUES LESS THAN clause, which can be either an integer or MAXVALUE.\n - For a LIST partition, this column contains the values defined in the\n partition's VALUES IN clause, which is a list of comma-separated integer\n values.\n - For others, it's an empty string.",
          "type": "string"
        }
      },
      "title": "Table Partition Metadata",
      "type": "object"
    },
    "bytebase.store.TessellationConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "patternProperties": {
        "^(bounding_box)$": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.jsonschema.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "^(cells_per_object)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Cells per object (SQL Server)"
        },
        "^(grid_levels)$": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.jsonschema.json"
          },
          "type": "array"
        }
      },
      "properties": {
        "boundingBox": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.jsonschema.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "cellsPerObject": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Cells per object (SQL Server)"
        },
        "gridLevels": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.jsonschema.json"
          },
          "type": "array"
        },
        "scheme": {
          "default": "",
          "description": "Tessellation scheme\n Examples: GEOMETRY_GRID, GEOGRAPHY_GRID, GEOMETRY_AUTO_GRID, GEOGRAPHY_AUTO_GRID",
          "type": "string"
        }
      },
      "title": "Tessellation Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.TablePartitionMetadata.jsonschema.bundle.json",
  "$ref": "#/$defs/bytebase.store.TablePartitionMetadata.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
