{
  "$defs": {
    "bytebase.store.Algorithm.FullMask.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "substitution": {
          "description": "substitution is the string used to replace the original value, the\n max length of the string is 16 bytes.",
          "type": "string"
        }
      },
      "required": [
        "substitution"
      ],
      "title": "Full Mask",
      "type": "object"
    },
    "bytebase.store.Algorithm.InnerOuterMask.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "prefix_len": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "substitution": {
          "type": "string"
        },
        "suffix_len": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "type": {
          "enum": [
            "MASK_TYPE_UNSPECIFIED",
            "INNER",
            "OUTER"
          ],
          "title": "Mask Type",
          "type": "string"
        }
      },
      "required": [
        "prefix_len",
        "suffix_len",
        "substitution",
        "type"
      ],
      "title": "Inner Outer Mask",
      "type": "object"
    },
    "bytebase.store.Algorithm.MD5Mask.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "salt": {
          "description": "salt is the salt value to generate a different hash that with the word alone.",
          "type": "string"
        }
      },
      "required": [
        "salt"
      ],
      "title": "M D5 Mask",
      "type": "object"
    },
    "bytebase.store.Algorithm.RangeMask.Slice.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "end": {
          "description": "stop is the stop index of the original value, should be less than the length of the original value.",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "start": {
          "description": "start is the start index of the original value, start from 0 and should be less than stop.",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "substitution": {
          "description": "OriginalValue[start:end) would be replaced with replace_with.",
          "type": "string"
        }
      },
      "required": [
        "start",
        "end",
        "substitution"
      ],
      "title": "Slice",
      "type": "object"
    },
    "bytebase.store.Algorithm.RangeMask.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "slices": {
          "description": "We store it as a repeated field to face the fact that the original value may have multiple parts should be masked.\n But frontend can be started with a single rule easily.",
          "items": {
            "$ref": "#/$defs/bytebase.store.Algorithm.RangeMask.Slice.schema.strict.json"
          },
          "type": "array"
        }
      },
      "title": "Range Mask",
      "type": "object"
    },
    "bytebase.store.Algorithm.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "full_mask": {
          "$ref": "#/$defs/bytebase.store.Algorithm.FullMask.schema.strict.json"
        },
        "inner_outer_mask": {
          "$ref": "#/$defs/bytebase.store.Algorithm.InnerOuterMask.schema.strict.json"
        },
        "md5_mask": {
          "$ref": "#/$defs/bytebase.store.Algorithm.MD5Mask.schema.strict.json"
        },
        "range_mask": {
          "$ref": "#/$defs/bytebase.store.Algorithm.RangeMask.schema.strict.json"
        }
      },
      "title": "Algorithm",
      "type": "object"
    }
  },
  "$id": "bytebase.store.Algorithm.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.Algorithm.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
