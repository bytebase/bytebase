{
  "$defs": {
    "bytebase.store.PlanConfig.ChangeDatabaseConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "enable_ghost": {
          "description": "Whether to use gh-ost for online schema migration.",
          "type": "boolean"
        },
        "enable_prior_backup": {
          "description": "If set, a backup of the modified data will be created automatically before any changes are applied.",
          "type": "boolean"
        },
        "ghost_flags": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "release": {
          "description": "The resource name of the release.\n Format: projects/{project}/releases/{release}",
          "type": "string"
        },
        "sheet": {
          "description": "The resource name of the sheet.\n Format: projects/{project}/sheets/{sheet}",
          "type": "string"
        },
        "targets": {
          "description": "The list of targets.\n Multi-database format: [instances/{instance-id}/databases/{database-name}].\n Single database group format: [projects/{project}/databaseGroups/{databaseGroup}].",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "type": {
          "enum": [
            "TYPE_UNSPECIFIED",
            "MIGRATE",
            "SDL"
          ],
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "sheet",
        "release",
        "type",
        "enable_prior_backup",
        "enable_ghost"
      ],
      "title": "Change Database Config",
      "type": "object"
    },
    "bytebase.store.PlanConfig.CreateDatabaseConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "character_set": {
          "description": "character_set is the character set of the database.",
          "type": "string"
        },
        "cluster": {
          "description": "cluster is the cluster of the database. This is only applicable to ClickHouse for \"ON CLUSTER \u003c\u003ccluster\u003e\u003e\".",
          "type": "string"
        },
        "collation": {
          "description": "collation is the collation of the database.",
          "type": "string"
        },
        "database": {
          "description": "The name of the database to create.",
          "type": "string"
        },
        "environment": {
          "description": "The environment resource.\n Format: environments/prod where prod is the environment resource ID.",
          "type": "string"
        },
        "owner": {
          "description": "owner is the owner of the database. This is only applicable to Postgres for \"WITH OWNER \u003c\u003cowner\u003e\u003e\".",
          "type": "string"
        },
        "table": {
          "description": "table is the name of the table, if it is not empty, Bytebase should create a table after creating the database.\n For example, in MongoDB, it only creates the database when we first store data in that database.",
          "type": "string"
        },
        "target": {
          "description": "The resource name of the instance on which the database is created.\n Format: instances/{instance}",
          "type": "string"
        }
      },
      "required": [
        "target",
        "database",
        "table",
        "character_set",
        "collation",
        "cluster",
        "owner",
        "environment"
      ],
      "title": "Create Database Config",
      "type": "object"
    },
    "bytebase.store.PlanConfig.Deployment.DatabaseGroupMapping.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "database_group": {
          "description": "Format: projects/{project}/databaseGroups/{databaseGroup}.",
          "type": "string"
        },
        "databases": {
          "description": "Format: instances/{instance-id}/databases/{database-name}.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "required": [
        "database_group"
      ],
      "title": "Database Group Mapping",
      "type": "object"
    },
    "bytebase.store.PlanConfig.Deployment.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "database_group_mappings": {
          "description": "The database group mapping.",
          "items": {
            "$ref": "#/$defs/bytebase.store.PlanConfig.Deployment.DatabaseGroupMapping.schema.strict.json"
          },
          "type": "array"
        },
        "environments": {
          "description": "The environments deploy order.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Deployment",
      "type": "object"
    },
    "bytebase.store.PlanConfig.ExportDataConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "format": {
          "description": "The format of the exported file.",
          "enum": [
            "FORMAT_UNSPECIFIED",
            "CSV",
            "JSON",
            "SQL",
            "XLSX"
          ],
          "title": "Export Format",
          "type": "string"
        },
        "password": {
          "description": "The zip password provided by users.\n Leave it empty if there is no need to encrypt the zip file.",
          "type": "string"
        },
        "sheet": {
          "description": "The resource name of the sheet.\n Format: projects/{project}/sheets/{sheet}",
          "type": "string"
        },
        "targets": {
          "description": "The list of targets.\n Multi-database format: [instances/{instance-id}/databases/{database-name}].\n Single database group format: [projects/{project}/databaseGroups/{databaseGroup}].",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "required": [
        "sheet",
        "format"
      ],
      "title": "Export Data Config",
      "type": "object"
    },
    "bytebase.store.PlanConfig.Spec.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "change_database_config": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.ChangeDatabaseConfig.schema.strict.json"
        },
        "create_database_config": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.CreateDatabaseConfig.schema.strict.json"
        },
        "export_data_config": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.ExportDataConfig.schema.strict.json"
        },
        "id": {
          "description": "A UUID4 string that uniquely identifies the Spec.",
          "type": "string"
        }
      },
      "required": [
        "id"
      ],
      "title": "Spec",
      "type": "object"
    },
    "bytebase.store.PlanConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "deployment": {
          "$ref": "#/$defs/bytebase.store.PlanConfig.Deployment.schema.strict.json"
        },
        "specs": {
          "items": {
            "$ref": "#/$defs/bytebase.store.PlanConfig.Spec.schema.strict.json"
          },
          "type": "array"
        }
      },
      "title": "Plan Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.PlanConfig.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.PlanConfig.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
