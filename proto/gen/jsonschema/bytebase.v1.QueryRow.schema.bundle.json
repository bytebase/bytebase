{
  "$defs": {
    "bytebase.v1.QueryRow.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "values": {
          "description": "Row values of the query result.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.RowValue.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Query Row",
      "type": "object"
    },
    "bytebase.v1.RowValue.Timestamp.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(googleTimestamp)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json"
        }
      },
      "properties": {
        "accuracy": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "The accuracy is the number of digits after the decimal point."
        },
        "google_timestamp": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json"
        }
      },
      "title": "Timestamp",
      "type": "object"
    },
    "bytebase.v1.RowValue.TimestampTZ.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(googleTimestamp)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json"
        }
      },
      "properties": {
        "accuracy": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "google_timestamp": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json"
        },
        "offset": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "The offset is in seconds east of UTC"
        },
        "zone": {
          "default": "",
          "description": "Zone is the time zone abbreviations in timezone database such as \"PDT\",\n \"PST\". https://en.wikipedia.org/wiki/List_of_tz_database_time_zones We\n retrieve the time zone information from the timestamptz field in the\n database. A timestamp is in UTC or epoch time, and with zone info, we can\n convert it to a local time string. Zone and offset are returned by\n time.Time.Zone()",
          "type": "string"
        }
      },
      "title": "TimestampTZ",
      "type": "object"
    },
    "bytebase.v1.RowValue.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(boolValue)$": {
          "type": "boolean"
        },
        "^(bytesValue)$": {
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "^(doubleValue)$": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ]
        },
        "^(floatValue)$": {
          "anyOf": [
            {
              "maximum": 3.4028234663852886e+38,
              "minimum": -3.4028234663852886e+38,
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ]
        },
        "^(int32Value)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ]
        },
        "^(int64Value)$": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ]
        },
        "^(nullValue)$": {
          "anyOf": [
            {
              "enum": [
                "NULL_VALUE"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "title": "Null Value"
        },
        "^(stringValue)$": {
          "type": "string"
        },
        "^(timestampTzValue)$": {
          "$ref": "#/$defs/bytebase.v1.RowValue.TimestampTZ.schema.json",
          "description": "timestamp_tz_value is used for the timestamptz data type, which\n accurately represents the timestamp with location information."
        },
        "^(timestampValue)$": {
          "$ref": "#/$defs/bytebase.v1.RowValue.Timestamp.schema.json",
          "description": "timestamp_value is used for the timestamp without time zone data type,\n meaning it only includes the timestamp without any time zone or location\n info. Although it may be expressed as a UTC value, it should be seen as a\n timestamp missing location context."
        },
        "^(uint32Value)$": {
          "anyOf": [
            {
              "maximum": 4294967295,
              "minimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^[0-9]+$",
              "type": "string"
            }
          ]
        },
        "^(uint64Value)$": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^[0-9]+$",
              "type": "string"
            }
          ]
        },
        "^(valueValue)$": {
          "$ref": "#/$defs/google.protobuf.Value.schema.json",
          "description": "value_value is used for Spanner and TUPLE ARRAY MAP in Clickhouse only."
        }
      },
      "properties": {
        "bool_value": {
          "type": "boolean"
        },
        "bytes_value": {
          "pattern": "^[A-Za-z0-9+/]*={0,2}$",
          "type": "string"
        },
        "double_value": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ]
        },
        "float_value": {
          "anyOf": [
            {
              "maximum": 3.4028234663852886e+38,
              "minimum": -3.4028234663852886e+38,
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ]
        },
        "int32_value": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ]
        },
        "int64_value": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ]
        },
        "null_value": {
          "anyOf": [
            {
              "enum": [
                "NULL_VALUE"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "title": "Null Value"
        },
        "string_value": {
          "type": "string"
        },
        "timestamp_tz_value": {
          "$ref": "#/$defs/bytebase.v1.RowValue.TimestampTZ.schema.json",
          "description": "timestamp_tz_value is used for the timestamptz data type, which\n accurately represents the timestamp with location information."
        },
        "timestamp_value": {
          "$ref": "#/$defs/bytebase.v1.RowValue.Timestamp.schema.json",
          "description": "timestamp_value is used for the timestamp without time zone data type,\n meaning it only includes the timestamp without any time zone or location\n info. Although it may be expressed as a UTC value, it should be seen as a\n timestamp missing location context."
        },
        "uint32_value": {
          "anyOf": [
            {
              "maximum": 4294967295,
              "minimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^[0-9]+$",
              "type": "string"
            }
          ]
        },
        "uint64_value": {
          "anyOf": [
            {
              "minimum": 0,
              "type": "integer"
            },
            {
              "pattern": "^[0-9]+$",
              "type": "string"
            }
          ]
        },
        "value_value": {
          "$ref": "#/$defs/google.protobuf.Value.schema.json",
          "description": "value_value is used for Spanner and TUPLE ARRAY MAP in Clickhouse only."
        }
      },
      "title": "Row Value",
      "type": "object"
    },
    "google.protobuf.Timestamp.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    },
    "google.protobuf.Value.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "title": "Value"
    }
  },
  "$id": "bytebase.v1.QueryRow.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.QueryRow.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
