{
  "$id": "bytebase.store.WorkspaceApprovalSetting.Rule.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "condition": {
      "$ref": "google.type.Expr.schema.json"
    },
    "source": {
      "anyOf": [
        {
          "pattern": "^SOURCE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "CHANGE_DATABASE",
            "CREATE_DATABASE",
            "EXPORT_DATA",
            "REQUEST_ROLE"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Source"
    },
    "template": {
      "$ref": "bytebase.store.ApprovalTemplate.schema.json"
    }
  },
  "title": "Rule",
  "type": "object"
}
