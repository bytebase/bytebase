{
  "$defs": {
    "bytebase.store.DataSourceExternalSecret.AppRoleAuthOption.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(mountPath)$": {
          "default": "",
          "description": "The path where the approle auth method is mounted.",
          "type": "string"
        },
        "^(roleId)$": {
          "default": "",
          "type": "string"
        },
        "^(secretId)$": {
          "default": "",
          "description": "The secret ID for the role without TTL.",
          "type": "string"
        }
      },
      "properties": {
        "mount_path": {
          "default": "",
          "description": "The path where the approle auth method is mounted.",
          "type": "string"
        },
        "role_id": {
          "default": "",
          "type": "string"
        },
        "secret_id": {
          "default": "",
          "description": "The secret ID for the role without TTL.",
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^SECRET_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "PLAIN",
                "ENVIRONMENT"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Secret Type"
        }
      },
      "title": "App Role Auth Option",
      "type": "object"
    },
    "bytebase.store.DataSourceExternalSecret.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(appRole)$": {
          "$ref": "#/$defs/bytebase.store.DataSourceExternalSecret.AppRoleAuthOption.schema.json"
        },
        "^(authType)$": {
          "anyOf": [
            {
              "pattern": "^AUTH_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "TOKEN",
                "VAULT_APP_ROLE"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Auth Type"
        },
        "^(engineName)$": {
          "default": "",
          "description": "engine name is the name for secret engine.",
          "type": "string"
        },
        "^(obfuscatedVaultSslCa)$": {
          "default": "",
          "type": "string"
        },
        "^(obfuscatedVaultSslCert)$": {
          "default": "",
          "type": "string"
        },
        "^(obfuscatedVaultSslKey)$": {
          "default": "",
          "type": "string"
        },
        "^(passwordKeyName)$": {
          "default": "",
          "description": "the key name for the password.",
          "type": "string"
        },
        "^(secretName)$": {
          "default": "",
          "description": "the secret name in the engine to store the password.",
          "type": "string"
        },
        "^(secretType)$": {
          "anyOf": [
            {
              "pattern": "^SECRET_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "VAULT_KV_V2",
                "AWS_SECRETS_MANAGER",
                "GCP_SECRET_MANAGER"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Secret Type"
        },
        "^(skipVaultTlsVerification)$": {
          "default": false,
          "description": "TLS configuration for connecting to Vault server.\n These fields are separate from the database TLS configuration in DataSource.\n skip_vault_tls_verification disables TLS certificate verification for Vault connections.\n Default is false (verification enabled) for security.\n Only set to true for development or when certificates cannot be properly validated.",
          "type": "boolean"
        },
        "^(vaultSslCa)$": {
          "default": "",
          "description": "CA certificate for Vault server verification.",
          "type": "string"
        },
        "^(vaultSslCert)$": {
          "default": "",
          "description": "Client certificate for mutual TLS authentication with Vault.",
          "type": "string"
        },
        "^(vaultSslKey)$": {
          "default": "",
          "description": "Client private key for mutual TLS authentication with Vault.",
          "type": "string"
        }
      },
      "properties": {
        "app_role": {
          "$ref": "#/$defs/bytebase.store.DataSourceExternalSecret.AppRoleAuthOption.schema.json"
        },
        "auth_type": {
          "anyOf": [
            {
              "pattern": "^AUTH_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "TOKEN",
                "VAULT_APP_ROLE"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Auth Type"
        },
        "engine_name": {
          "default": "",
          "description": "engine name is the name for secret engine.",
          "type": "string"
        },
        "obfuscated_vault_ssl_ca": {
          "default": "",
          "type": "string"
        },
        "obfuscated_vault_ssl_cert": {
          "default": "",
          "type": "string"
        },
        "obfuscated_vault_ssl_key": {
          "default": "",
          "type": "string"
        },
        "password_key_name": {
          "default": "",
          "description": "the key name for the password.",
          "type": "string"
        },
        "secret_name": {
          "default": "",
          "description": "the secret name in the engine to store the password.",
          "type": "string"
        },
        "secret_type": {
          "anyOf": [
            {
              "pattern": "^SECRET_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "VAULT_KV_V2",
                "AWS_SECRETS_MANAGER",
                "GCP_SECRET_MANAGER"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Secret Type"
        },
        "skip_vault_tls_verification": {
          "default": false,
          "description": "TLS configuration for connecting to Vault server.\n These fields are separate from the database TLS configuration in DataSource.\n skip_vault_tls_verification disables TLS certificate verification for Vault connections.\n Default is false (verification enabled) for security.\n Only set to true for development or when certificates cannot be properly validated.",
          "type": "boolean"
        },
        "token": {
          "type": "string"
        },
        "url": {
          "default": "",
          "type": "string"
        },
        "vault_ssl_ca": {
          "default": "",
          "description": "CA certificate for Vault server verification.",
          "type": "string"
        },
        "vault_ssl_cert": {
          "default": "",
          "description": "Client certificate for mutual TLS authentication with Vault.",
          "type": "string"
        },
        "vault_ssl_key": {
          "default": "",
          "description": "Client private key for mutual TLS authentication with Vault.",
          "type": "string"
        }
      },
      "title": "Data Source External Secret",
      "type": "object"
    }
  },
  "$id": "bytebase.store.DataSourceExternalSecret.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.DataSourceExternalSecret.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
