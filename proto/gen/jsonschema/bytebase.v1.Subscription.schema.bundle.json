{
  "$defs": {
    "bytebase.v1.Subscription.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(activeInstances)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(expiresTime)$": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json"
        },
        "^(orgName)$": {
          "default": "",
          "type": "string"
        }
      },
      "properties": {
        "active_instances": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "expires_time": {
          "$ref": "#/$defs/google.protobuf.Timestamp.schema.json"
        },
        "instances": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "org_name": {
          "default": "",
          "type": "string"
        },
        "plan": {
          "anyOf": [
            {
              "pattern": "^PLAN_TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "FREE",
                "TEAM",
                "ENTERPRISE"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Plan Type"
        },
        "seats": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "trialing": {
          "default": false,
          "type": "boolean"
        }
      },
      "title": "Subscription",
      "type": "object"
    },
    "google.protobuf.Timestamp.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "format": "date-time",
      "title": "Timestamp",
      "type": "string"
    }
  },
  "$id": "bytebase.v1.Subscription.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.Subscription.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
