{
  "$id": "bytebase.v1.InstanceResource.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(dataSources)$": {
      "description": "Data source configurations for the instance.",
      "items": {
        "$ref": "bytebase.v1.DataSource.schema.json"
      },
      "type": "array"
    },
    "^(engineVersion)$": {
      "default": "",
      "description": "The version of the database engine.",
      "type": "string"
    }
  },
  "properties": {
    "activation": {
      "default": false,
      "description": "Whether the instance is activated.",
      "type": "boolean"
    },
    "data_sources": {
      "description": "Data source configurations for the instance.",
      "items": {
        "$ref": "bytebase.v1.DataSource.schema.json"
      },
      "type": "array"
    },
    "engine": {
      "anyOf": [
        {
          "pattern": "^ENGINE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "CLICKHOUSE",
            "MYSQL",
            "POSTGRES",
            "SNOWFLAKE",
            "SQLITE",
            "TIDB",
            "MONGODB",
            "REDIS",
            "ORACLE",
            "SPANNER",
            "MSSQL",
            "REDSHIFT",
            "MARIADB",
            "OCEANBASE",
            "STARROCKS",
            "DORIS",
            "HIVE",
            "ELASTICSEARCH",
            "BIGQUERY",
            "DYNAMODB",
            "DATABRICKS",
            "COCKROACHDB",
            "COSMOSDB",
            "TRINO",
            "CASSANDRA"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "description": "The database engine type.",
      "title": "Engine"
    },
    "engine_version": {
      "default": "",
      "description": "The version of the database engine.",
      "type": "string"
    },
    "environment": {
      "description": "The environment resource.\n Format: environments/prod where prod is the environment resource ID.",
      "type": "string"
    },
    "name": {
      "default": "",
      "description": "The name of the instance.\n Format: instances/{instance}",
      "type": "string"
    },
    "title": {
      "default": "",
      "description": "The display title of the instance.",
      "type": "string"
    }
  },
  "title": "Instance Resource",
  "type": "object"
}
