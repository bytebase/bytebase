{
  "$defs": {
    "bytebase.store.PlanCheckRunConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "databaseName": {
          "type": "string"
        },
        "enableGhost": {
          "description": "Whether to use gh-ost for online schema migration.",
          "type": "boolean"
        },
        "enablePriorBackup": {
          "description": "If set, a backup of the modified data will be created automatically before any changes are applied.",
          "type": "boolean"
        },
        "enableSdl": {
          "description": "Whether this is a Schema Definition Language (SDL) change.",
          "type": "boolean"
        },
        "ghostFlags": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "instanceId": {
          "type": "string"
        },
        "sheetUid": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      },
      "required": [
        "sheetUid",
        "instanceId",
        "databaseName",
        "enablePriorBackup",
        "enableGhost",
        "enableSdl"
      ],
      "title": "Plan Check Run Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.PlanCheckRunConfig.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.PlanCheckRunConfig.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
