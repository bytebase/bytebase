{
  "$defs": {
    "bytebase.v1.QueryOption.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "mssqlExplainFormat": {
          "enum": [
            "MSSQL_EXPLAIN_FORMAT_UNSPECIFIED",
            "MSSQL_EXPLAIN_FORMAT_ALL",
            "MSSQL_EXPLAIN_FORMAT_XML"
          ],
          "title": "MSSQL Explain Format",
          "type": "string"
        },
        "redisRunCommandsOn": {
          "enum": [
            "REDIS_RUN_COMMANDS_ON_UNSPECIFIED",
            "SINGLE_NODE",
            "ALL_NODES"
          ],
          "title": "Redis Run Commands On",
          "type": "string"
        }
      },
      "required": [
        "redisRunCommandsOn",
        "mssqlExplainFormat"
      ],
      "title": "Query Option",
      "type": "object"
    },
    "bytebase.v1.QueryRequest.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "container": {
          "description": "Container is the container name to execute the query against, used for\n CosmosDB only.",
          "type": "string"
        },
        "dataSourceId": {
          "description": "The id of data source.\n It is used for querying admin data source even if the instance has\n read-only data sources. Or it can be used to query a specific read-only\n data source.",
          "type": "string"
        },
        "explain": {
          "description": "Explain the statement.",
          "type": "boolean"
        },
        "limit": {
          "description": "The maximum number of rows to return.",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "name": {
          "description": "The name is the instance name to execute the query against.\n Format: instances/{instance}/databases/{databaseName}",
          "type": "string"
        },
        "queryOption": {
          "$ref": "#/$defs/bytebase.v1.QueryOption.jsonschema.strict.json"
        },
        "schema": {
          "description": "The default schema to search objects. Equals to the current schema in\n Oracle and search path in Postgres.",
          "type": "string"
        },
        "statement": {
          "description": "The SQL statement to execute.",
          "type": "string"
        }
      },
      "required": [
        "name",
        "statement",
        "limit",
        "dataSourceId",
        "explain"
      ],
      "title": "Query Request",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.QueryRequest.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.QueryRequest.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
