{
  "$defs": {
    "bytebase.v1.GroupMember.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "A member of a group with a role.",
      "properties": {
        "member": {
          "description": "Format: users/hello@world.com",
          "title": "Member is the principal who belong to this group.",
          "type": "string"
        },
        "role": {
          "description": "The member's role in the group.",
          "enum": [
            "ROLE_UNSPECIFIED",
            "OWNER",
            "MEMBER"
          ],
          "title": "Role",
          "type": "string"
        }
      },
      "required": [
        "member",
        "role"
      ],
      "title": "Group Member",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.GroupMember.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.GroupMember.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
