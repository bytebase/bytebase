{
  "$id": "bytebase.v1.IssueComment.TaskPriorBackup.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Task prior backup event information.",
  "properties": {
    "database": {
      "description": "The database that was backed up.",
      "type": "string"
    },
    "error": {
      "description": "Error message if backup failed.",
      "type": "string"
    },
    "originalLine": {
      "description": "The original line number in the statement.",
      "maximum": 2147483647,
      "minimum": -2147483648,
      "type": "integer"
    },
    "tables": {
      "description": "The tables that were backed up.",
      "items": {
        "$ref": "bytebase.v1.IssueComment.TaskPriorBackup.Table.jsonschema.strict.json"
      },
      "type": "array"
    },
    "task": {
      "description": "The task for which backup was performed.",
      "type": "string"
    }
  },
  "required": [
    "task",
    "database",
    "error"
  ],
  "title": "Task Prior Backup",
  "type": "object"
}
