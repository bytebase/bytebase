{
  "$defs": {
    "bytebase.v1.BoundingBox.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the spatial bounds for GEOMETRY spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum X coordinate"
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum X coordinate"
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum Y coordinate"
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum Y coordinate"
        }
      },
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.v1.DimensionConstraint.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionConstraint defines constraints for a spatial dimension.",
      "patternProperties": {
        "^(maxValue)$": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum value for this dimension"
        },
        "^(minValue)$": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum value for this dimension"
        }
      },
      "properties": {
        "dimension": {
          "default": "",
          "description": "Dimension name/type (X, Y, Z, M, etc.)",
          "type": "string"
        },
        "max_value": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Maximum value for this dimension"
        },
        "min_value": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Minimum value for this dimension"
        },
        "tolerance": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Tolerance for this dimension"
        }
      },
      "title": "Dimension Constraint",
      "type": "object"
    },
    "bytebase.v1.DimensionalConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionalConfig defines dimensional and constraint parameters for spatial indexes.",
      "patternProperties": {
        "^(dataType)$": {
          "default": "",
          "description": "Spatial data type (GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.)",
          "type": "string"
        }
      },
      "properties": {
        "constraints": {
          "description": "Coordinate system constraints",
          "items": {
            "$ref": "#/$defs/bytebase.v1.DimensionConstraint.schema.json"
          },
          "type": "array"
        },
        "data_type": {
          "default": "",
          "description": "Spatial data type (GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.)",
          "type": "string"
        },
        "dimensions": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of dimensions (2-4, default 2)"
        },
        "srid": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Spatial reference system identifier (SRID)"
        }
      },
      "title": "Dimensional Config",
      "type": "object"
    },
    "bytebase.v1.GridLevel.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a tessellation grid level with its density.",
      "properties": {
        "density": {
          "default": "",
          "description": "Grid density (LOW, MEDIUM, HIGH)",
          "type": "string"
        },
        "level": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Grid level number (1-4 for SQL Server)"
        }
      },
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.v1.SpatialIndexConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "SpatialIndexConfig defines the spatial index configuration for spatial databases.",
      "properties": {
        "dimensional": {
          "$ref": "#/$defs/bytebase.v1.DimensionalConfig.schema.json",
          "description": "Dimensional configuration"
        },
        "method": {
          "default": "",
          "description": "Spatial indexing method (e.g., \"SPATIAL\", \"R-TREE\", \"GIST\")",
          "type": "string"
        },
        "storage": {
          "$ref": "#/$defs/bytebase.v1.StorageConfig.schema.json",
          "description": "Storage and performance configuration"
        },
        "tessellation": {
          "$ref": "#/$defs/bytebase.v1.TessellationConfig.schema.json",
          "description": "Tessellation configuration for grid-based spatial indexes"
        }
      },
      "title": "Spatial Index Config",
      "type": "object"
    },
    "bytebase.v1.StorageConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "StorageConfig defines storage and performance parameters for spatial indexes.",
      "patternProperties": {
        "^(allowPageLocks)$": {
          "default": false,
          "type": "boolean"
        },
        "^(allowRowLocks)$": {
          "default": false,
          "type": "boolean"
        },
        "^(commitInterval)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(dataCompression)$": {
          "default": "",
          "type": "string"
        },
        "^(dropExisting)$": {
          "default": false,
          "type": "boolean"
        },
        "^(padIndex)$": {
          "default": false,
          "description": "SQL Server specific parameters",
          "type": "boolean"
        },
        "^(sdoLevel)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(sortInTempdb)$": {
          "default": "",
          "type": "string"
        },
        "^(workTablespace)$": {
          "default": "",
          "type": "string"
        }
      },
      "properties": {
        "allow_page_locks": {
          "default": false,
          "type": "boolean"
        },
        "allow_row_locks": {
          "default": false,
          "type": "boolean"
        },
        "buffering": {
          "default": "",
          "description": "Buffering mode for PostgreSQL (auto, on, off)",
          "type": "string"
        },
        "commit_interval": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "data_compression": {
          "default": "",
          "type": "string"
        },
        "drop_existing": {
          "default": false,
          "type": "boolean"
        },
        "fillfactor": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Fill factor percentage (1-100)"
        },
        "maxdop": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "online": {
          "default": false,
          "type": "boolean"
        },
        "pad_index": {
          "default": false,
          "description": "SQL Server specific parameters",
          "type": "boolean"
        },
        "sdo_level": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "sort_in_tempdb": {
          "default": "",
          "type": "string"
        },
        "tablespace": {
          "default": "",
          "description": "Tablespace configuration for Oracle",
          "type": "string"
        },
        "work_tablespace": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Storage Config",
      "type": "object"
    },
    "bytebase.v1.TessellationConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "patternProperties": {
        "^(boundingBox)$": {
          "$ref": "#/$defs/bytebase.v1.BoundingBox.schema.json",
          "description": "Bounding box for GEOMETRY tessellation (not used for GEOGRAPHY)"
        },
        "^(cellsPerObject)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of cells per object (1-8192 for SQL Server)"
        },
        "^(gridLevels)$": {
          "description": "Grid levels and densities for multi-level tessellation",
          "items": {
            "$ref": "#/$defs/bytebase.v1.GridLevel.schema.json"
          },
          "type": "array"
        }
      },
      "properties": {
        "bounding_box": {
          "$ref": "#/$defs/bytebase.v1.BoundingBox.schema.json",
          "description": "Bounding box for GEOMETRY tessellation (not used for GEOGRAPHY)"
        },
        "cells_per_object": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of cells per object (1-8192 for SQL Server)"
        },
        "grid_levels": {
          "description": "Grid levels and densities for multi-level tessellation",
          "items": {
            "$ref": "#/$defs/bytebase.v1.GridLevel.schema.json"
          },
          "type": "array"
        },
        "scheme": {
          "default": "",
          "description": "Tessellation scheme (e.g., \"GEOMETRY_GRID\", \"GEOGRAPHY_GRID\", \"GEOMETRY_AUTO_GRID\")",
          "type": "string"
        }
      },
      "title": "Tessellation Config",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.SpatialIndexConfig.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.SpatialIndexConfig.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
