{
  "$id": "bytebase.v1.GetSchemaStringRequest.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "metadata": {
      "$ref": "bytebase.v1.DatabaseMetadata.schema.json",
      "description": "If use the metadata to generate the schema string, the type is OBJECT_TYPE_UNSPECIFIED.\n Also the schema and object are empty."
    },
    "name": {
      "default": "",
      "description": "The name of the database.\n Format: instances/{instance}/databases/{database}",
      "type": "string"
    },
    "object": {
      "default": "",
      "description": "It's empty for DATABASE and SCHEMA.",
      "type": "string"
    },
    "schema": {
      "default": "",
      "description": "It's empty for DATABASE.",
      "type": "string"
    },
    "type": {
      "anyOf": [
        {
          "pattern": "^OBJECT_TYPE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "DATABASE",
            "SCHEMA",
            "TABLE",
            "VIEW",
            "MATERIALIZED_VIEW",
            "FUNCTION",
            "PROCEDURE",
            "SEQUENCE"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Object Type"
    }
  },
  "title": "Get Schema String Request",
  "type": "object"
}
