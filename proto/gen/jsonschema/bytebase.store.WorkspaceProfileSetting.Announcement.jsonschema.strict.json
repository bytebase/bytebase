{
  "$id": "bytebase.store.WorkspaceProfileSetting.Announcement.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "level": {
      "description": "The alert level of the announcement.",
      "enum": [
        "ALERT_LEVEL_UNSPECIFIED",
        "ALERT_LEVEL_INFO",
        "ALERT_LEVEL_WARNING",
        "ALERT_LEVEL_CRITICAL"
      ],
      "title": "Alert Level",
      "type": "string"
    },
    "link": {
      "description": "The optional link, user can follow the link to check extra details",
      "type": "string"
    },
    "text": {
      "description": "The text of the announcement.",
      "type": "string"
    }
  },
  "required": [
    "level",
    "text",
    "link"
  ],
  "title": "Announcement",
  "type": "object"
}
