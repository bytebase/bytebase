{
  "$defs": {
    "bytebase.v1.DimensionConstraint.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionConstraint defines constraints for a spatial dimension.",
      "properties": {
        "dimension": {
          "description": "Dimension name/type (X, Y, Z, M, etc.)",
          "type": "string"
        },
        "max_value": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ],
          "description": "Maximum value for this dimension"
        },
        "min_value": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ],
          "description": "Minimum value for this dimension"
        },
        "tolerance": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ],
          "description": "Tolerance for this dimension"
        }
      },
      "required": [
        "dimension",
        "min_value",
        "max_value",
        "tolerance"
      ],
      "title": "Dimension Constraint",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.DimensionConstraint.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.DimensionConstraint.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
