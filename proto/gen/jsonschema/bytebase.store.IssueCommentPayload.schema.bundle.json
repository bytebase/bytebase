      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(issueUpdate)$": {
          "$ref": "#/$defs/bytebase.store.IssueCommentPayload.IssueUpdate.schema.json"
        },
        "^(stageEnd)$": {
          "$ref": "#/$defs/bytebase.store.IssueCommentPayload.StageEnd.schema.json"
        },
        "^(taskPriorBackup)$": {
