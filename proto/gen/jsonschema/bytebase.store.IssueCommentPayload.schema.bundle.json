        },
        "task_prior_backup": {
          "$ref": "#/$defs/bytebase.store.IssueCommentPayload.TaskPriorBackup.schema.json"
        },
        "task_update": {
          "$ref": "#/$defs/bytebase.store.IssueCommentPayload.TaskUpdate.schema.json"
        }
