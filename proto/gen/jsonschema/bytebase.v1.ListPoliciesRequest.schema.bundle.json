      "patternProperties": {
        "^(policyType)$": {
          "anyOf": [
            {
              "enum": [
                "POLICY_TYPE_UNSPECIFIED",
                "ROLLOUT_POLICY",
                "MASKING_RULE",
                "MASKING_EXCEPTION",
                "TAG",
                "DATA_SOURCE_QUERY",
                "DATA_QUERY"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
