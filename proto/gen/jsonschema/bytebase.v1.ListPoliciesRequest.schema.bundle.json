            {
              "enum": [
                "POLICY_TYPE_UNSPECIFIED",
                "ROLLOUT_POLICY",
                "MASKING_RULE",
                "MASKING_EXCEPTION",
                "TAG",
                "DATA_SOURCE_QUERY",
