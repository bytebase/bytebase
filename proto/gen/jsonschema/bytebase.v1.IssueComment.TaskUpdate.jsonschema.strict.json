{
  "$id": "bytebase.v1.IssueComment.TaskUpdate.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Task update event information.",
  "properties": {
    "fromSheet": {
      "description": "Format: projects/{project}/sheets/{sheet}",
      "type": "string"
    },
    "tasks": {
      "description": "The tasks that were updated.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "toSheet": {
      "description": "Format: projects/{project}/sheets/{sheet}",
      "type": "string"
    },
    "toStatus": {
      "enum": [
        "STATUS_UNSPECIFIED",
        "PENDING",
        "RUNNING",
        "DONE",
        "FAILED",
        "SKIPPED",
        "CANCELED"
      ],
      "title": "Status",
      "type": "string"
    }
  },
  "title": "Task Update",
  "type": "object"
}
