{
  "$defs": {
    "bytebase.store.ChangedResourceDatabase.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "name": {
          "default": "",
          "type": "string"
        },
        "schemas": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceSchema.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Changed Resource Database",
      "type": "object"
    },
    "bytebase.store.ChangedResourceFunction.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "name": {
          "default": "",
          "type": "string"
        },
        "ranges": {
          "description": "The ranges of substrings correspond to the statements on the sheet.",
          "items": {
            "$ref": "#/$defs/bytebase.store.Range.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Changed Resource Function",
      "type": "object"
    },
    "bytebase.store.ChangedResourceProcedure.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "name": {
          "default": "",
          "type": "string"
        },
        "ranges": {
          "description": "The ranges of substrings correspond to the statements on the sheet.",
          "items": {
            "$ref": "#/$defs/bytebase.store.Range.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Changed Resource Procedure",
      "type": "object"
    },
    "bytebase.store.ChangedResourceSchema.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "functions": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceFunction.schema.json"
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "type": "string"
        },
        "procedures": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceProcedure.schema.json"
          },
          "type": "array"
        },
        "tables": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceTable.schema.json"
          },
          "type": "array"
        },
        "views": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceView.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Changed Resource Schema",
      "type": "object"
    },
    "bytebase.store.ChangedResourceTable.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(tableRows)$": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "The estimated row count of the table."
        }
      },
      "properties": {
        "name": {
          "default": "",
          "type": "string"
        },
        "ranges": {
          "description": "The ranges of substrings correspond to the statements on the sheet.",
          "items": {
            "$ref": "#/$defs/bytebase.store.Range.schema.json"
          },
          "type": "array"
        },
        "table_rows": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "The estimated row count of the table."
        }
      },
      "title": "Changed Resource Table",
      "type": "object"
    },
    "bytebase.store.ChangedResourceView.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "name": {
          "default": "",
          "type": "string"
        },
        "ranges": {
          "description": "The ranges of substrings correspond to the statements on the sheet.",
          "items": {
            "$ref": "#/$defs/bytebase.store.Range.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Changed Resource View",
      "type": "object"
    },
    "bytebase.store.ChangedResources.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "databases": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceDatabase.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Changed Resources",
      "type": "object"
    },
    "bytebase.store.ChangelogPayload.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(changedResources)$": {
          "$ref": "#/$defs/bytebase.store.ChangedResources.schema.json"
        },
        "^(dumpVersion)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Dump format version for drift detection reliability.\n Engine-specific version stored when baseline/migration is created.\n 0 = legacy changelog (pre-versioning)."
        },
        "^(gitCommit)$": {
          "default": "",
          "type": "string"
        },
        "^(taskRun)$": {
          "default": "",
          "description": "Format: projects/{project}/rollouts/{rollout}/stages/{stage}/tasks/{task}/taskruns/{taskrun}",
          "type": "string"
        }
      },
      "properties": {
        "changed_resources": {
          "$ref": "#/$defs/bytebase.store.ChangedResources.schema.json"
        },
        "dump_version": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Dump format version for drift detection reliability.\n Engine-specific version stored when baseline/migration is created.\n 0 = legacy changelog (pre-versioning)."
        },
        "git_commit": {
          "default": "",
          "type": "string"
        },
        "issue": {
          "default": "",
          "description": "Format: projects/{project}/issues/{issue}",
          "type": "string"
        },
        "revision": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "The revision uid.\n optional"
        },
        "sheet": {
          "default": "",
          "description": "The sheet that holds the content.\n Format: projects/{project}/sheets/{sheet}",
          "type": "string"
        },
        "task_run": {
          "default": "",
          "description": "Format: projects/{project}/rollouts/{rollout}/stages/{stage}/tasks/{task}/taskruns/{taskrun}",
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "BASELINE",
                "MIGRATE",
                "SDL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Type"
        },
        "version": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Changelog Payload",
      "type": "object"
    },
    "bytebase.store.Range.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Range represents a span within a text or sequence.",
      "properties": {
        "end": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "End index (exclusive)."
        },
        "start": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Start index (inclusive)."
        }
      },
      "title": "Range",
      "type": "object"
    }
  },
  "$id": "bytebase.store.ChangelogPayload.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.ChangelogPayload.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
