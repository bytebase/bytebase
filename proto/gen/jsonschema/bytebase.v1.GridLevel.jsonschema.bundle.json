{
  "$defs": {
    "bytebase.v1.GridLevel.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a tessellation grid level with its density.",
      "properties": {
        "density": {
          "default": "",
          "description": "Grid density (LOW, MEDIUM, HIGH)",
          "type": "string"
        },
        "level": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Grid level number (1-4 for SQL Server)"
        }
      },
      "title": "Grid Level",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.GridLevel.jsonschema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.GridLevel.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
