{
  "$id": "bytebase.store.GenerationMetadata.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "expression": {
      "default": "",
      "type": "string"
    },
    "type": {
      "anyOf": [
        {
          "pattern": "^TYPE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "TYPE_VIRTUAL",
            "TYPE_STORED"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Type"
    }
  },
  "title": "Generation Metadata",
  "type": "object"
}
