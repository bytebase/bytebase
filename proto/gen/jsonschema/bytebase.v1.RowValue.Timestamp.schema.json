{
  "$id": "bytebase.v1.RowValue.Timestamp.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(googleTimestamp)$": {
      "$ref": "google.protobuf.Timestamp.schema.json"
    }
  },
  "properties": {
    "accuracy": {
      "anyOf": [
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "The accuracy is the number of digits after the decimal point."
    },
    "google_timestamp": {
      "$ref": "google.protobuf.Timestamp.schema.json"
    }
  },
  "title": "Timestamp",
  "type": "object"
}
