{
  "$id": "bytebase.store.MaskingExceptionPolicy.MaskingException.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "action": {
      "anyOf": [
        {
          "pattern": "^ACTION_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "QUERY",
            "EXPORT"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "description": "action is the action by which the user can access sensitive data.",
      "title": "Action"
    },
    "condition": {
      "$ref": "google.type.Expr.schema.json",
      "description": "The condition that is associated with this exception policy instance."
    },
    "member": {
      "default": "",
      "description": "Format: users/{userUID} or groups/{group email}",
      "title": "Member is the principal who binds to this exception policy instance.",
      "type": "string"
    }
  },
  "title": "Masking Exception",
  "type": "object"
}
