{
  "$id": "bytebase.store.ChangedResourceTable.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(tableRows)$": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "The estimated row count of the table."
    }
  },
  "properties": {
    "name": {
      "default": "",
      "type": "string"
    },
    "ranges": {
      "description": "The ranges of substrings correspond to the statements on the sheet.",
      "items": {
        "$ref": "bytebase.store.Range.schema.json"
      },
      "type": "array"
    },
    "table_rows": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0,
      "description": "The estimated row count of the table."
    }
  },
  "title": "Changed Resource Table",
  "type": "object"
}
