{
  "$id": "bytebase.v1.DiffMetadataRequest.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(sourceCatalog)$": {
      "$ref": "bytebase.v1.DatabaseCatalog.schema.json"
    },
    "^(sourceMetadata)$": {
      "$ref": "bytebase.v1.DatabaseMetadata.schema.json",
      "description": "The metadata of the source schema."
    },
    "^(targetCatalog)$": {
      "$ref": "bytebase.v1.DatabaseCatalog.schema.json"
    },
    "^(targetMetadata)$": {
      "$ref": "bytebase.v1.DatabaseMetadata.schema.json",
      "description": "The metadata of the target schema."
    }
  },
  "properties": {
    "engine": {
      "anyOf": [
        {
          "pattern": "^ENGINE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "CLICKHOUSE",
            "MYSQL",
            "POSTGRES",
            "SNOWFLAKE",
            "SQLITE",
            "TIDB",
            "MONGODB",
            "REDIS",
            "ORACLE",
            "SPANNER",
            "MSSQL",
            "REDSHIFT",
            "MARIADB",
            "OCEANBASE",
            "STARROCKS",
            "DORIS",
            "HIVE",
            "ELASTICSEARCH",
            "BIGQUERY",
            "DYNAMODB",
            "DATABRICKS",
            "COCKROACHDB",
            "COSMOSDB",
            "TRINO",
            "CASSANDRA"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "description": "The database engine of the schema.",
      "title": "Engine"
    },
    "source_catalog": {
      "$ref": "bytebase.v1.DatabaseCatalog.schema.json"
    },
    "source_metadata": {
      "$ref": "bytebase.v1.DatabaseMetadata.schema.json",
      "description": "The metadata of the source schema."
    },
    "target_catalog": {
      "$ref": "bytebase.v1.DatabaseCatalog.schema.json"
    },
    "target_metadata": {
      "$ref": "bytebase.v1.DatabaseMetadata.schema.json",
      "description": "The metadata of the target schema."
    }
  },
  "title": "Diff Metadata Request",
  "type": "object"
}
