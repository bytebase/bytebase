{
  "$defs": {
    "bytebase.v1.ColumnCatalog.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Column metadata within a table.",
      "patternProperties": {
        "^(objectSchema)$": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.schema.json",
          "description": "Object schema for complex column types like JSON."
        },
        "^(semanticType)$": {
          "default": "",
          "description": "The semantic type describing the data purpose.",
          "type": "string"
        }
      },
      "properties": {
        "classification": {
          "default": "",
          "description": "The data classification level for this column.",
          "type": "string"
        },
        "labels": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "User-defined labels for this column.",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "name": {
          "default": "",
          "description": "The column name.",
          "type": "string"
        },
        "object_schema": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.schema.json",
          "description": "Object schema for complex column types like JSON."
        },
        "semantic_type": {
          "default": "",
          "description": "The semantic type describing the data purpose.",
          "type": "string"
        }
      },
      "title": "Column Catalog",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.ArrayKind.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Array type with element schema.",
      "properties": {
        "kind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.schema.json",
          "description": "The schema of array elements."
        }
      },
      "title": "Array Kind",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.StructKind.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Structure type with named properties.",
      "properties": {
        "properties": {
          "additionalProperties": {
            "$ref": "#/$defs/bytebase.v1.ObjectSchema.schema.json"
          },
          "description": "Properties of the struct.",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "title": "Struct Kind",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Schema definition for object-type columns.",
      "patternProperties": {
        "^(arrayKind)$": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.ArrayKind.schema.json",
          "description": "Array schema."
        },
        "^(semanticType)$": {
          "default": "",
          "description": "The semantic type of this object.",
          "type": "string"
        },
        "^(structKind)$": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.StructKind.schema.json",
          "description": "Struct schema."
        }
      },
      "properties": {
        "array_kind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.ArrayKind.schema.json",
          "description": "Array schema."
        },
        "semantic_type": {
          "default": "",
          "description": "The semantic type of this object.",
          "type": "string"
        },
        "struct_kind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.StructKind.schema.json",
          "description": "Struct schema."
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "STRING",
                "NUMBER",
                "BOOLEAN",
                "OBJECT",
                "ARRAY"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "The data type of this object.",
          "title": "Type"
        }
      },
      "title": "Object Schema",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.ColumnCatalog.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.ColumnCatalog.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
