{
  "$defs": {
    "bytebase.store.BoundingBox.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the bounding box for spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        }
      },
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.store.DimensionalConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionalConfig defines dimensional and constraint parameters for spatial indexes.",
      "patternProperties": {
        "^(data_type)$": {
          "default": "",
          "description": "Spatial data type\n Examples: GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.",
          "type": "string"
        },
        "^(layer_gtype)$": {
          "default": "",
          "description": "Oracle geometry type constraint\n Examples: POINT, LINE, POLYGON, COLLECTION",
          "type": "string"
        },
        "^(operator_class)$": {
          "default": "",
          "description": "PostgreSQL operator class\n Examples: gist_geometry_ops_2d, gist_geometry_ops_nd, etc.",
          "type": "string"
        },
        "^(parallel_build)$": {
          "default": false,
          "description": "Parallel index creation",
          "type": "boolean"
        }
      },
      "properties": {
        "dataType": {
          "default": "",
          "description": "Spatial data type\n Examples: GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.",
          "type": "string"
        },
        "dimensions": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Number of dimensions (2-4, default 2)"
        },
        "layerGtype": {
          "default": "",
          "description": "Oracle geometry type constraint\n Examples: POINT, LINE, POLYGON, COLLECTION",
          "type": "string"
        },
        "operatorClass": {
          "default": "",
          "description": "PostgreSQL operator class\n Examples: gist_geometry_ops_2d, gist_geometry_ops_nd, etc.",
          "type": "string"
        },
        "parallelBuild": {
          "default": false,
          "description": "Parallel index creation",
          "type": "boolean"
        }
      },
      "title": "Dimensional Config",
      "type": "object"
    },
    "bytebase.store.GridLevel.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a grid level for spatial tessellation.",
      "properties": {
        "density": {
          "default": "",
          "type": "string"
        },
        "level": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        }
      },
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.store.SpatialIndexConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "SpatialIndexConfig is the configuration for spatial indexes across different database engines.",
      "patternProperties": {
        "^(engine_specific)$": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "Database-specific parameters (stored as key-value pairs for extensibility)",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "properties": {
        "dimensional": {
          "$ref": "#/$defs/bytebase.store.DimensionalConfig.jsonschema.json",
          "description": "Dimensional and constraint parameters"
        },
        "engineSpecific": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "Database-specific parameters (stored as key-value pairs for extensibility)",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "method": {
          "default": "",
          "description": "Index method/type (database-specific)\n Examples: \"SPATIAL\" (MySQL/SQL Server), \"GIST\"/\"SPGIST\" (PostgreSQL), \"MDSYS.SPATIAL_INDEX_V2\" (Oracle)",
          "type": "string"
        },
        "storage": {
          "$ref": "#/$defs/bytebase.store.StorageConfig.jsonschema.json",
          "description": "Storage and performance parameters"
        },
        "tessellation": {
          "$ref": "#/$defs/bytebase.store.TessellationConfig.jsonschema.json",
          "description": "Tessellation configuration (primarily SQL Server)"
        }
      },
      "title": "Spatial Index Config",
      "type": "object"
    },
    "bytebase.store.StorageConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "StorageConfig defines storage and performance parameters for spatial indexes.",
      "patternProperties": {
        "^(allow_page_locks)$": {
          "default": false,
          "type": "boolean"
        },
        "^(allow_row_locks)$": {
          "default": false,
          "type": "boolean"
        },
        "^(commit_interval)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(data_compression)$": {
          "default": "",
          "type": "string"
        },
        "^(drop_existing)$": {
          "default": false,
          "type": "boolean"
        },
        "^(pad_index)$": {
          "default": false,
          "description": "SQL Server parameters",
          "type": "boolean"
        },
        "^(sdo_level)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(sort_in_tempdb)$": {
          "default": "",
          "type": "string"
        },
        "^(work_tablespace)$": {
          "default": "",
          "type": "string"
        }
      },
      "properties": {
        "allowPageLocks": {
          "default": false,
          "type": "boolean"
        },
        "allowRowLocks": {
          "default": false,
          "type": "boolean"
        },
        "buffering": {
          "default": "",
          "type": "string"
        },
        "commitInterval": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "dataCompression": {
          "default": "",
          "type": "string"
        },
        "dropExisting": {
          "default": false,
          "type": "boolean"
        },
        "fillfactor": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "PostgreSQL parameters"
        },
        "maxdop": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "online": {
          "default": false,
          "type": "boolean"
        },
        "padIndex": {
          "default": false,
          "description": "SQL Server parameters",
          "type": "boolean"
        },
        "sdoLevel": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "sortInTempdb": {
          "default": "",
          "type": "string"
        },
        "tablespace": {
          "default": "",
          "description": "Oracle parameters",
          "type": "string"
        },
        "workTablespace": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Storage Config",
      "type": "object"
    },
    "bytebase.store.TessellationConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "patternProperties": {
        "^(bounding_box)$": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.jsonschema.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "^(cells_per_object)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Cells per object (SQL Server)"
        },
        "^(grid_levels)$": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.jsonschema.json"
          },
          "type": "array"
        }
      },
      "properties": {
        "boundingBox": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.jsonschema.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "cellsPerObject": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Cells per object (SQL Server)"
        },
        "gridLevels": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.jsonschema.json"
          },
          "type": "array"
        },
        "scheme": {
          "default": "",
          "description": "Tessellation scheme\n Examples: GEOMETRY_GRID, GEOGRAPHY_GRID, GEOMETRY_AUTO_GRID, GEOGRAPHY_AUTO_GRID",
          "type": "string"
        }
      },
      "title": "Tessellation Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.SpatialIndexConfig.jsonschema.bundle.json",
  "$ref": "#/$defs/bytebase.store.SpatialIndexConfig.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
