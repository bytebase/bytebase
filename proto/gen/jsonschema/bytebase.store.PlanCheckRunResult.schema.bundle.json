    },
    "bytebase.store.ChangedResources.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "databases": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceDatabase.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Changed Resources",
      "type": "object"
    },
    "bytebase.store.PlanCheckRunResult.Result.SqlReviewReport.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(endPosition)$": {
          "$ref": "#/$defs/bytebase.store.Position.schema.json"
        },
        "^(startPosition)$": {
          "$ref": "#/$defs/bytebase.store.Position.schema.json",
          "description": "Position of the SQL statement."
        }
      },
      "properties": {
        "end_position": {
          "$ref": "#/$defs/bytebase.store.Position.schema.json"
        },
        "start_position": {
          "$ref": "#/$defs/bytebase.store.Position.schema.json",
          "description": "Position of the SQL statement."
        }
      },
      "title": "Sql Review Report",
      "type": "object"
    },
    "bytebase.store.PlanCheckRunResult.Result.SqlSummaryReport.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(affectedRows)$": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "^(changedResources)$": {
          "$ref": "#/$defs/bytebase.store.ChangedResources.schema.json"
        },
        "^(statementTypes)$": {
          "description": "statement_types are the types of statements found in the SQL.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "properties": {
        "affected_rows": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "changed_resources": {
          "$ref": "#/$defs/bytebase.store.ChangedResources.schema.json"
        },
        "statement_types": {
          "description": "statement_types are the types of statements found in the SQL.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Sql Summary Report",
      "type": "object"
    },
    "bytebase.store.PlanCheckRunResult.Result.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(sqlReviewReport)$": {
          "$ref": "#/$defs/bytebase.store.PlanCheckRunResult.Result.SqlReviewReport.schema.json"
        },
        "^(sqlSummaryReport)$": {
          "$ref": "#/$defs/bytebase.store.PlanCheckRunResult.Result.SqlSummaryReport.schema.json"
        }
      },
      "properties": {
        "code": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        },
        "content": {
          "default": "",
          "type": "string"
        },
        "sql_review_report": {
          "$ref": "#/$defs/bytebase.store.PlanCheckRunResult.Result.SqlReviewReport.schema.json"
        },
        "sql_summary_report": {
          "$ref": "#/$defs/bytebase.store.PlanCheckRunResult.Result.SqlSummaryReport.schema.json"
        },
        "status": {
          "anyOf": [
            {
              "pattern": "^STATUS_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "SUCCESS",
                "WARNING",
                "ERROR"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Status"
        },
        "title": {
          "default": "",
          "type": "string"
        }
      },
      "title": "Result",
      "type": "object"
    },
    "bytebase.store.PlanCheckRunResult.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "error": {
          "default": "",
          "type": "string"
        },
        "results": {
          "items": {
            "$ref": "#/$defs/bytebase.store.PlanCheckRunResult.Result.schema.json"
          },
          "type": "array"
        }
      },
      "title": "Plan Check Run Result",
      "type": "object"
    },
    "bytebase.store.Position.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Handling unknown positions:\n - If the entire position is unknown, leave this field as nil/undefined\n - If only line is known, set line and leave column as 0 (e.g., line=5, column=0)\n - If only column is known (rare), set column and leave line as 0\n Frontends should check for nil/undefined/zero values and handle them appropriately.",
      "properties": {
        "column": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Examples:\n - \"SELECT * FROM t\" - column 8 is '*'\n - \"SELECT ä½ å¥½ FROM t\" - column 8 is 'ä½ ' (even though it's at byte offset 7)\n - \"SELECT ðŸ˜€ FROM t\" - column 8 is 'ðŸ˜€' (even though it's 4 bytes in UTF-8)",
          "title": "Column position in a text (one-based).\n Column is measured in Unicode code points (characters/runes), not bytes or grapheme clusters.\n First character of the line is column 1.\n A value of 0 indicates the column information is unknown."
        },
        "line": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Line position in a text (one-based).\n First line of the text is line 1.\n A value of 0 indicates the line information is unknown."
        }
      },
      "title": "Position in a text expressed as one-based line and one-based column.\n We use 1-based numbering to match the majority of industry standards:\n - Monaco Editor uses 1-based (https://microsoft.github.io/monaco-editor/typedoc/interfaces/IPosition.html)\n - GitHub Actions uses 1-based (https://docs.github.com/en/actions/using-workflows/workflow-commands-for-github-actions#setting-an-error-message)\n - Most text editors display 1-based positions to users\n Note: LSP uses 0-based (https://microsoft.github.io/language-server-protocol/specifications/lsp/3.17/specification/#position),\n but we follow the canonical user-facing standards.",
      "type": "object"
    },
    "bytebase.store.Range.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Range represents a span within a text or sequence.",
      "properties": {
        "end": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "End index (exclusive)."
        },
        "start": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Start index (inclusive)."
        }
      },
      "title": "Range",
      "type": "object"
    }
  },
  "$id": "bytebase.store.PlanCheckRunResult.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.PlanCheckRunResult.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
