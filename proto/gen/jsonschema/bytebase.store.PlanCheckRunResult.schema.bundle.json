    "bytebase.store.ChangedResourceSchema.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "functions": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceFunction.schema.json"
          },
          "type": "array"
        },
        "name": {
          "default": "",
          "type": "string"
        },
        "procedures": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceProcedure.schema.json"
          },
          "type": "array"
        },
        "tables": {
          "items": {
            "$ref": "#/$defs/bytebase.store.ChangedResourceTable.schema.json"
          },
