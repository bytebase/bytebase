{
  "$defs": {
    "bytebase.store.ProcedureMetadata.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "ProcedureMetadata is the metadata for procedures.",
      "properties": {
        "characterSetClient": {
          "description": "MySQL specific metadata.",
          "type": "string"
        },
        "collationConnection": {
          "type": "string"
        },
        "comment": {
          "type": "string"
        },
        "databaseCollation": {
          "type": "string"
        },
        "definition": {
          "description": "The definition is the definition of a procedure.",
          "type": "string"
        },
        "name": {
          "description": "The name of the procedure.",
          "type": "string"
        },
        "signature": {
          "description": "The signature is the name with the number and type of input arguments the\n function takes.",
          "type": "string"
        },
        "skipDump": {
          "type": "boolean"
        },
        "sqlMode": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "definition",
        "signature",
        "characterSetClient",
        "collationConnection",
        "databaseCollation",
        "sqlMode",
        "comment",
        "skipDump"
      ],
      "title": "Procedure Metadata",
      "type": "object"
    }
  },
  "$id": "bytebase.store.ProcedureMetadata.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.ProcedureMetadata.jsonschema.strict.json",
