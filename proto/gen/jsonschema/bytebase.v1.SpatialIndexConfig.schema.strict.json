{
  "$id": "bytebase.v1.SpatialIndexConfig.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "SpatialIndexConfig defines the spatial index configuration for spatial databases.",
  "properties": {
    "dimensional": {
      "$ref": "bytebase.v1.DimensionalConfig.schema.strict.json",
      "description": "Dimensional configuration"
    },
    "method": {
      "description": "Spatial indexing method (e.g., \"SPATIAL\", \"R-TREE\", \"GIST\")",
      "type": "string"
    },
    "storage": {
      "$ref": "bytebase.v1.StorageConfig.schema.strict.json",
      "description": "Storage and performance configuration"
    },
    "tessellation": {
      "$ref": "bytebase.v1.TessellationConfig.schema.strict.json",
      "description": "Tessellation configuration for grid-based spatial indexes"
    }
  },
  "required": [
    "method"
  ],
  "title": "Spatial Index Config",
  "type": "object"
}
