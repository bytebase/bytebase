{
  "$defs": {
    "bytebase.store.BoundingBox.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the bounding box for spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        }
      },
      "required": [
        "xmin",
        "ymin",
        "xmax",
        "ymax"
      ],
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.store.DimensionalConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionalConfig defines dimensional and constraint parameters for spatial indexes.",
      "properties": {
        "data_type": {
          "description": "Spatial data type\n Examples: GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.",
          "type": "string"
        },
        "dimensions": {
          "description": "Number of dimensions (2-4, default 2)",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "layer_gtype": {
          "description": "Oracle geometry type constraint\n Examples: POINT, LINE, POLYGON, COLLECTION",
          "type": "string"
        },
        "operator_class": {
          "description": "PostgreSQL operator class\n Examples: gist_geometry_ops_2d, gist_geometry_ops_nd, etc.",
          "type": "string"
        },
        "parallel_build": {
          "description": "Parallel index creation",
          "type": "boolean"
        }
      },
      "required": [
        "dimensions",
        "data_type",
        "operator_class",
        "layer_gtype",
        "parallel_build"
      ],
      "title": "Dimensional Config",
      "type": "object"
    },
    "bytebase.store.GridLevel.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a grid level for spatial tessellation.",
      "properties": {
        "density": {
          "type": "string"
        },
        "level": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      },
      "required": [
        "level",
        "density"
      ],
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.store.SpatialIndexConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "SpatialIndexConfig is the configuration for spatial indexes across different database engines.",
      "properties": {
        "dimensional": {
          "$ref": "#/$defs/bytebase.store.DimensionalConfig.schema.strict.json",
          "description": "Dimensional and constraint parameters"
        },
        "engine_specific": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "Database-specific parameters (stored as key-value pairs for extensibility)",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "method": {
          "description": "Index method/type (database-specific)\n Examples: \"SPATIAL\" (MySQL/SQL Server), \"GIST\"/\"SPGIST\" (PostgreSQL), \"MDSYS.SPATIAL_INDEX_V2\" (Oracle)",
          "type": "string"
        },
        "storage": {
          "$ref": "#/$defs/bytebase.store.StorageConfig.schema.strict.json",
          "description": "Storage and performance parameters"
        },
        "tessellation": {
          "$ref": "#/$defs/bytebase.store.TessellationConfig.schema.strict.json",
          "description": "Tessellation configuration (primarily SQL Server)"
        }
      },
      "required": [
        "method"
      ],
      "title": "Spatial Index Config",
      "type": "object"
    },
    "bytebase.store.StorageConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "StorageConfig defines storage and performance parameters for spatial indexes.",
      "properties": {
        "allow_page_locks": {
          "type": "boolean"
        },
        "allow_row_locks": {
          "type": "boolean"
        },
        "buffering": {
          "type": "string"
        },
        "commit_interval": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "data_compression": {
          "type": "string"
        },
        "drop_existing": {
          "type": "boolean"
        },
        "fillfactor": {
          "description": "PostgreSQL parameters",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "maxdop": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "online": {
          "type": "boolean"
        },
        "pad_index": {
          "description": "SQL Server parameters",
          "type": "boolean"
        },
        "sdo_level": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "sort_in_tempdb": {
          "type": "string"
        },
        "tablespace": {
          "description": "Oracle parameters",
          "type": "string"
        },
        "work_tablespace": {
          "type": "string"
        }
      },
      "required": [
        "fillfactor",
        "buffering",
        "tablespace",
        "work_tablespace",
        "sdo_level",
        "commit_interval",
        "pad_index",
        "sort_in_tempdb",
        "drop_existing",
        "online",
        "allow_row_locks",
        "allow_page_locks",
        "maxdop",
        "data_compression"
      ],
      "title": "Storage Config",
      "type": "object"
    },
    "bytebase.store.TessellationConfig.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "properties": {
        "bounding_box": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.schema.strict.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "cells_per_object": {
          "description": "Cells per object (SQL Server)",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "grid_levels": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.schema.strict.json"
          },
          "type": "array"
        },
        "scheme": {
          "description": "Tessellation scheme\n Examples: GEOMETRY_GRID, GEOGRAPHY_GRID, GEOMETRY_AUTO_GRID, GEOGRAPHY_AUTO_GRID",
          "type": "string"
        }
      },
      "required": [
        "scheme",
        "cells_per_object"
      ],
      "title": "Tessellation Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.SpatialIndexConfig.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.SpatialIndexConfig.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
