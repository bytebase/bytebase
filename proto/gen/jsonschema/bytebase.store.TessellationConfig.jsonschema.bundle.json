{
  "$defs": {
    "bytebase.store.BoundingBox.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the bounding box for spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            },
            {
              "type": "string"
            }
          ],
          "default": 0
        }
      },
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.store.GridLevel.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a grid level for spatial tessellation.",
      "properties": {
        "density": {
          "default": "",
          "type": "string"
        },
        "level": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0
        }
      },
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.store.TessellationConfig.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "patternProperties": {
        "^(bounding_box)$": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.jsonschema.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "^(cells_per_object)$": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Cells per object (SQL Server)"
        },
        "^(grid_levels)$": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.jsonschema.json"
          },
          "type": "array"
        }
      },
      "properties": {
        "boundingBox": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.jsonschema.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "cellsPerObject": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Cells per object (SQL Server)"
        },
        "gridLevels": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.jsonschema.json"
          },
          "type": "array"
        },
        "scheme": {
          "default": "",
          "description": "Tessellation scheme\n Examples: GEOMETRY_GRID, GEOGRAPHY_GRID, GEOMETRY_AUTO_GRID, GEOGRAPHY_AUTO_GRID",
          "type": "string"
        }
      },
      "title": "Tessellation Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.TessellationConfig.jsonschema.bundle.json",
  "$ref": "#/$defs/bytebase.store.TessellationConfig.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
