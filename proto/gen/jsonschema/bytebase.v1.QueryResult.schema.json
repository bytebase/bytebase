{
  "$id": "bytebase.v1.QueryResult.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(columnNames)$": {
      "description": "Column names of the query result.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "^(columnTypeNames)$": {
      "description": "Column types of the query result.\n The types come from the Golang SQL driver.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "^(permissionDenied)$": {
      "$ref": "bytebase.v1.QueryResult.PermissionDenied.schema.json"
    },
    "^(postgresError)$": {
      "$ref": "bytebase.v1.QueryResult.PostgresError.schema.json"
    },
    "^(rowsCount)$": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "^(syntaxError)$": {
      "$ref": "bytebase.v1.QueryResult.SyntaxError.schema.json"
    }
  },
  "properties": {
    "column_names": {
      "description": "Column names of the query result.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "column_type_names": {
      "description": "Column types of the query result.\n The types come from the Golang SQL driver.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "error": {
      "default": "",
      "description": "The error message if the query failed.",
      "type": "string"
    },
    "latency": {
      "$ref": "google.protobuf.Duration.schema.json",
      "description": "The time it takes to execute the query."
    },
    "masked": {
      "description": "Masking reasons for each column (empty for non-masked columns).",
      "items": {
        "$ref": "bytebase.v1.MaskingReason.schema.json"
      },
      "type": "array"
    },
    "messages": {
      "description": "Informational or debug messages returned by the database engine during query execution.\n Examples include PostgreSQL's RAISE NOTICE, MSSQL's PRINT, or Oracle's DBMS_OUTPUT.PUT_LINE.",
      "items": {
        "$ref": "bytebase.v1.QueryResult.Message.schema.json"
      },
      "type": "array"
    },
    "permission_denied": {
      "$ref": "bytebase.v1.QueryResult.PermissionDenied.schema.json"
    },
    "postgres_error": {
      "$ref": "bytebase.v1.QueryResult.PostgresError.schema.json"
    },
    "rows": {
      "description": "Rows of the query result.",
      "items": {
        "$ref": "bytebase.v1.QueryRow.schema.json"
      },
      "type": "array"
    },
    "rows_count": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "statement": {
      "default": "",
      "description": "The query statement for the result.",
      "type": "string"
    },
    "syntax_error": {
      "$ref": "bytebase.v1.QueryResult.SyntaxError.schema.json"
    }
  },
  "title": "Query Result",
  "type": "object"
}
