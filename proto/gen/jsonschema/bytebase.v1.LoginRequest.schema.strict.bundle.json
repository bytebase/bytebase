{
  "$defs": {
    "bytebase.v1.IdentityProviderContext.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Context for identity provider authentication.",
      "properties": {
        "oauth2_context": {
          "$ref": "#/$defs/bytebase.v1.OAuth2IdentityProviderContext.schema.strict.json",
          "description": "OAuth2 authentication context."
        },
        "oidc_context": {
          "$ref": "#/$defs/bytebase.v1.OIDCIdentityProviderContext.schema.strict.json",
          "description": "OpenID Connect authentication context."
        }
      },
      "title": "Identity Provider Context",
      "type": "object"
    },
    "bytebase.v1.LoginRequest.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "email": {
          "description": "User's email address.",
          "type": "string"
        },
        "idp_context": {
          "$ref": "#/$defs/bytebase.v1.IdentityProviderContext.schema.strict.json",
          "description": "The idp_context is using to get the user information from identity provider."
        },
        "idp_name": {
          "description": "The name of the identity provider.\n Format: idps/{idp}",
          "type": "string"
        },
        "mfa_temp_token": {
          "description": "The mfa_temp_token is used to verify the user's identity by MFA.",
          "type": "string"
        },
        "otp_code": {
          "description": "The otp_code is used to verify the user's identity by MFA.",
          "type": "string"
        },
        "password": {
          "description": "User's password for authentication.",
          "type": "string"
        },
        "recovery_code": {
          "description": "The recovery_code is used to recovery the user's identity with MFA.",
          "type": "string"
        },
        "web": {
          "description": "If web is set, we will set access token, refresh token, and user to the cookie.",
          "type": "boolean"
        }
      },
      "required": [
        "email",
        "password",
        "web",
        "idp_name"
      ],
      "title": "Login Request",
      "type": "object"
    },
    "bytebase.v1.OAuth2IdentityProviderContext.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "OAuth2 authentication context.",
      "properties": {
        "code": {
          "description": "Authorization code from OAuth2 provider.",
          "type": "string"
        }
      },
      "required": [
        "code"
      ],
      "title": "O Auth2 Identity Provider Context",
      "type": "object"
    },
    "bytebase.v1.OIDCIdentityProviderContext.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "OpenID Connect authentication context.",
      "properties": {
        "code": {
          "description": "Authorization code from OIDC provider.",
          "type": "string"
        }
      },
      "required": [
        "code"
      ],
      "title": "OIDC Identity Provider Context",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.LoginRequest.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.LoginRequest.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
