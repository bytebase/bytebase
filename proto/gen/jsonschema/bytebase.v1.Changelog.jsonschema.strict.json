{
  "$id": "bytebase.v1.Changelog.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "changedResources": {
      "$ref": "bytebase.v1.ChangedResources.jsonschema.strict.json"
    },
    "createTime": {
      "$ref": "google.protobuf.Timestamp.jsonschema.strict.json"
    },
    "dumpVersionMismatch": {
      "description": "True if this changelog's dump version differs from current engine dump version.\n When true and drift is detected, the drift may be a false positive from Bytebase upgrade.",
      "type": "boolean"
    },
    "issue": {
      "description": "Format: projects/{project}/issues/{issue}",
      "type": "string"
    },
    "name": {
      "description": "Format: instances/{instance}/databases/{database}/changelogs/{changelog}",
      "type": "string"
    },
    "prevSchema": {
      "type": "string"
    },
    "prevSchemaSize": {
      "type": "integer"
    },
    "revision": {
      "description": "Could be empty\n Or present but not found if deleted",
      "type": "string"
    },
    "schema": {
      "type": "string"
    },
    "schemaSize": {
      "type": "integer"
    },
    "statement": {
      "description": "The statement is used for preview purpose.",
      "type": "string"
    },
    "statementSheet": {
      "description": "The name of the sheet resource.\n Format: projects/{project}/sheets/{sheet}",
      "type": "string"
    },
    "statementSize": {
      "type": "integer"
    },
    "status": {
      "enum": [
        "STATUS_UNSPECIFIED",
        "PENDING",
        "DONE",
        "FAILED"
      ],
      "title": "Status",
      "type": "string"
    },
    "taskRun": {
      "description": "Format: projects/{projects}/rollouts/{rollout}/stages/{stage}/tasks/{task}/taskRuns/{taskRun}",
      "type": "string"
    },
    "type": {
      "enum": [
        "TYPE_UNSPECIFIED",
        "BASELINE",
        "MIGRATE",
        "SDL"
      ],
      "title": "Type",
      "type": "string"
    },
    "version": {
      "description": "Could be empty",
      "type": "string"
    }
  },
  "required": [
    "name",
    "status",
    "statement",
    "statementSize",
    "statementSheet",
    "schema",
    "schemaSize",
    "prevSchema",
    "prevSchemaSize",
    "issue",
    "taskRun",
    "version",
    "revision",
    "type",
    "dumpVersionMismatch"
  ],
  "title": "Changelog",
  "type": "object"
}
