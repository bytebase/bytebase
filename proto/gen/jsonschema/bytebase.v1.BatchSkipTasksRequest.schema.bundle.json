{
  "$defs": {
    "bytebase.v1.BatchSkipTasksRequest.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "parent": {
          "default": "",
          "description": "The name of the parent of the tasks.\n Format: projects/{project}/rollouts/{rollout}/stages/{stage}",
          "type": "string"
        },
        "reason": {
          "default": "",
          "description": "The reason for skipping the tasks.",
          "maxLength": 1000,
          "type": "string"
        },
        "tasks": {
          "description": "The tasks to skip.\n Format: projects/{project}/rollouts/{rollout}/stages/{stage}/tasks/{task}",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Batch Skip Tasks Request",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.BatchSkipTasksRequest.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.BatchSkipTasksRequest.schema.json",
