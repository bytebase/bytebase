{
  "$defs": {
    "bytebase.store.FieldMapping.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "FieldMapping saves the field names from user info API of identity provider.\n As we save all raw json string of user info response data into `principal.idp_user_info`,\n we can extract the relevant data based with `FieldMapping`.",
      "patternProperties": {
        "^(displayName)$": {
          "default": "",
          "description": "DisplayName is the field name of display name in 3rd-party idp user info. Optional.",
          "type": "string"
        }
      },
      "properties": {
        "display_name": {
          "default": "",
          "description": "DisplayName is the field name of display name in 3rd-party idp user info. Optional.",
          "type": "string"
        },
        "groups": {
          "default": "",
          "description": "Groups is the field name of groups in 3rd-party idp user info. Optional.\n Mainly used for OIDC: https://developer.okta.com/docs/guides/customize-tokens-groups-claim/main/",
          "type": "string"
        },
        "identifier": {
          "default": "",
          "description": "Identifier is the field name of the unique identifier in 3rd-party idp user info. Required.",
          "type": "string"
        },
        "phone": {
          "default": "",
          "description": "Phone is the field name of primary phone in 3rd-party idp user info. Optional.",
          "type": "string"
        }
      },
      "title": "Field Mapping",
      "type": "object"
    },
    "bytebase.store.IdentityProviderConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(ldapConfig)$": {
          "$ref": "#/$defs/bytebase.store.LDAPIdentityProviderConfig.schema.json"
        },
        "^(oauth2Config)$": {
          "$ref": "#/$defs/bytebase.store.OAuth2IdentityProviderConfig.schema.json"
        },
        "^(oidcConfig)$": {
          "$ref": "#/$defs/bytebase.store.OIDCIdentityProviderConfig.schema.json"
        }
      },
      "properties": {
        "ldap_config": {
          "$ref": "#/$defs/bytebase.store.LDAPIdentityProviderConfig.schema.json"
        },
        "oauth2_config": {
          "$ref": "#/$defs/bytebase.store.OAuth2IdentityProviderConfig.schema.json"
        },
        "oidc_config": {
          "$ref": "#/$defs/bytebase.store.OIDCIdentityProviderConfig.schema.json"
        }
      },
      "title": "Identity Provider Config",
      "type": "object"
    },
    "bytebase.store.LDAPIdentityProviderConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "LDAPIdentityProviderConfig is the structure for LDAP identity provider config.",
      "patternProperties": {
        "^(baseDn)$": {
          "default": "",
          "description": "BaseDN is the base DN to search for users, e.g. \"ou=users,dc=example,dc=com\".",
          "type": "string"
        },
        "^(bindDn)$": {
          "default": "",
          "description": "BindDN is the DN of the user to bind as a service account to perform\n search requests.",
          "type": "string"
        },
        "^(bindPassword)$": {
          "default": "",
          "description": "BindPassword is the password of the user to bind as a service account.",
          "type": "string"
        },
        "^(fieldMapping)$": {
          "$ref": "#/$defs/bytebase.store.FieldMapping.schema.json",
          "description": "FieldMapping is the mapping of the user attributes returned by the LDAP\n server."
        },
        "^(securityProtocol)$": {
          "anyOf": [
            {
              "pattern": "^SECURITY_PROTOCOL_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "START_TLS",
                "LDAPS"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "SecurityProtocol is the security protocol to be used for establishing\n connections with the LDAP server.",
          "title": "Security Protocol"
        },
        "^(skipTlsVerify)$": {
          "default": false,
          "description": "SkipTLSVerify controls whether to skip TLS certificate verification.",
          "type": "boolean"
        },
        "^(userFilter)$": {
          "default": "",
          "description": "UserFilter is the filter to search for users, e.g. \"(uid=%s)\".",
          "type": "string"
        }
      },
      "properties": {
        "base_dn": {
          "default": "",
          "description": "BaseDN is the base DN to search for users, e.g. \"ou=users,dc=example,dc=com\".",
          "type": "string"
        },
        "bind_dn": {
          "default": "",
          "description": "BindDN is the DN of the user to bind as a service account to perform\n search requests.",
          "type": "string"
        },
        "bind_password": {
          "default": "",
          "description": "BindPassword is the password of the user to bind as a service account.",
          "type": "string"
        },
        "field_mapping": {
          "$ref": "#/$defs/bytebase.store.FieldMapping.schema.json",
          "description": "FieldMapping is the mapping of the user attributes returned by the LDAP\n server."
        },
        "host": {
          "default": "",
          "description": "Host is the hostname or IP address of the LDAP server, e.g.\n \"ldap.example.com\".",
          "type": "string"
        },
        "port": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "Port is the port number of the LDAP server, e.g. 389. When not set, the\n default port of the corresponding security protocol will be used, i.e. 389\n for StartTLS and 636 for LDAPS."
        },
        "security_protocol": {
          "anyOf": [
            {
              "pattern": "^SECURITY_PROTOCOL_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "START_TLS",
                "LDAPS"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "SecurityProtocol is the security protocol to be used for establishing\n connections with the LDAP server.",
          "title": "Security Protocol"
        },
        "skip_tls_verify": {
          "default": false,
          "description": "SkipTLSVerify controls whether to skip TLS certificate verification.",
          "type": "boolean"
        },
        "user_filter": {
          "default": "",
          "description": "UserFilter is the filter to search for users, e.g. \"(uid=%s)\".",
          "type": "string"
        }
      },
      "title": "LDAP Identity Provider Config",
      "type": "object"
    },
    "bytebase.store.OAuth2IdentityProviderConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "OAuth2IdentityProviderConfig is the structure for OAuth2 identity provider config.",
      "patternProperties": {
        "^(authStyle)$": {
          "anyOf": [
            {
              "pattern": "^OAUTH2_AUTH_STYLE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "IN_PARAMS",
                "IN_HEADER"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "O Auth2 Auth Style"
        },
        "^(authUrl)$": {
          "default": "",
          "type": "string"
        },
        "^(clientId)$": {
          "default": "",
          "type": "string"
        },
        "^(clientSecret)$": {
          "default": "",
          "type": "string"
        },
        "^(fieldMapping)$": {
          "$ref": "#/$defs/bytebase.store.FieldMapping.schema.json"
        },
        "^(skipTlsVerify)$": {
          "default": false,
          "type": "boolean"
        },
        "^(tokenUrl)$": {
          "default": "",
          "type": "string"
        },
        "^(userInfoUrl)$": {
          "default": "",
          "type": "string"
        }
      },
      "properties": {
        "auth_style": {
          "anyOf": [
            {
              "pattern": "^OAUTH2_AUTH_STYLE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "IN_PARAMS",
                "IN_HEADER"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "O Auth2 Auth Style"
        },
        "auth_url": {
          "default": "",
          "type": "string"
        },
        "client_id": {
          "default": "",
          "type": "string"
        },
        "client_secret": {
          "default": "",
          "type": "string"
        },
        "field_mapping": {
          "$ref": "#/$defs/bytebase.store.FieldMapping.schema.json"
        },
        "scopes": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "skip_tls_verify": {
          "default": false,
          "type": "boolean"
        },
        "token_url": {
          "default": "",
          "type": "string"
        },
        "user_info_url": {
          "default": "",
          "type": "string"
        }
      },
      "title": "O Auth2 Identity Provider Config",
      "type": "object"
    },
    "bytebase.store.OIDCIdentityProviderConfig.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "OIDCIdentityProviderConfig is the structure for OIDC identity provider config.",
      "patternProperties": {
        "^(authStyle)$": {
          "anyOf": [
            {
              "pattern": "^OAUTH2_AUTH_STYLE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "IN_PARAMS",
                "IN_HEADER"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "O Auth2 Auth Style"
        },
        "^(clientId)$": {
          "default": "",
          "type": "string"
        },
        "^(clientSecret)$": {
          "default": "",
          "type": "string"
        },
        "^(fieldMapping)$": {
          "$ref": "#/$defs/bytebase.store.FieldMapping.schema.json"
        },
        "^(skipTlsVerify)$": {
          "default": false,
          "type": "boolean"
        }
      },
      "properties": {
        "auth_style": {
          "anyOf": [
            {
              "pattern": "^OAUTH2_AUTH_STYLE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "IN_PARAMS",
                "IN_HEADER"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "O Auth2 Auth Style"
        },
        "client_id": {
          "default": "",
          "type": "string"
        },
        "client_secret": {
          "default": "",
          "type": "string"
        },
        "field_mapping": {
          "$ref": "#/$defs/bytebase.store.FieldMapping.schema.json"
        },
        "issuer": {
          "default": "",
          "type": "string"
        },
        "scopes": {
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "skip_tls_verify": {
          "default": false,
          "type": "boolean"
        }
      },
      "title": "OIDC Identity Provider Config",
      "type": "object"
    }
  },
  "$id": "bytebase.store.IdentityProviderConfig.schema.bundle.json",
  "$ref": "#/$defs/bytebase.store.IdentityProviderConfig.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
