{
  "$id": "bytebase.store.SchemaMetadata.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "SchemaMetadata is the metadata for schemas.\n This is the concept of schema in Postgres, but it's a no-op for MySQL.",
  "properties": {
    "comment": {
      "type": "string"
    },
    "enumTypes": {
      "items": {
        "$ref": "bytebase.store.EnumTypeMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "events": {
      "items": {
        "$ref": "bytebase.store.EventMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "externalTables": {
      "description": "The list of external tables in a schema.",
      "items": {
        "$ref": "bytebase.store.ExternalTableMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "functions": {
      "description": "The list of functions in a schema.",
      "items": {
        "$ref": "bytebase.store.FunctionMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "materializedViews": {
      "description": "The list of materialized views in a schema.",
      "items": {
        "$ref": "bytebase.store.MaterializedViewMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "name": {
      "description": "The schema name.\n It is an empty string for databases without such concept such as MySQL.",
      "type": "string"
    },
    "owner": {
      "type": "string"
    },
    "packages": {
      "description": "The list of packages in a schema.",
      "items": {
        "$ref": "bytebase.store.PackageMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "procedures": {
      "description": "The list of procedures in a schema.",
      "items": {
        "$ref": "bytebase.store.ProcedureMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "sequences": {
      "description": "The list of sequences in a schema.",
      "items": {
        "$ref": "bytebase.store.SequenceMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "skipDump": {
      "type": "boolean"
    },
    "streams": {
      "description": "The list of streams in a schema, currently only used for Snowflake.",
      "items": {
        "$ref": "bytebase.store.StreamMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "tables": {
      "description": "The list of tables in a schema.",
      "items": {
        "$ref": "bytebase.store.TableMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "tasks": {
      "description": "The list of tasks in a schema, currently only used for Snowflake.",
      "items": {
        "$ref": "bytebase.store.TaskMetadata.jsonschema.strict.json"
      },
      "type": "array"
    },
    "views": {
      "description": "The list of views in a schema.",
      "items": {
        "$ref": "bytebase.store.ViewMetadata.jsonschema.strict.json"
      },
      "type": "array"
    }
  },
  "required": [
    "name",
    "owner",
    "comment",
    "skipDump"
  ],
  "title": "Schema Metadata",
  "type": "object"
}
