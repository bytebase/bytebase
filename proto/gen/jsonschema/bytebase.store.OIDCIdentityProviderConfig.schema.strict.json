{
  "$id": "bytebase.store.OIDCIdentityProviderConfig.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "OIDCIdentityProviderConfig is the structure for OIDC identity provider config.",
  "properties": {
    "auth_style": {
      "enum": [
        "OAUTH2_AUTH_STYLE_UNSPECIFIED",
        "IN_PARAMS",
        "IN_HEADER"
      ],
      "title": "O Auth2 Auth Style",
      "type": "string"
    },
    "client_id": {
      "type": "string"
    },
    "client_secret": {
      "type": "string"
    },
    "field_mapping": {
      "$ref": "bytebase.store.FieldMapping.schema.strict.json"
    },
    "issuer": {
      "type": "string"
    },
    "scopes": {
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "skip_tls_verify": {
      "type": "boolean"
    }
  },
  "required": [
    "issuer",
    "client_id",
    "client_secret",
    "skip_tls_verify",
    "auth_style"
  ],
  "title": "OIDC Identity Provider Config",
  "type": "object"
}
