{
  "$defs": {
    "bytebase.v1.BatchUpdateIssuesStatusRequest.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "issues": {
          "description": "The list of issues to update.\n Format: projects/{project}/issues/{issue}",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "parent": {
          "description": "The parent resource shared by all issues being updated.\n Format: projects/{project}\n If the operation spans parents, a dash (-) may be accepted as a wildcard.\n We only support updating the status of databases for now.",
          "type": "string"
        },
        "reason": {
          "description": "The reason for the status change.",
          "type": "string"
        },
        "status": {
          "description": "The new status.",
          "enum": [
            "ISSUE_STATUS_UNSPECIFIED",
            "OPEN",
            "DONE",
            "CANCELED"
          ],
          "title": "Issue Status",
          "type": "string"
        }
      },
      "required": [
        "parent",
        "status",
        "reason"
      ],
      "title": "Batch Update Issues Status Request",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.BatchUpdateIssuesStatusRequest.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.BatchUpdateIssuesStatusRequest.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
