{
  "$id": "bytebase.store.EnumTypeMetadata.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(skip_dump)$": {
      "default": false,
      "type": "boolean"
    }
  },
  "properties": {
    "comment": {
      "default": "",
      "type": "string"
    },
    "name": {
      "default": "",
      "description": "The name of the enum type.",
      "type": "string"
    },
    "skipDump": {
      "default": false,
      "type": "boolean"
    },
    "values": {
      "description": "The enum values of the type.",
      "items": {
        "type": "string"
      },
      "type": "array"
    }
  },
  "title": "Enum Type Metadata",
  "type": "object"
}
