        "^(end_position)$": {
          "$ref": "#/$defs/bytebase.store.Position.jsonschema.json",
          "description": "Ending position in SQL for this backup operation."
        },
        "^(source_table)$": {
          "$ref": "#/$defs/bytebase.store.PriorBackupDetail.Item.Table.jsonschema.json",
          "description": "The original table that was backed up."
        },
        "^(start_position)$": {
          "$ref": "#/$defs/bytebase.store.Position.jsonschema.json",
          "description": "Starting position in SQL for this backup operation."
        },
        "^(target_table)$": {
          "$ref": "#/$defs/bytebase.store.PriorBackupDetail.Item.Table.jsonschema.json",
          "description": "The backup table where data was copied."
        }
      },
      "properties": {
        "endPosition": {
          "$ref": "#/$defs/bytebase.store.Position.jsonschema.json",
          "description": "Ending position in SQL for this backup operation."
        },
        "sourceTable": {
          "$ref": "#/$defs/bytebase.store.PriorBackupDetail.Item.Table.jsonschema.json",
          "description": "The original table that was backed up."
        },
        "startPosition": {
          "$ref": "#/$defs/bytebase.store.Position.jsonschema.json",
          "description": "Starting position in SQL for this backup operation."
        },
        "targetTable": {
          "$ref": "#/$defs/bytebase.store.PriorBackupDetail.Item.Table.jsonschema.json",
          "description": "The backup table where data was copied."
        }
      },
      "title": "Item",
      "type": "object"
    },
