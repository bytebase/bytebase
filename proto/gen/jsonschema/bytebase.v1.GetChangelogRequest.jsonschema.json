{
  "$id": "bytebase.v1.GetChangelogRequest.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "name": {
      "default": "",
      "description": "The name of the changelog to retrieve.\n Format: instances/{instance}/databases/{database}/changelogs/{changelog}",
      "type": "string"
    },
    "view": {
      "anyOf": [
        {
          "pattern": "^CHANGELOG_VIEW_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "CHANGELOG_VIEW_BASIC",
            "CHANGELOG_VIEW_FULL"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Changelog View"
    }
  },
  "title": "Get Changelog Request",
  "type": "object"
}
