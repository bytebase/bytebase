{
  "$defs": {
    "bytebase.v1.ColumnCatalog.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Column metadata within a table.",
      "properties": {
        "classification": {
          "description": "The data classification level for this column.",
          "type": "string"
        },
        "labels": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "User-defined labels for this column.",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "name": {
          "description": "The column name.",
          "type": "string"
        },
        "object_schema": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.schema.strict.json",
          "description": "Object schema for complex column types like JSON."
        },
        "semantic_type": {
          "description": "The semantic type describing the data purpose.",
          "type": "string"
        }
      },
      "required": [
        "name",
        "semantic_type",
        "classification"
      ],
      "title": "Column Catalog",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.ArrayKind.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Array type with element schema.",
      "properties": {
        "kind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.schema.strict.json",
          "description": "The schema of array elements."
        }
      },
      "title": "Array Kind",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.StructKind.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Structure type with named properties.",
      "properties": {
        "properties": {
          "additionalProperties": {
            "$ref": "#/$defs/bytebase.v1.ObjectSchema.schema.strict.json"
          },
          "description": "Properties of the struct.",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        }
      },
      "title": "Struct Kind",
      "type": "object"
    },
    "bytebase.v1.ObjectSchema.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Schema definition for object-type columns.",
      "properties": {
        "array_kind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.ArrayKind.schema.strict.json",
          "description": "Array schema."
        },
        "semantic_type": {
          "description": "The semantic type of this object.",
          "type": "string"
        },
        "struct_kind": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.StructKind.schema.strict.json",
          "description": "Struct schema."
        },
        "type": {
          "description": "The data type of this object.",
          "enum": [
            "TYPE_UNSPECIFIED",
            "STRING",
            "NUMBER",
            "BOOLEAN",
            "OBJECT",
            "ARRAY"
          ],
          "title": "Type",
          "type": "string"
        }
      },
      "required": [
        "type",
        "semantic_type"
      ],
      "title": "Object Schema",
      "type": "object"
    },
    "bytebase.v1.SchemaCatalog.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Schema metadata within a database.",
      "properties": {
        "name": {
          "description": "The schema name.",
          "type": "string"
        },
        "tables": {
          "description": "The tables in the schema.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.TableCatalog.schema.strict.json"
          },
          "type": "array"
        }
      },
      "required": [
        "name"
      ],
      "title": "Schema Catalog",
      "type": "object"
    },
    "bytebase.v1.TableCatalog.Columns.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Column list for regular tables.",
      "properties": {
        "columns": {
          "description": "The columns in the table.",
          "items": {
            "$ref": "#/$defs/bytebase.v1.ColumnCatalog.schema.strict.json"
          },
          "type": "array"
        }
      },
      "title": "Columns",
      "type": "object"
    },
    "bytebase.v1.TableCatalog.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Table metadata within a schema.",
      "properties": {
        "classification": {
          "description": "The data classification level for this table.",
          "type": "string"
        },
        "columns": {
          "$ref": "#/$defs/bytebase.v1.TableCatalog.Columns.schema.strict.json",
          "description": "Regular table columns."
        },
        "name": {
          "description": "The table name.",
          "type": "string"
        },
        "object_schema": {
          "$ref": "#/$defs/bytebase.v1.ObjectSchema.schema.strict.json",
          "description": "Object schema for JSON/XML columns."
        }
      },
      "required": [
        "name",
        "classification"
      ],
      "title": "Table Catalog",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.SchemaCatalog.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.v1.SchemaCatalog.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
