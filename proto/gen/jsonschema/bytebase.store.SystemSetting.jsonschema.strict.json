{
  "$id": "bytebase.store.SystemSetting.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "authSecret": {
      "description": "Authentication secret for token signing (32-character random string).",
      "type": "string"
    },
    "license": {
      "description": "Enterprise license JWT token.",
      "type": "string"
    },
    "workspaceId": {
      "description": "Unique workspace identifier (UUID).",
      "type": "string"
    }
  },
  "required": [
    "authSecret",
    "workspaceId",
    "license"
  ],
  "title": "System Setting",
  "type": "object"
}
