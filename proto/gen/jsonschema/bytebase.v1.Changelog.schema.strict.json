{
  "$id": "bytebase.v1.Changelog.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "changed_resources": {
      "$ref": "bytebase.v1.ChangedResources.schema.strict.json"
    },
    "create_time": {
      "$ref": "google.protobuf.Timestamp.schema.strict.json"
    },
    "dump_version_mismatch": {
      "description": "True if this changelog's dump version differs from current engine dump version.\n When true and drift is detected, the drift may be a false positive from Bytebase upgrade.",
      "type": "boolean"
    },
    "issue": {
      "description": "Format: projects/{project}/issues/{issue}",
      "type": "string"
    },
    "name": {
      "description": "Format: instances/{instance}/databases/{database}/changelogs/{changelog}",
      "type": "string"
    },
    "prev_schema": {
      "type": "string"
    },
    "prev_schema_size": {
      "type": "integer"
    },
    "revision": {
      "description": "Could be empty\n Or present but not found if deleted",
      "type": "string"
    },
    "schema": {
      "type": "string"
    },
    "schema_size": {
      "type": "integer"
    },
    "statement": {
      "description": "The statement is used for preview purpose.",
      "type": "string"
    },
    "statement_sheet": {
      "description": "The name of the sheet resource.\n Format: projects/{project}/sheets/{sheet}",
      "type": "string"
    },
    "statement_size": {
      "type": "integer"
    },
    "status": {
      "enum": [
        "STATUS_UNSPECIFIED",
        "PENDING",
        "DONE",
        "FAILED"
      ],
      "title": "Status",
      "type": "string"
    },
    "task_run": {
      "description": "Format: projects/{projects}/rollouts/{rollout}/stages/{stage}/tasks/{task}/taskRuns/{taskRun}",
      "type": "string"
    },
    "type": {
      "enum": [
        "TYPE_UNSPECIFIED",
        "BASELINE",
        "MIGRATE",
        "SDL"
      ],
      "title": "Type",
      "type": "string"
    },
    "version": {
      "description": "Could be empty",
      "type": "string"
    }
  },
  "required": [
    "name",
    "status",
    "statement",
    "statement_size",
    "statement_sheet",
    "schema",
    "schema_size",
    "prev_schema",
    "prev_schema_size",
    "issue",
    "task_run",
    "version",
    "revision",
    "type",
    "dump_version_mismatch"
  ],
  "title": "Changelog",
  "type": "object"
}
