{
  "$defs": {
    "bytebase.v1.QueryOption.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(mssqlExplainFormat)$": {
          "anyOf": [
            {
              "pattern": "^MSSQL_EXPLAIN_FORMAT_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "MSSQL_EXPLAIN_FORMAT_ALL",
                "MSSQL_EXPLAIN_FORMAT_XML"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "MSSQL Explain Format"
        },
        "^(redisRunCommandsOn)$": {
          "anyOf": [
            {
              "pattern": "^REDIS_RUN_COMMANDS_ON_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "SINGLE_NODE",
                "ALL_NODES"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Redis Run Commands On"
        }
      },
      "properties": {
        "mssql_explain_format": {
          "anyOf": [
            {
              "pattern": "^MSSQL_EXPLAIN_FORMAT_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "MSSQL_EXPLAIN_FORMAT_ALL",
                "MSSQL_EXPLAIN_FORMAT_XML"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "MSSQL Explain Format"
        },
        "redis_run_commands_on": {
          "anyOf": [
            {
              "pattern": "^REDIS_RUN_COMMANDS_ON_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "SINGLE_NODE",
                "ALL_NODES"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "title": "Redis Run Commands On"
        }
      },
      "title": "Query Option",
      "type": "object"
    },
    "bytebase.v1.QueryRequest.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(dataSourceId)$": {
          "default": "",
          "description": "The id of data source.\n It is used for querying admin data source even if the instance has\n read-only data sources. Or it can be used to query a specific read-only\n data source.",
          "type": "string"
        },
        "^(queryOption)$": {
          "$ref": "#/$defs/bytebase.v1.QueryOption.schema.json"
        }
      },
      "properties": {
        "container": {
          "description": "Container is the container name to execute the query against, used for\n CosmosDB only.",
          "type": "string"
        },
        "data_source_id": {
          "default": "",
          "description": "The id of data source.\n It is used for querying admin data source even if the instance has\n read-only data sources. Or it can be used to query a specific read-only\n data source.",
          "type": "string"
        },
        "explain": {
          "default": false,
          "description": "Explain the statement.",
          "type": "boolean"
        },
        "limit": {
          "anyOf": [
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            },
            {
              "pattern": "^-?[0-9]+$",
              "type": "string"
            }
          ],
          "default": 0,
          "description": "The maximum number of rows to return."
        },
        "name": {
          "default": "",
          "description": "The name is the instance name to execute the query against.\n Format: instances/{instance}/databases/{databaseName}",
          "type": "string"
        },
        "query_option": {
          "$ref": "#/$defs/bytebase.v1.QueryOption.schema.json"
        },
        "schema": {
          "description": "The default schema to search objects. Equals to the current schema in\n Oracle and search path in Postgres.",
          "type": "string"
        },
        "statement": {
          "default": "",
          "description": "The SQL statement to execute.",
          "type": "string"
        }
      },
      "title": "Query Request",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.QueryRequest.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.QueryRequest.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
