{
  "$id": "bytebase.v1.Release.VCSSource.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "description": "Version control system source information.",
  "properties": {
    "url": {
      "description": "The url link to the e.g., GitHub commit or pull request.",
      "type": "string"
    },
    "vcsType": {
      "description": "The type of VCS.",
      "enum": [
        "VCS_TYPE_UNSPECIFIED",
        "GITHUB",
        "GITLAB",
        "BITBUCKET",
        "AZURE_DEVOPS"
      ],
      "title": "VCS Type",
      "type": "string"
    }
  },
  "required": [
    "vcsType",
    "url"
  ],
  "title": "VCS Source",
  "type": "object"
}
