{
  "$defs": {
    "bytebase.store.EnvironmentSetting.Environment.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "color": {
          "type": "string"
        },
        "id": {
          "description": "The resource id of the environment.\n This value should be 4-63 characters, and valid characters\n are /[a-z][0-9]-/.",
          "type": "string"
        },
        "tags": {
          "additionalProperties": {
            "type": "string"
          },
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "title": {
          "description": "The display name of the environment.",
          "type": "string"
        }
      },
      "required": [
        "id",
        "title",
        "color"
      ],
      "title": "Environment",
      "type": "object"
    },
    "bytebase.store.EnvironmentSetting.schema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "environments": {
          "items": {
            "$ref": "#/$defs/bytebase.store.EnvironmentSetting.Environment.schema.strict.json"
          },
          "type": "array"
        }
      },
      "title": "Environment Setting",
      "type": "object"
    }
  },
  "$id": "bytebase.store.EnvironmentSetting.schema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.EnvironmentSetting.schema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
