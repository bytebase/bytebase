{
  "$defs": {
    "bytebase.v1.StreamMetadata.schema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(tableName)$": {
          "default": "",
          "description": "The table_name is the name of the table/view that the stream is created on.",
          "type": "string"
        }
      },
      "properties": {
        "comment": {
          "default": "",
          "description": "The comment of the stream.",
          "type": "string"
        },
        "definition": {
          "default": "",
          "description": "The definition of the stream.",
          "type": "string"
        },
        "mode": {
          "anyOf": [
            {
              "pattern": "^MODE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "DEFAULT",
                "APPEND_ONLY",
                "INSERT_ONLY"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "The mode of the stream.",
          "title": "Mode"
        },
        "name": {
          "default": "",
          "description": "The name is the name of a stream.",
          "type": "string"
        },
        "owner": {
          "default": "",
          "description": "The owner of the stream.",
          "type": "string"
        },
        "stale": {
          "default": false,
          "description": "Indicates whether the stream was last read before the `stale_after` time.",
          "type": "boolean"
        },
        "table_name": {
          "default": "",
          "description": "The table_name is the name of the table/view that the stream is created on.",
          "type": "string"
        },
        "type": {
          "anyOf": [
            {
              "pattern": "^TYPE_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "DELTA"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "The type of the stream.",
          "title": "Type"
        }
      },
      "title": "Stream Metadata",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.StreamMetadata.schema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.StreamMetadata.schema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
