{
  "$defs": {
    "bytebase.store.BoundingBox.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "BoundingBox defines the bounding box for spatial indexes.",
      "properties": {
        "xmax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "xmin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "ymax": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        },
        "ymin": {
          "anyOf": [
            {
              "type": "number"
            },
            {
              "enum": [
                "Infinity",
                "-Infinity",
                "NaN"
              ],
              "type": "string"
            }
          ]
        }
      },
      "required": [
        "xmin",
        "ymin",
        "xmax",
        "ymax"
      ],
      "title": "Bounding Box",
      "type": "object"
    },
    "bytebase.store.DependencyColumn.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DependencyColumn is the metadata for dependency columns.",
      "properties": {
        "column": {
          "description": "The column is the name of a reference column.",
          "type": "string"
        },
        "schema": {
          "description": "The schema is the schema of a reference column.",
          "type": "string"
        },
        "table": {
          "description": "The table is the table of a reference column.",
          "type": "string"
        }
      },
      "required": [
        "schema",
        "table",
        "column"
      ],
      "title": "Dependency Column",
      "type": "object"
    },
    "bytebase.store.DimensionalConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "DimensionalConfig defines dimensional and constraint parameters for spatial indexes.",
      "properties": {
        "dataType": {
          "description": "Spatial data type\n Examples: GEOMETRY, GEOGRAPHY, POINT, POLYGON, etc.",
          "type": "string"
        },
        "dimensions": {
          "description": "Number of dimensions (2-4, default 2)",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "layerGtype": {
          "description": "Oracle geometry type constraint\n Examples: POINT, LINE, POLYGON, COLLECTION",
          "type": "string"
        },
        "operatorClass": {
          "description": "PostgreSQL operator class\n Examples: gist_geometry_ops_2d, gist_geometry_ops_nd, etc.",
          "type": "string"
        },
        "parallelBuild": {
          "description": "Parallel index creation",
          "type": "boolean"
        }
      },
      "required": [
        "dimensions",
        "dataType",
        "operatorClass",
        "layerGtype",
        "parallelBuild"
      ],
      "title": "Dimensional Config",
      "type": "object"
    },
    "bytebase.store.GridLevel.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "GridLevel defines a grid level for spatial tessellation.",
      "properties": {
        "density": {
          "type": "string"
        },
        "level": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      },
      "required": [
        "level",
        "density"
      ],
      "title": "Grid Level",
      "type": "object"
    },
    "bytebase.store.IndexMetadata.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "IndexMetadata is the metadata for indexes.",
      "properties": {
        "comment": {
          "description": "The comment is the comment of an index.",
          "type": "string"
        },
        "definition": {
          "description": "The definition of an index.",
          "type": "string"
        },
        "descending": {
          "description": "The ordered list of descending flags for the index columns.",
          "items": {
            "type": "boolean"
          },
          "type": "array"
        },
        "expressions": {
          "description": "The expressions are the ordered columns or expressions of an index.\n This could refer to a column or an expression.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "granularity": {
          "description": "The number of granules in the block. It's a ClickHouse specific field.",
          "type": "integer"
        },
        "isConstraint": {
          "description": "It's a PostgreSQL specific field.\n The unique constraint and unique index are not the same thing in PostgreSQL.",
          "type": "boolean"
        },
        "keyLength": {
          "description": "The ordered list of key lengths for the index.\n If the key length is not specified, it is -1.",
          "items": {
            "type": "integer"
          },
          "type": "array"
        },
        "name": {
          "description": "The name of the index.",
          "type": "string"
        },
        "opclassDefaults": {
          "description": "True if the operator class is the default. (PostgreSQL specific).",
          "items": {
            "type": "boolean"
          },
          "type": "array"
        },
        "opclassNames": {
          "description": "https://www.postgresql.org/docs/current/catalog-pg-opclass.html\n Name of the operator class for each column. (PostgreSQL specific).",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "parentIndexName": {
          "description": "The index name of the parent index.",
          "type": "string"
        },
        "parentIndexSchema": {
          "description": "The schema name of the parent index.",
          "type": "string"
        },
        "primary": {
          "description": "The primary is whether the index is a primary key index.",
          "type": "boolean"
        },
        "spatialConfig": {
          "$ref": "#/$defs/bytebase.store.SpatialIndexConfig.jsonschema.strict.json",
          "description": "Spatial index specific configuration"
        },
        "type": {
          "description": "The type is the type of an index.",
          "type": "string"
        },
        "unique": {
          "description": "The unique is whether the index is unique.",
          "type": "boolean"
        },
        "visible": {
          "description": "The visible is whether the index is visible.",
          "type": "boolean"
        }
      },
      "required": [
        "name",
        "type",
        "unique",
        "primary",
        "visible",
        "comment",
        "definition",
        "parentIndexSchema",
        "parentIndexName",
        "granularity",
        "isConstraint"
      ],
      "title": "Index Metadata",
      "type": "object"
    },
    "bytebase.store.MaterializedViewMetadata.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "MaterializedViewMetadata is the metadata for materialized views.",
      "properties": {
        "comment": {
          "description": "The comment is the comment of a view.",
          "type": "string"
        },
        "definition": {
          "description": "The definition is the definition of a view.",
          "type": "string"
        },
        "dependencyColumns": {
          "description": "The list of dependency columns of the view.",
          "items": {
            "$ref": "#/$defs/bytebase.store.DependencyColumn.jsonschema.strict.json"
          },
          "type": "array"
        },
        "indexes": {
          "description": "The list of indexes in the materialized view.",
          "items": {
            "$ref": "#/$defs/bytebase.store.IndexMetadata.jsonschema.strict.json"
          },
          "type": "array"
        },
        "name": {
          "description": "The name of the materialized view.",
          "type": "string"
        },
        "skipDump": {
          "type": "boolean"
        },
        "triggers": {
          "description": "The ordered list of columns in the materialized view.",
          "items": {
            "$ref": "#/$defs/bytebase.store.TriggerMetadata.jsonschema.strict.json"
          },
          "type": "array"
        }
      },
      "required": [
        "name",
        "definition",
        "comment",
        "skipDump"
      ],
      "title": "Materialized View Metadata",
      "type": "object"
    },
    "bytebase.store.SpatialIndexConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "SpatialIndexConfig is the configuration for spatial indexes across different database engines.",
      "properties": {
        "dimensional": {
          "$ref": "#/$defs/bytebase.store.DimensionalConfig.jsonschema.strict.json",
          "description": "Dimensional and constraint parameters"
        },
        "engineSpecific": {
          "additionalProperties": {
            "type": "string"
          },
          "description": "Database-specific parameters (stored as key-value pairs for extensibility)",
          "propertyNames": {
            "type": "string"
          },
          "type": "object"
        },
        "method": {
          "description": "Index method/type (database-specific)\n Examples: \"SPATIAL\" (MySQL/SQL Server), \"GIST\"/\"SPGIST\" (PostgreSQL), \"MDSYS.SPATIAL_INDEX_V2\" (Oracle)",
          "type": "string"
        },
        "storage": {
          "$ref": "#/$defs/bytebase.store.StorageConfig.jsonschema.strict.json",
          "description": "Storage and performance parameters"
        },
        "tessellation": {
          "$ref": "#/$defs/bytebase.store.TessellationConfig.jsonschema.strict.json",
          "description": "Tessellation configuration (primarily SQL Server)"
        }
      },
      "required": [
        "method"
      ],
      "title": "Spatial Index Config",
      "type": "object"
    },
    "bytebase.store.StorageConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "StorageConfig defines storage and performance parameters for spatial indexes.",
      "properties": {
        "allowPageLocks": {
          "type": "boolean"
        },
        "allowRowLocks": {
          "type": "boolean"
        },
        "buffering": {
          "type": "string"
        },
        "commitInterval": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "dataCompression": {
          "type": "string"
        },
        "dropExisting": {
          "type": "boolean"
        },
        "fillfactor": {
          "description": "PostgreSQL parameters",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "maxdop": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "online": {
          "type": "boolean"
        },
        "padIndex": {
          "description": "SQL Server parameters",
          "type": "boolean"
        },
        "sdoLevel": {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "sortInTempdb": {
          "type": "string"
        },
        "tablespace": {
          "description": "Oracle parameters",
          "type": "string"
        },
        "workTablespace": {
          "type": "string"
        }
      },
      "required": [
        "fillfactor",
        "buffering",
        "tablespace",
        "workTablespace",
        "sdoLevel",
        "commitInterval",
        "padIndex",
        "sortInTempdb",
        "dropExisting",
        "online",
        "allowRowLocks",
        "allowPageLocks",
        "maxdop",
        "dataCompression"
      ],
      "title": "Storage Config",
      "type": "object"
    },
    "bytebase.store.TessellationConfig.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "TessellationConfig defines tessellation parameters for spatial indexes.",
      "properties": {
        "boundingBox": {
          "$ref": "#/$defs/bytebase.store.BoundingBox.jsonschema.strict.json",
          "description": "Bounding box for GEOMETRY indexes (SQL Server)"
        },
        "cellsPerObject": {
          "description": "Cells per object (SQL Server)",
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        "gridLevels": {
          "description": "Grid level configuration (SQL Server)",
          "items": {
            "$ref": "#/$defs/bytebase.store.GridLevel.jsonschema.strict.json"
          },
          "type": "array"
        },
        "scheme": {
          "description": "Tessellation scheme\n Examples: GEOMETRY_GRID, GEOGRAPHY_GRID, GEOMETRY_AUTO_GRID, GEOGRAPHY_AUTO_GRID",
          "type": "string"
        }
      },
      "required": [
        "scheme",
        "cellsPerObject"
      ],
      "title": "Tessellation Config",
      "type": "object"
    },
    "bytebase.store.TriggerMetadata.jsonschema.strict.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "properties": {
        "body": {
          "description": "The body of the trigger.",
          "type": "string"
        },
        "characterSetClient": {
          "type": "string"
        },
        "collationConnection": {
          "type": "string"
        },
        "comment": {
          "type": "string"
        },
        "event": {
          "description": "The event that triggers this action, such as INSERT, UPDATE, DELETE, or TRUNCATE.",
          "type": "string"
        },
        "name": {
          "description": "The name of the trigger.",
          "type": "string"
        },
        "skipDump": {
          "type": "boolean"
        },
        "sqlMode": {
          "type": "string"
        },
        "timing": {
          "description": "The timing of when the trigger fires, such as BEFORE or AFTER.",
          "type": "string"
        }
      },
      "required": [
        "name",
        "event",
        "timing",
        "body",
        "sqlMode",
        "characterSetClient",
        "collationConnection",
        "comment",
        "skipDump"
      ],
      "title": "Trigger Metadata",
      "type": "object"
    }
  },
  "$id": "bytebase.store.MaterializedViewMetadata.jsonschema.strict.bundle.json",
  "$ref": "#/$defs/bytebase.store.MaterializedViewMetadata.jsonschema.strict.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
