{
  "$id": "bytebase.v1.PlanCheckRun.Result.SqlSummaryReport.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(affected_rows)$": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "^(changed_resources)$": {
      "$ref": "bytebase.v1.ChangedResources.jsonschema.json"
    },
    "^(statement_types)$": {
      "description": "statement_types are the types of statements that are found in the sql.",
      "items": {
        "type": "string"
      },
      "type": "array"
    }
  },
  "properties": {
    "affectedRows": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "changedResources": {
      "$ref": "bytebase.v1.ChangedResources.jsonschema.json"
    },
    "statementTypes": {
      "description": "statement_types are the types of statements that are found in the sql.",
      "items": {
        "type": "string"
      },
      "type": "array"
    }
  },
  "title": "Sql Summary Report",
  "type": "object"
}
