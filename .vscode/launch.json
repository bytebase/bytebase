{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Dev Binary",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/bytebase-build/bytebase",
      "args": [
        "--port",
        "8080",
        "--external-url",
        "http://localhost:3000",
        "--data",
        "${workspaceFolder}/bytebase-build"
      ]
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0
    }
  ]
}
