[
  {
    "selector": "[data-label='bb-dashboard-static-sidebar']",
    "type": "shield",
    "url": ".+/db/"
  },
  {
    "selector": "[data-label='bb-database-detail-action-buttons-container']",
    "type": "shield",
    "url": ".+/db/"
  },
  {
    "selector": "[data-label='bb-dashboard-header']",
    "type": "shield",
    "url": ".+/db/"
  },
  {
    "selector": "[data-label='bb-migration-history-establish-baseline-alert'] [data-label='bb-alert-ok-button']",
    "type": "shield",
    "url": ".+?/db/.+?#migration-history$",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "Since this is a read-only DEMO, you can contact support@bytebase.com to request a full DEMO.",
        "zh-CN": "但由于这是个只读 DEMO，您可以联系 support@bytebase.com 来申请完整的 DEMO 进行尝试。"
      },
      "alwaysShow": true
    },
    "additionStyle": {
      "backdropFilter": "unset"
    }
  },
  {
    "selector": "[data-label='bb-dashboard-header-database-entry']",
    "url": "^(?!.+/db)",
    "cover": true,
    "dialog": {
      "title": "",
      "description": {
        "en-US": "Let's focus on the Database",
        "zh-CN": "我们来查看数据库模块"
      },
      "alwaysShow": true
    }
  },
  {
    "selector": "[data-label='bb-database-table']",
    "url": ".+?/db",
    "cover": true,
    "dialog": {
      "title": "",
      "description": {
        "en-US": "Choose the target database",
        "zh-CN": "选择一个目标数据库"
      },
      "alwaysShow": true,
      "position": "top"
    }
  },
  {
    "selector": "[data-label='bb-database-detail-tab'] button:nth-child(2)",
    "url": ".+?/db/.+?(?<!#migration-history)$",
    "type": "tooltip",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "Let's focus on the database schema change management",
        "zh-CN": "让我们专注在数据库变更管理场景"
      },
      "position": "right"
    }
  },
  {
    "selector": "[data-label='bb-establish-baseline-button']",
    "url": ".+?/db/.+?#migration-history$",
    "type": "tooltip",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "Creating a new BASELINE can start a new Schema life cycle",
        "zh-CN": "新建一个 BASELINE 可以开始一个新的 Schema 生命周期"
      },
      "position": "right"
    },
    "additionStyle": {
      "marginLeft": "8px"
    }
  },
  {
    "selector": "[data-label='bb-database-overview-description-list']",
    "url": ".+?/db/.+?#overview$",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "The basic information of the database is shown here",
        "zh-CN": "这里展示了数据库的基本信息"
      },
      "alwaysShow": true,
      "position": "topright"
    }
  },
  {
    "selector": "[data-label='bb-migration-history-table'] tr:nth-child(2) > td:nth-child(1)",
    "url": ".+?/db/.+?#migration-history$",
    "type": "tooltip",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "Every change will generate a new Schema version",
        "zh-CN": "每次变更都将产生一个新的 Schema 版本，点我看看"
      },
      "alwaysShow": true,
      "position": "right"
    },
    "additionStyle": {
      "marginLeft": "-16px",
      "marginTop": "8px"
    }
  },
  {
    "selector": "[data-label='bb-migration-history-table'] tr:nth-child(3) > td:nth-child(5)",
    "url": ".+?/db/.+?#migration-history$",
    "type": "tooltip",
    "position": "left",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "Hover the SQL, you can get the full of them",
        "zh-CN": "让我们专注在数据库变更管理场景"
      },
      "position": "left"
    },
    "additionStyle": {
      "marginTop": "8px"
    }
  },
  {
    "selector": "[data-label='bb-migration-history-table'] tr:nth-child(4) > td:nth-child(3)",
    "url": ".+?/db/.+?#migration-history$",
    "type": "tooltip",
    "position": "left",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "BASELINE records the initial state of the database schema",
        "zh-CN": "BASELINE 记录了数据库 Schema 的初始状态，点我看看"
      },
      "position": "right"
    }
  },
  {
    "selector": "[data-label='bb-migration-history-diff-switch']",
    "url": ".+?/db/.+?/history/.+",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "You can also view the full Schema snapshot after the change",
        "zh-CN": "也可以查看变更后的完整 Schema 快照"
      },
      "position": "right",
      "showOnce": "true"
    },
    "additionStyle": {
      "boxShadow": "rgb(78 70 211) 0px 0px 1px 2px"
    }
  },
  {
    "selector": "[data-label='bb-migration-history-view-drift-button']",
    "url": ".+?/db/.+?/history/.+",
    "type": "tooltip",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "A schema drift was detected in this change, indicating that a change beyond the control of the Bytebase platform occurred prior to this change.",
        "zh-CN": "这次变更检查到了 Schema 偏差，说明这次变更之前发生了脱离 Bytebase 平台管控的变更"
      },
      "position": "right"
    },
    "additionStyle": {
      "marginLeft": "8px"
    }
  },
  {
    "selector": "[data-label='bb-migration-history-code-diff-block']",
    "url": ".+?/db/.+?/history/.+",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "This is the different codes between the two versions",
        "zh-CN": "这是两个数据库 Schema 版本之间的差异"
      },
      "alwaysShow": true,
      "position": "topright"
    }
  },
  {
    "selector": "[data-label='bb-migration-history-code-block']",
    "url": ".+?/db/.+?/history/.+",
    "dialog": {
      "title": "",
      "description": {
        "en-US": "This is the current full schema of the database",
        "zh-CN": "这是现在数据库的完整 Schema"
      },
      "alwaysShow": true,
      "position": "topright"
    }
  }
]
