  "scripts": {
    "copy:monaco-workers": "sh ./scripts/copy_monaco_workers.sh",
    "copy:config-files": "sh ./scripts/copy_config_files.sh",
    "copy:all": "npm run copy:monaco-workers && npm run copy:config-files",
    "dev": "export BB_GIT_COMMIT_ID_FE=`git rev-parse HEAD` && export NODE_OPTIONS=--max_old_space_size=8000 && npm run copy:all && vite --mode dev-local",
    "release": "export NODE_OPTIONS=--max_old_space_size=8000 && npm run copy:all && vite build --mode release --outDir=../backend/server/dist --emptyOutDir",
    "dev-embed": "export BB_GIT_COMMIT_ID_FE=`git rev-parse HEAD` && export NODE_OPTIONS=--max_old_space_size=8000 && npm run copy:all && vite build --mode dev --outDir=../backend/server/dist --emptyOutDir",
    "release-docker": "export NODE_OPTIONS=--max_old_space_size=8000 && npm run copy:all && vite build --mode release",
    "release-aws": "export NODE_OPTIONS=--max_old_space_size=8000 && npm run copy:all && vite build --mode release-aws",
    "lint": "eslint --cache --ext .js,.ts,.tsx,.vue src",
    "lint:fix": "eslint --cache --ext .js,.ts,.tsx,.vue --fix src",
    "bundle-visualize": "vite-bundle-visualizer -c vite.config.ts",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "test": "vitest run --passWithNoTests",
    "coverage": "vitest --coverage",
    "postinstall": "npm run copy:monaco-workers && monaco-treemending"
  },
