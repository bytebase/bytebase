{
  "extends": "./tsconfig.app.json",
  "includes": ["src/**/__tests__/*"],
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",

    "verbatimModuleSyntax": true,
    "lib": ["WebWorker", "ES2022"],
    "types": ["node", "jsdom", "unplugin-icons/types/vue", "vite/client"],
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "paths": {
      "@/*": ["./src/*"],
      "@sql-lsp/*": ["./src/plugins/sql-lsp/*"]
    }
  }
}
