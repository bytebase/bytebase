{
  "$id": "bytebase.v1.QueryResult.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(column_names)$": {
      "description": "Column names of the query result.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "^(column_type_names)$": {
      "description": "Column types of the query result.\n The types come from the Golang SQL driver.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "^(permission_denied)$": {
      "$ref": "bytebase.v1.QueryResult.PermissionDenied.jsonschema.json"
    },
    "^(postgres_error)$": {
      "$ref": "bytebase.v1.QueryResult.PostgresError.jsonschema.json"
    },
    "^(rows_count)$": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "^(syntax_error)$": {
      "$ref": "bytebase.v1.QueryResult.SyntaxError.jsonschema.json"
    }
  },
  "properties": {
    "columnNames": {
      "description": "Column names of the query result.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "columnTypeNames": {
      "description": "Column types of the query result.\n The types come from the Golang SQL driver.",
      "items": {
        "type": "string"
      },
      "type": "array"
    },
    "error": {
      "default": "",
      "description": "The error message if the query failed.",
      "type": "string"
    },
    "latency": {
      "$ref": "google.protobuf.Duration.jsonschema.json",
      "description": "The time it takes to execute the query."
    },
    "masked": {
      "description": "Masking reasons for each column (empty for non-masked columns).",
      "items": {
        "$ref": "bytebase.v1.MaskingReason.jsonschema.json"
      },
      "type": "array"
    },
    "messages": {
      "description": "Informational or debug messages returned by the database engine during query execution.\n Examples include PostgreSQL's RAISE NOTICE, MSSQL's PRINT, or Oracle's DBMS_OUTPUT.PUT_LINE.",
      "items": {
        "$ref": "bytebase.v1.QueryResult.Message.jsonschema.json"
      },
      "type": "array"
    },
    "permissionDenied": {
      "$ref": "bytebase.v1.QueryResult.PermissionDenied.jsonschema.json"
    },
    "postgresError": {
      "$ref": "bytebase.v1.QueryResult.PostgresError.jsonschema.json"
    },
    "rows": {
      "description": "Rows of the query result.",
      "items": {
        "$ref": "bytebase.v1.QueryRow.jsonschema.json"
      },
      "type": "array"
    },
    "rowsCount": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "statement": {
      "default": "",
      "description": "The query statement for the result.",
      "type": "string"
    },
    "syntaxError": {
      "$ref": "bytebase.v1.QueryResult.SyntaxError.jsonschema.json"
    }
  },
  "title": "Query Result",
  "type": "object"
}
