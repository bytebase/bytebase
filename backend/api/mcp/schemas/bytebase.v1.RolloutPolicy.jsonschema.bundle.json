{
  "$defs": {
    "bytebase.v1.RolloutPolicy.Checkers.RequiredStatusChecks.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(plan_check_enforcement)$": {
          "anyOf": [
            {
              "pattern": "^PLAN_CHECK_ENFORCEMENT_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "ERROR_ONLY",
                "STRICT"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "Enforcement level for plan check results during rollout validation.",
          "title": "Plan Check Enforcement"
        }
      },
      "properties": {
        "planCheckEnforcement": {
          "anyOf": [
            {
              "pattern": "^PLAN_CHECK_ENFORCEMENT_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "ERROR_ONLY",
                "STRICT"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "Enforcement level for plan check results during rollout validation.",
          "title": "Plan Check Enforcement"
        }
      },
      "title": "Required Status Checks",
      "type": "object"
    },
    "bytebase.v1.RolloutPolicy.Checkers.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "patternProperties": {
        "^(required_issue_approval)$": {
          "default": false,
          "description": "Whether issue approval is required before proceeding with rollout.",
          "type": "boolean"
        },
        "^(required_status_checks)$": {
          "$ref": "#/$defs/bytebase.v1.RolloutPolicy.Checkers.RequiredStatusChecks.jsonschema.json",
          "description": "Status checks that must pass before rollout can be executed."
        }
      },
      "properties": {
        "requiredIssueApproval": {
          "default": false,
          "description": "Whether issue approval is required before proceeding with rollout.",
          "type": "boolean"
        },
        "requiredStatusChecks": {
          "$ref": "#/$defs/bytebase.v1.RolloutPolicy.Checkers.RequiredStatusChecks.jsonschema.json",
          "description": "Status checks that must pass before rollout can be executed."
        }
      },
      "title": "Checkers",
      "type": "object"
    },
    "bytebase.v1.RolloutPolicy.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Rollout policy configuration.",
      "properties": {
        "automatic": {
          "default": false,
          "description": "Whether rollout is automatic without manual approval.",
          "type": "boolean"
        },
        "checkers": {
          "$ref": "#/$defs/bytebase.v1.RolloutPolicy.Checkers.jsonschema.json",
          "description": "Checkers that must pass before rollout execution.\n These checks are performed in UI workflows only."
        },
        "roles": {
          "description": "The roles that can approve rollout execution.",
          "items": {
            "type": "string"
          },
          "type": "array"
        }
      },
      "title": "Rollout Policy",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.RolloutPolicy.jsonschema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.RolloutPolicy.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
