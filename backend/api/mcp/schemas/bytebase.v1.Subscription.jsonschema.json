{
  "$id": "bytebase.v1.Subscription.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(active_instances)$": {
      "anyOf": [
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "^(expires_time)$": {
      "$ref": "google.protobuf.Timestamp.jsonschema.json"
    },
    "^(org_name)$": {
      "default": "",
      "type": "string"
    }
  },
  "properties": {
    "activeInstances": {
      "anyOf": [
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "expiresTime": {
      "$ref": "google.protobuf.Timestamp.jsonschema.json"
    },
    "instances": {
      "anyOf": [
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "orgName": {
      "default": "",
      "type": "string"
    },
    "plan": {
      "anyOf": [
        {
          "pattern": "^PLAN_TYPE_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "FREE",
            "TEAM",
            "ENTERPRISE"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Plan Type"
    },
    "seats": {
      "anyOf": [
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        },
        {
          "pattern": "^-?[0-9]+$",
          "type": "string"
        }
      ],
      "default": 0
    },
    "trialing": {
      "default": false,
      "type": "boolean"
    }
  },
  "title": "Subscription",
  "type": "object"
}
