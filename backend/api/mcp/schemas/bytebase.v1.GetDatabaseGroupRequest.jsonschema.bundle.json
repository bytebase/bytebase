{
  "$defs": {
    "bytebase.v1.GetDatabaseGroupRequest.jsonschema.json": {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "additionalProperties": false,
      "description": "Request message for getting a database group.",
      "properties": {
        "name": {
          "default": "",
          "description": "The name of the database group to retrieve.\n Format: projects/{project}/databaseGroups/{databaseGroup}",
          "type": "string"
        },
        "view": {
          "anyOf": [
            {
              "pattern": "^DATABASE_GROUP_VIEW_UNSPECIFIED$",
              "type": "string"
            },
            {
              "enum": [
                "DATABASE_GROUP_VIEW_BASIC",
                "DATABASE_GROUP_VIEW_FULL"
              ],
              "type": "string"
            },
            {
              "maximum": 2147483647,
              "minimum": -2147483648,
              "type": "integer"
            }
          ],
          "default": 0,
          "description": "The view to return. Defaults to DATABASE_GROUP_VIEW_BASIC.",
          "title": "Database Group View"
        }
      },
      "title": "Get Database Group Request",
      "type": "object"
    }
  },
  "$id": "bytebase.v1.GetDatabaseGroupRequest.jsonschema.bundle.json",
  "$ref": "#/$defs/bytebase.v1.GetDatabaseGroupRequest.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema"
}
