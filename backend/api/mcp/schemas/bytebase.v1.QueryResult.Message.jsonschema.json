  "$id": "bytebase.v1.QueryResult.Message.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "properties": {
    "content": {
      "default": "",
      "type": "string"
    },
    "level": {
      "anyOf": [
        {
          "pattern": "^LEVEL_UNSPECIFIED$",
          "type": "string"
        },
        {
          "enum": [
            "INFO",
            "WARNING",
            "DEBUG",
            "LOG",
            "NOTICE",
            "EXCEPTION"
          ],
          "type": "string"
        },
        {
          "maximum": 2147483647,
          "minimum": -2147483648,
          "type": "integer"
        }
      ],
      "default": 0,
      "title": "Level"
    }
  },
  "title": "Message",
  "type": "object"
}
