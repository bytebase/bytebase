{
  "$id": "bytebase.v1.EnumTypeMetadata.jsonschema.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "additionalProperties": false,
  "patternProperties": {
    "^(skip_dump)$": {
      "default": false,
      "description": "Whether to skip this enum type during schema dump operations.",
      "type": "boolean"
    }
  },
  "properties": {
    "comment": {
      "default": "",
      "description": "The comment describing the enum type.",
      "type": "string"
    },
    "name": {
      "default": "",
      "description": "The name of a type.",
      "type": "string"
    },
    "skipDump": {
      "default": false,
      "description": "Whether to skip this enum type during schema dump operations.",
      "type": "boolean"
    },
    "values": {
      "description": "The enum values of a type.",
      "items": {
        "type": "string"
      },
      "type": "array"
    }
  },
  "title": "Enum Type Metadata",
  "type": "object"
